[
    {
        "title": "送春词",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "昨来楼上迎春处，今日登楼又送归。兰蕊残妆含露泣，",
            "万古至今同此恨，无如一醉尽忘机。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 1
    },
    {
        "title": "送李尚书镇滑州",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "南徐报政入文昌，东郡须才别建章。视草名高同蜀客，",
            "自古相门还出相，如今人望在岩廊。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 2
    },
    {
        "title": "送浑大夫赴丰州",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "凤衔新诏降恩华，又见旌旗出浑家。故吏来辞辛属国，",
            "其奈明年好春日，无人唤看牡丹花。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 3
    },
    {
        "title": "送源中丞充新罗册立使",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "相门才子称华簪，持节东行捧德音。身带霜威辞凤阙，",
            "想见扶桑受恩处，一时西拜尽倾心。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 4
    },
    {
        "title": "送王司马之陕州",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "暂辍清斋出太常，空携诗卷赴甘棠。府公既有朝中旧，",
            "两京大道多游客，每遇词人战一场。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 5
    },
    {
        "title": "洛中送杨处厚入关便游蜀",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "洛阳秋日正凄凄，君去西秦更向西。旧学三冬今转富，",
            "早识卧龙应有分，不妨从此蹑丹梯。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 6
    },
    {
        "title": "送周使君罢渝州归郢州别墅",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "君思郢上吟归去，故自渝南掷郡章。野戍岸边留画舸，",
            "只恐鸣驺催上道，不容待得晚菘尝。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 7
    },
    {
        "title": "奉送浙西李仆射相公赴镇",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "建节东行是旧游，欢声喜气满吴州。郡人重得黄丞相，",
            "自怜不识平津阁，遥望旌旗汝水头。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 8
    },
    {
        "title": "重送浙西李相公顷廉问江南已经七载后历滑台剑南两镇遂入相今复领旧地新加旌旄",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "江北万人看玉节，江南千骑引金铙。凤从池上游沧海，",
            "碧鸡白马回翔久，却忆朱方是乐郊。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 9
    },
    {
        "title": "送前进士蔡京赴学究科",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "耳闻战鼓带经锄，振发声名自里闾。已是世间能赋客，",
            "幸遇天官旧丞相，知君无翼上空虚。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 10
    },
    {
        "title": "送唐舍人出镇闽中",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "暂辞鸳鹭出蓬瀛，忽拥貔貅镇粤城。闽岭夏云迎皂盖，",
            "了却人间婚嫁事，复归朝右作公卿。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 11
    },
    {
        "title": "送李中丞赴楚州",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "缇骑朱旗入楚城，士林皆贺振家声。儿童但喜迎宾守，",
            "忆君初得昆山玉，同向扬州携手行。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 12
    },
    {
        "title": "奉送李户部侍郎自河南尹再除本官归阙",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "昔年内署振雄词，今日东都结去思。宫女犹传洞箫赋，",
            "想到金闺待通籍，一时惊喜见风仪。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 13
    },
    {
        "title": "送蕲州李郎中赴任",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "楚关蕲水路非赊，东望云山日夕佳。薤叶照人呈夏簟，",
            "北地交亲长引领，早将玄鬓到京华。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 14
    },
    {
        "title": "送国子令狐博士赴兴元觐省",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "相门才子高阳族，学省清资五品官。谏院过时荣棣萼，",
            "伯仲到家人尽贺，柳营莲府递相欢。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 15
    },
    {
        "title": "送李二十九兄员外赴邠宁使幕",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "家袭韦平身业文，素风清白至今贫。南宫通籍新郎吏，",
            "鼎门为别霜天晓，剩把离觞三五巡。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 16
    },
    {
        "title": "送分司陈郎中只召直史馆重修三圣实录",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "蝉鸣官树引行车，言自成周赴玉除。远取南朝贵公子，",
            "若问旧人刘子政，如今白首在南徐。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 17
    },
    {
        "title": "送慧则法师归上都因呈广宣上人",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "昨日东林看讲时，都人象马蹋琉璃。雪山童子应前世，",
            "休公久别如相问，楚客逢秋心更悲。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 18
    },
    {
        "title": "送义舟师却还黔南",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "黔江秋水浸云霓，独泛慈航路不迷。猿狖窥斋林叶动，",
            "常说摩围似灵鹫，却将山屐上丹梯。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 19
    },
    {
        "title": "送景玄师东归",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "东林寺里一沙弥，心爱当时才子诗。山下偶随流水出，",
            "想到旧房抛锡杖，小松应有过簷枝。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 20
    },
    {
        "title": "汉寿城春望",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "汉寿城边野草春，荒祠古墓对荆榛。田中牧竖烧刍狗，",
            "不知何日东瀛变，此地还成要路津。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 21
    },
    {
        "title": "荆门道怀古",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "南国山川旧帝畿，宋台梁馆尚依稀。马嘶古道行人歇，",
            "徒使词臣庾开府，咸阳终日苦思归。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 22
    },
    {
        "title": "朗州窦员外见示与澧州元郎中郡斋赠答长句二篇因以继和",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "鸳鹭差池出建章，彩旗朱户蔚相望。新恩共理犬牙地，",
            "应怜一罢金闺籍，枉渚逢春十度伤。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 23
    },
    {
        "title": "早春对雪奉寄澧州元郎中",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "新赐鱼书墨未干，贤人暂出远人安。朝驱旌旆行时令，",
            "宁知楚客思公子，北望长吟澧有兰。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 24
    },
    {
        "title": "窦朗州见示与澧州元郎中早秋赠答命同作",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "邻境诸侯同舍郎，芷江兰浦恨无梁。秋风门外旌旗动，"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 25
    },
    {
        "title": "松滋渡望峡中",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "渡头轻雨洒寒梅，云际溶溶雪水来。梦渚草长迷楚望，",
            "十二碧峰何处所，永安宫外是荒台。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 26
    },
    {
        "title": "衢州徐员外使君遗以缟纻兼竹书箱因成一篇用答佳贶",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "烂柯山下旧仙郎，列宿来添婺女光。远放歌声分白纻，",
            "闻说天台有遗爱，人将琪树比甘棠。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 27
    },
    {
        "title": "唐秀才赠端州紫石砚，以诗答之",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "端州石砚人间重，赠我因知正草玄。阙里庙堂空旧物，",
            "此日佣工记名姓，因君数到墨池前。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 28
    },
    {
        "title": "览董评事思归之什因以诗赠",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "几年油幕佐征东，却泛沧浪狎钓童。欹枕醉眠成戏蝶，",
            "更说扁舟动乡思，青菰已熟奈秋风。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 29
    },
    {
        "title": "谢寺双桧",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "双桧苍然古貌奇，含烟吐雾郁参差。晚依禅客当金殿，",
            "长明灯是前朝焰，曾照青青年少时。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 30
    },
    {
        "title": "寄杨八寿州",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "风猎红旗入寿春，满城歌舞向朱轮。八公山下清淮水，",
            "圣朝方用敢言者，次第应须旧谏臣。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 31
    },
    {
        "title": "洛中寺北楼见贺监草书题诗",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "高楼贺监昔曾登，壁上笔踪龙虎腾。中国书流尚皇象，",
            "唯恐尘埃转磨灭，再三珍重嘱山僧。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 32
    },
    {
        "title": "闻韩宾擢第归觐以诗美之兼贺韩十五曹长时韩牧永州",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "零陵香草满郊坰，丹穴雏飞入翠屏。孝若归来成画赞，",
            "为报儒林丈人道，如今从此鬓星星。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 33
    },
    {
        "title": "宣上人远寄和礼部王侍郎放榜后诗，因而继和",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "礼闱新榜动长安，九陌人人走马看。一日声名遍天下，",
            "借问至公谁印可，支郎天眼定中观。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 34
    },
    {
        "title": "赠东岳张炼师",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "东岳真人张炼师，高情雅淡世间稀。堪为列女书青简，",
            "云衢不要吹箫伴，只拟乘鸾独自飞。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 35
    },
    {
        "title": "秘书崔少监见示坠马长句，因而和之",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "麟台少监旧仙郎，洛水桥边坠马伤。尘污腰间青襞绶，",
            "犹赖德全如醉者，不妨吟咏入篇章。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 36
    },
    {
        "title": "寄杨虢州与之旧姻",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "避地江湖知几春，今来本郡拥朱轮。阮郎无复里中旧，",
            "玉城山里多灵药，摆落功名且养神。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 37
    },
    {
        "title": "秋日题窦员外崇德里新居",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "长爱街西风景闲，到君居处暂开颜。清光门外一渠水，",
            "莫言堆案无余地，认得诗人在此间。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 38
    },
    {
        "title": "蒙恩转仪曹郎依前充集贤学士举韩潮州自代因寄七言",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "翔鸾阙下谢恩初，通籍由来在石渠。暂入南宫判祥瑞，",
            "今日荐君嗟久滞，不惟文体似相如。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 39
    },
    {
        "title": "途次华州陪钱大夫登城北楼春望因睹李崔令狐三相国唱和之什翰林旧侣继踵华城山水清高鸾凤翔集皆忝宿眷遂题此诗",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "城楼四望出风尘，见尽关西渭北春。百二山河雄上国，",
            "莫怪老郎呈滥吹，宦途虽别旧情亲。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 40
    },
    {
        "title": "始闻秋风",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "昔看黄菊与君别，今听玄蝉我却回。五夜飕飗枕前觉，",
            "天地肃清堪四望，为君扶病上高台。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 41
    },
    {
        "title": "洛中初冬拜表有怀上京故人",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "凤楼南面控三条，拜表郎官早渡桥。清洛晓光铺碧簟，",
            "寄谢殷勤九天侣，抢榆水击各逍遥。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 42
    },
    {
        "title": "尉迟郎中见示自南迁牵复却至洛城东旧居之作因以和之",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "曾遭飞语十年谪，新受恩光万里还。朝服不妨游洛浦，",
            "留作功成退身地，如今只是暂时闲。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 43
    },
    {
        "title": "洛中酬福建陈判官见赠",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "潦倒声名拥肿材，一生多故苦邅回。南宫旧籍遥相管，",
            "怪君近日文锋利，新向延平看剑来。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 44
    },
    {
        "title": "和苏十郎中谢病闲居时严常侍萧给事同过访叹初有二毛之作",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "清羸隐几望云空，左掖鸳鸾到室中。一卷素书消永日，",
            "莫怪人人惊早白，缘君尚是黑头翁。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 45
    },
    {
        "title": "酬淮南廖参谋秋夕见过之作",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "扬州从事夜相寻，无限新诗月下吟。初服已惊玄发长，",
            "不逐繁华访闲散，知君摆落俗人心。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 46
    },
    {
        "title": "题王郎中宣义里新居",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "爱君新买街西宅，客到如游鄠杜间。雨后退朝贪种树，",
            "见拟移居作邻里，不论时节请开关。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 47
    },
    {
        "title": "酬朗州崔员外与任十四兄侍御同过鄙人旧居见怀之什时守吴郡",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "昔日居邻招屈亭，枫林橘树鹧鸪声。一辞御苑青门去，",
            "何人万里能相忆，同舍仙郎与外兄。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 48
    },
    {
        "title": "刘驸马水亭避暑",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "千竿竹翠数莲红，水阁虚凉玉簟空。琥珀盏红疑漏酒，",
            "尽日逍遥避烦暑，再三珍重主人翁。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 49
    },
    {
        "title": "述旧贺迁寄陕虢孙常侍",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "南宫幸袭芝兰后，左辅曾交印绶来。多病未离清洛苑，",
            "闻说随车有零雨，此时偏动子荆才。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 50
    },
    {
        "title": "江陵严司空见示与成都武相公唱和，因命同作",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "南荆西蜀大行台，幕府旌门相对开。名重三司平水土，",
            "不是郢中清唱发，谁当丞相掞天才。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 51
    },
    {
        "title": "庙庭偃松诗",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "势轧枝偏根已危，高情一见与扶持。忽从憔悴有生意，",
            "谢公莫道东山去，待取阴成满凤池。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 52
    },
    {
        "title": "赠致仕滕庶子先辈",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "朝服归来昼锦荣，登科记上更无兄。寿觞每使曾孙献，",
            "凌寒却向山阴去，衣绣郎君雪里行。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 53
    },
    {
        "title": "哭吕衡州，时予方谪居",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "一夜霜风凋玉芝，苍生望绝士林悲。空怀济世安人略，",
            "朔方徙岁行当满，欲为君刊第二碑。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 54
    },
    {
        "title": "夔州窦员外使君见示悼妓诗顾余尝识之因命同作",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "前年曾见两鬟时，今日惊吟悼妓诗。凤管学成知有籍，",
            "寂寞鱼山青草里，何人更立智琼祠。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 55
    },
    {
        "title": "窦夔州见寄寒食日忆故姬小红吹笙，因和之",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "鸾声窈眇管参差，清韵初调众乐随。幽院妆成花下弄，",
            "闻道今年寒食日，东山旧路独行迟。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 56
    },
    {
        "title": "哭庞京兆",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "俊骨英才气褎然，策名飞步冠群贤。逢时已自致高位。",
            "得疾还因倚少年。天上别归京兆府，人间空数茂陵阡。",
            "今朝穗帐哭君处，前日见铺歌舞筵。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 57
    },
    {
        "title": "送李庚先辈赴选",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "一家何啻十朱轮，诸父双飞秉大钧。曾脱素衣参幕客，",
            "今日山公旧宾主，知君不负帝城春。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 58
    },
    {
        "title": "阳山庙观赛神",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "汉家都尉旧征蛮，血食如今配此山，曲盖幽深苍桧下，",
            "日落风生庙门外，几人连蹋竹歌还。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 59
    },
    {
        "title": "送僧元暠东游",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "宝书翻译学初成，振锡如飞白足轻。彭泽因家凡几世，",
            "从此多逢大居士，何人不愿解珠璎。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 60
    },
    {
        "title": "赠日本僧智藏",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "浮杯万里过沧溟，遍礼名山适性灵。深夜降龙潭水黑，",
            "为问中华学道者，几人雄猛得宁馨。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 61
    },
    {
        "title": "送元简上人适越",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "孤云出岫本无依，胜境名山即是归。久向吴门游好寺，",
            "更入天台石桥去，垂珠璀璨拂三衣。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 62
    },
    {
        "title": "送宗密上人归南山草堂寺，因谒河南尹白侍郎",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "宿习修来得慧根，多闻第一却忘言。自从七祖传心印，",
            "河南白尹大檀越，好把真经相对翻。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 63
    },
    {
        "title": "西塞山怀古",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "西晋楼船下益州，金陵王气黯然收。千寻铁锁沉江底，",
            "今逢四海为家日，故垒萧萧芦荻秋。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 64
    },
    {
        "title": "广宣上人寄在蜀与韦令公唱和诗卷因以令公手扎答诗示之",
        "author": "刘禹锡",
        "biography": "",
        "paragraphs": [
            "碧云佳句久传芳，曾向成都住草堂。振锡常过长者宅，",
            "若许相期同结社，吾家本自有柴桑。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 65
    }
]