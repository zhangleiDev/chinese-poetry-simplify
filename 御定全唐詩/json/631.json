[
    {
        "title": "旅泊吴门",
        "author": "张贲",
        "biography": "",
        "paragraphs": [
            "一舸吴江晚，堪忧病广文。鲈鱼谁与伴，鸥鸟自成群。",
            "反照纵横水，斜空断续云。异乡无限思，尽付酒醺醺。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百三十一",
        "no#": 1
    },
    {
        "title": "贲中间有吴门旅泊之什蒙鲁望垂和更作一章以伸酬谢",
        "author": "张贲",
        "biography": "",
        "paragraphs": [
            "偶发陶匏响，皆蒙组绣文。清秋将落帽，子夏正离群。",
            "有恨书燕雁，无聊赋郢云。遍看心自醉，不是酒能醺。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百三十一",
        "no#": 2
    },
    {
        "title": "酬袭美先见寄倒来韵",
        "author": "张贲",
        "biography": "",
        "paragraphs": [
            "寻疑天意丧斯文，故选茅峰寄白云。酒后只留沧海客，",
            "为有此身犹苦患，不知何者是玄𫄸。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百三十一",
        "no#": 3
    },
    {
        "title": "奉和袭美醉中即席见赠次韵",
        "author": "张贲",
        "biography": "",
        "paragraphs": [
            "桂枝新下月中仙，学海词锋誉蔼然。文阵已推忠信甲，",
            "共许逢蒙快弓箭，再穿杨叶在明年。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百三十一",
        "no#": 4
    },
    {
        "title": "奉和袭美题褚家林亭",
        "author": "张贲",
        "biography": "",
        "paragraphs": [
            "疏野林亭震泽西，朗吟闲步喜相携。时时风折芦花乱，",
            "今朝偶得高阳伴，从放山翁醉似泥。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百三十一",
        "no#": 5
    },
    {
        "title": "奉和袭美伤开元观顾道士",
        "author": "张贲",
        "biography": "",
        "paragraphs": [
            "凤麟胶尽夜如何，共叹先生剑解多。几度吊来唯白鹤，",
            "惆怅真灵又空返，玉书谁授紫微歌。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百三十一",
        "no#": 6
    },
    {
        "title": "和鲁望白菊",
        "author": "张贲",
        "biography": "",
        "paragraphs": [
            "雪彩冰姿号女华，寄身多是地仙家。有时南国和霜立，",
            "自知终古清香在，更出梅妆弄晚霞。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百三十一",
        "no#": 7
    },
    {
        "title": "奉和袭美先辈悼鹤",
        "author": "张贲",
        "biography": "",
        "paragraphs": [
            "池塘萧索掩空笼，玉树同嗟一土中。莎径罢鸣唯泣露，",
            "云减雾消无处问，只留华发与衰翁。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百三十一",
        "no#": 8
    },
    {
        "title": "偶约道流终乖文会答皮陆",
        "author": "张贲",
        "biography": "",
        "paragraphs": [
            "仙侣无何访蔡经，两烦韶濩出彤庭。",
            "人间若有登楼望，应怪文星近客星。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百三十一",
        "no#": 9
    },
    {
        "title": "和袭美寒夜见访",
        "author": "张贲",
        "biography": "",
        "paragraphs": [
            "云孤鹤独且相亲，仿效从它折角巾。",
            "不用吴江叹留滞，风姿俱是玉清人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百三十一",
        "no#": 10
    },
    {
        "title": "和袭美醉中先起次韵",
        "author": "张贲",
        "biography": "",
        "paragraphs": [
            "何事桃源路忽迷，惟留云雨怨空闺。",
            "仙郎共许多情调，莫遣重歌浊水泥。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百三十一",
        "no#": 11
    },
    {
        "title": "和皮陆酒病偶作",
        "author": "张贲",
        "biography": "",
        "paragraphs": [
            "白编椰席镂冰明，应助杨青解宿酲。",
            "难继二贤金玉唱，可怜空作断猿声。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百三十一",
        "no#": 12
    },
    {
        "title": "送浙东德师侍御罢府西归",
        "author": "张贲",
        "biography": "",
        "paragraphs": [
            "孤云独鸟本无依，江海重逢故旧稀。",
            "杨柳渐疏芦苇白，可怜斜日送君归。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百三十一",
        "no#": 13
    },
    {
        "title": "以青z3饭分送袭美鲁望因成一绝",
        "author": "张贲",
        "biography": "",
        "paragraphs": [
            "谁屑琼瑶事青z3，旧传名品出华阳。",
            "应宜仙子胡麻拌，因送刘郎与阮郎。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百三十一",
        "no#": 14
    },
    {
        "title": "玩金㶉𫛶和陆鲁望",
        "author": "张贲",
        "biography": "",
        "paragraphs": [
            "翠羽红襟镂彩云，双飞常笑白鸥群。",
            "谁怜化作雕金质，从倩沉檀十里闻。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百三十一",
        "no#": 15
    },
    {
        "title": "悼鹤和袭美",
        "author": "张贲",
        "biography": "",
        "paragraphs": [
            "渥顶鲜毛品格驯，莎庭闲暇重难群。",
            "无端日暮东风起，飘散春空一片云。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百三十一",
        "no#": 16
    },
    {
        "title": "览皮先辈盛制，因作十韵以寄，用伸款仰",
        "author": "崔璐",
        "biography": "",
        "paragraphs": [
            "河岳挺灵异，星辰精气殊。在人为英杰，与国作祯符。",
            "襄阳得奇士，俊迈真龙驹。勇果鲁仲由，文赋蜀相如。",
            "浑浩江海广，葩华桃李敷。小言入无间，大言塞空虚。",
            "几人游赤水，夫子得玄珠。鬼神争奥秘，天地惜洪炉。",
            "既有曾参行，仍兼君子儒。吾知上帝意，将使居黄枢。",
            "好保千金体，须为万姓谟。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百三十一",
        "no#": 17
    },
    {
        "title": "浙东罢府西归酬别张广文皮先辈陆秀才",
        "author": "李縠",
        "biography": "",
        "paragraphs": [
            "岂有头风笔下痊，浪成蛮语向初筵。兰亭旧趾虽曾见，",
            "相逢只恨相知晚，一曲骊歌又几年。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百三十一",
        "no#": 18
    },
    {
        "title": "和皮日休悼鹤",
        "author": "李縠",
        "biography": "",
        "paragraphs": [
            "才子襟期本上清，陆云家鹤伴闲情。犹怜反顾五六里，",
            "人间华表堪留语，剩向秋风寄一声。",
            "道林曾放雪翎飞，应悔庭除闭羽衣。",
            "料得王恭披鹤氅，倚吟犹待月中归。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百三十一",
        "no#": 19
    },
    {
        "title": "醉中袭美先月中归",
        "author": "李縠",
        "biography": "",
        "paragraphs": [
            "休文虽即逃琼液，阿鹜还须掩玉闺。",
            "月落金鸡一声后，不知谁悔醉如泥。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百三十一",
        "no#": 20
    },
    {
        "title": "奉酬皮先辈霜菊见赠",
        "author": "崔璞",
        "biography": "",
        "paragraphs": [
            "菊花开晚过秋风，闻道芳香正满丛。",
            "争奈病夫难强饮，应须速自召车公。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百三十一",
        "no#": 21
    },
    {
        "title": "蒙恩除替将还京洛偶叙所怀因成六韵呈军事院诸公郡中一二秀才",
        "author": "崔璞",
        "biography": "",
        "paragraphs": [
            "两载求人瘼，三春受代归。务繁多簿籍，才短乏恩威。",
            "共理乖天奖，分忧值岁饥。遽蒙交郡印，安敢整朝衣。",
            "作牧惭为政，思乡念式微。傥容还故里，高卧掩柴扉。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百三十一",
        "no#": 22
    },
    {
        "title": "和皮日休悼鹤",
        "author": "魏朴",
        "biography": "",
        "paragraphs": [
            "直欲裁诗问杳冥，岂教灵化亦浮生。风林月动疑留魄，",
            "人间飞去犹堪恨，况是泉台远玉京。",
            "经秋宋玉已悲伤，况报胎禽昨夜亡。",
            "霜晓起来无问处，伴僧弹指绕荷塘。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百三十一",
        "no#": 23
    },
    {
        "title": "皮袭美见留小宴次韵",
        "author": "羊昭业",
        "biography": "",
        "paragraphs": [
            "泽国春来少遇晴，有花开日且飞觥。王戎似电休推病，",
            "知君不肯然官烛，争得华筵彻夜明。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百三十一",
        "no#": 24
    },
    {
        "title": "送羊振文归觐桂阳",
        "author": "颜萱",
        "biography": "",
        "paragraphs": [
            "高挂吴帆喜动容，问安归去指湘峰。悬鱼庭内芝兰秀，",
            "临岐独有沾襟恋，南巷当年共化龙。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百三十一",
        "no#": 25
    },
    {
        "title": "送圆载上人",
        "author": "颜萱",
        "biography": "",
        "paragraphs": [
            "师来一世恣经行，却泛沧波问去程。心静已能防渴鹿，",
            "料得还乡无别利，只应先见日华生。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百三十一",
        "no#": 26
    },
    {
        "title": "过张祜处士丹阳故居",
        "author": "颜萱",
        "biography": "",
        "paragraphs": [
            "忆昔为儿逐我兄，曾抛竹马拜先生。书斋已换当时主，",
            "柴扉草屋无人问，犹向荒田责地征。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百三十一",
        "no#": 27
    },
    {
        "title": "和袭美伤顾道士",
        "author": "郑璧",
        "biography": "",
        "paragraphs": [
            "斜汉银澜一夜东，飘飘何处五云中。空留华表千年约，",
            "门人不睹飞升去，犹与浮生哭恨同。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百三十一",
        "no#": 28
    },
    {
        "title": "奉和陆鲁望白菊",
        "author": "郑璧",
        "biography": "",
        "paragraphs": [
            "白艳轻明带露痕，始知佳色重难群。终朝疑笑梁王雪，",
            "琼妃若会宽裁剪，堪作蟾宫夜舞裙。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百三十一",
        "no#": 29
    },
    {
        "title": "和袭美索友人酒",
        "author": "郑璧",
        "biography": "",
        "paragraphs": [
            "乘兴闲来小谢家，便裁诗句乞榴花。",
            "邴原虽不无端醉，也爱临风从鹿车。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百三十一",
        "no#": 30
    },
    {
        "title": "文燕润卿不至",
        "author": "郑璧",
        "biography": "",
        "paragraphs": [
            "已知羽驾朝金阙，不用烧兰望玉京。",
            "应是易迁明月好，玉皇留看舞双成。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百三十一",
        "no#": 31
    }
]