[
    {
        "title": "车驾西游因而有作",
        "author": "温庭筠",
        "biography": "",
        "paragraphs": [
            "宣曲长杨瑞气凝，上林狐兔待秋鹰。",
            "谁将词赋陪雕辇，寂寞相如卧茂林。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十九",
        "no#": 1
    },
    {
        "title": "伤温德彝",
        "author": "温庭筠",
        "biography": "",
        "paragraphs": [
            "昔年戎虏犯榆关，一败龙城匹马还。",
            "侯印不闻封李广，他人丘垄似天山。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十九",
        "no#": 2
    },
    {
        "title": "赠少年",
        "author": "温庭筠",
        "biography": "",
        "paragraphs": [
            "江海相逢客恨多，秋风叶下洞庭波。",
            "酒酣夜别淮阴市，月照高楼一曲歌。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十九",
        "no#": 3
    },
    {
        "title": "赠郑征君家匡山首春与丞相赞皇公游止",
        "author": "温庭筠",
        "biography": "",
        "paragraphs": [
            "一抛兰棹逐燕鸿，曾向江湖识谢公。",
            "每到朱门还怅望，故山多在画屏中。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十九",
        "no#": 4
    },
    {
        "title": "夏中病痁作",
        "author": "温庭筠",
        "biography": "",
        "paragraphs": [
            "山鬼扬威正气愁，便辞珍簟袭狐裘。",
            "西窗一夕悲人事，团扇无情不待秋。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十九",
        "no#": 5
    },
    {
        "title": "题友人居",
        "author": "温庭筠",
        "biography": "",
        "paragraphs": [
            "尽日松堂看画图，绮疏岑寂似清都。",
            "若教烟水无鸥鸟，张翰何由到五湖。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十九",
        "no#": 6
    },
    {
        "title": "题李相公敕赐锦屏风",
        "author": "温庭筠",
        "biography": "",
        "paragraphs": [
            "丰沛曾为社稷臣，赐书名画墨犹新。",
            "几人同保山河誓，犹自栖栖九陌尘。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十九",
        "no#": 7
    },
    {
        "title": "蔡中郎坟",
        "author": "温庭筠",
        "biography": "",
        "paragraphs": [
            "古坟零落野花春，闻说中郎有后身。",
            "今日爱才非昔日，莫抛心力作词人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十九",
        "no#": 8
    },
    {
        "title": "元处士池上",
        "author": "温庭筠",
        "biography": "",
        "paragraphs": [
            "蓼穗菱丛思蟪蛄，水萤江鸟满烟蒲。",
            "愁红一片风前落，池上秋波似五湖。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十九",
        "no#": 9
    },
    {
        "title": "华阴韦氏林亭",
        "author": "温庭筠",
        "biography": "",
        "paragraphs": [
            "自有林亭不得闲，陌尘宫树是非间。",
            "终南长在茅簷外，别向人间看华山。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十九",
        "no#": 10
    },
    {
        "title": "寄裴生乞钓钩",
        "author": "温庭筠",
        "biography": "",
        "paragraphs": [
            "一随菱棹谒王侯，深愧移文负钓舟。",
            "今日太湖风色好，却将诗句乞鱼钩。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十九",
        "no#": 11
    },
    {
        "title": "长安春晚二首",
        "author": "温庭筠",
        "biography": "",
        "paragraphs": [
            "曲江春半日迟迟，正是王孙怅望时。",
            "杏花落尽不归去，江上东风吹柳丝。",
            "四方无事太平年，万象鲜明禁火前。",
            "九重细雨惹春色，轻染龙池杨柳烟。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十九",
        "no#": 12
    },
    {
        "title": "三月十八日雪中作",
        "author": "温庭筠",
        "biography": "",
        "paragraphs": [
            "芍药蔷薇语早梅，不知谁是艳阳才。",
            "今朝领得春风意，不复饶君雪里开。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十九",
        "no#": 13
    },
    {
        "title": "咸阳值雨",
        "author": "温庭筠",
        "biography": "",
        "paragraphs": [
            "咸阳桥上雨如悬，万点空蒙隔钓船。",
            "还似洞庭春水色，晓云将入岳阳天。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十九",
        "no#": 14
    },
    {
        "title": "弹筝人",
        "author": "温庭筠",
        "biography": "",
        "paragraphs": [
            "天宝年中事玉皇，曾将新曲教宁王。",
            "钿蝉金雁今零落，一曲伊州泪万行。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十九",
        "no#": 15
    },
    {
        "title": "瑶瑟怨",
        "author": "温庭筠",
        "biography": "",
        "paragraphs": [
            "冰簟银床梦不成，碧天如水夜云轻。",
            "雁声远过潇湘去，十二楼中月自明。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十九",
        "no#": 16
    },
    {
        "title": "题端正树",
        "author": "温庭筠",
        "biography": "",
        "paragraphs": [
            "路傍佳树碧云愁，曾侍金舆幸驿楼。",
            "草木荣枯似人事，绿阴寂寞汉陵秋。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十九",
        "no#": 17
    },
    {
        "title": "渭上题三首",
        "author": "温庭筠",
        "biography": "",
        "paragraphs": [
            "吕公荣达子陵归，万古烟波绕钓矶。",
            "桥上一通名利迹，至今江鸟背人飞。",
            "目极云霄思浩然，风帆一片水连天。",
            "轻桡便是东归路，不肯忘机作钓船。",
            "烟水何曾息世机，暂时相向亦依依。",
            "所嗟白首磻溪叟，一下渔舟更不归。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十九",
        "no#": 18
    },
    {
        "title": "经故翰林袁学士居",
        "author": "温庭筠",
        "biography": "",
        "paragraphs": [
            "剑逐惊波玉委尘，谢安门下更何人。",
            "西州城外花千树，尽是羊昙醉后春。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十九",
        "no#": 19
    },
    {
        "title": "题城南杜邠公林亭",
        "author": "温庭筠",
        "biography": "",
        "paragraphs": [
            "卓氏垆前金线柳，隋家堤畔锦帆风。",
            "贪为两地分霖雨，不见池莲照水红。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十九",
        "no#": 20
    },
    {
        "title": "夜看牡丹",
        "author": "温庭筠",
        "biography": "",
        "paragraphs": [
            "高低深浅一阑红，把火殷勤绕露丛。",
            "希逸近来成懒病，不能容易向春风。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十九",
        "no#": 21
    },
    {
        "title": "宿城南亡友别墅",
        "author": "温庭筠",
        "biography": "",
        "paragraphs": [
            "水流花落叹浮生，又伴游人宿杜城。",
            "还似昔年残梦里，透帘斜月独闻莺。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十九",
        "no#": 22
    },
    {
        "title": "过分水岭",
        "author": "温庭筠",
        "biography": "",
        "paragraphs": [
            "溪水无情似有情，入山三日得同行。",
            "岭头便是分头处，惜别潺湲一夜声。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十九",
        "no#": 23
    },
    {
        "title": "鄠杜郊居",
        "author": "温庭筠",
        "biography": "",
        "paragraphs": [
            "槿篱芳援近樵家，垄麦青青一径斜。",
            "寂寞游人寒食后，夜来风雨送梨花。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十九",
        "no#": 24
    },
    {
        "title": "题河中紫极宫",
        "author": "温庭筠",
        "biography": "",
        "paragraphs": [
            "昔年曾伴玉真游，每到仙宫即是秋。",
            "曼倩不归花落尽，满丛烟露月当楼。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十九",
        "no#": 25
    },
    {
        "title": "四皓",
        "author": "温庭筠",
        "biography": "",
        "paragraphs": [
            "商于甪里便成功，一寸沉机万古同。",
            "但得戚姬甘定分，不应真有紫芝翁。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十九",
        "no#": 26
    },
    {
        "title": "赠张炼师",
        "author": "温庭筠",
        "biography": "",
        "paragraphs": [
            "丹溪药尽变金骨，清洛月寒吹玉笙。",
            "他日隐居无访处，碧桃花发水纵横。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十九",
        "no#": 27
    }
]