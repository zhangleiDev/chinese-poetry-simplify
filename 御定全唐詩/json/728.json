[
    {
        "title": "吟叙",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "历代兴亡亿万心，圣人观古贵知今。",
            "古今成败无多事，月殿花台幸一吟。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 1
    },
    {
        "title": "闲吟",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "考摭妍媸用破心，剪裁千古献当今。",
            "闲吟不是闲吟事，事有闲思闲要吟。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 2
    },
    {
        "title": "唐虞门：唐尧",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "祅氛不起瑞烟轻，端拱垂衣日月明。",
            "传事四方无外役，茅茨深处土阶平。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 3
    },
    {
        "title": "唐虞门：虞舜",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "进善惩奸立帝功，功成揖让益温恭。",
            "满朝卿士多元凯，为黜兜苗与四凶。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 4
    },
    {
        "title": "唐虞门：舜妃",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "苍梧一望隔重云，帝子悲寻不记春。",
            "何事泪痕偏在竹，贞姿应念节高人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 5
    },
    {
        "title": "唐虞门：再吟",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "潇湘何代泣幽魂，骨化重泉志尚存。",
            "若地道中休下泪，不应新竹有啼痕。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 6
    },
    {
        "title": "三代门：夏禹",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "尧违天孽赖询谟，顿免洪波浸碧虚。",
            "海内生灵微伯禹，尽应随浪化为鱼。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 7
    },
    {
        "title": "三代门：再吟",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "万古龙门一旦开，无成甘死作黄能。",
            "司空定有匡尧术，九载之前何处来。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 8
    },
    {
        "title": "三代门：太康",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "师保何人为琢磨，安知父祖苦辛多。",
            "酒酣禽色方为乐，讵肯闲听五子歌。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 9
    },
    {
        "title": "三代门：后稷",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "人惟邦本本由农，旷古谁高后稷功。",
            "百谷且繁三曜在，牲牢郊祀信无穷。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 10
    },
    {
        "title": "三代门：文王",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "昭然明德报天休，礿祭惟馨胜杀牛。",
            "二老五侯何所诈，不归商受尽归周。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 11
    },
    {
        "title": "三代门：武王",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "文王寝膳武王随，内竖言安色始怡。",
            "七载岂堪囚羑里，一夫为报亦何疑。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 12
    },
    {
        "title": "三代门：太公",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "昌猎关西纣猎东，纣怜崇虎弃非熊。",
            "危邦自谓多麟凤，肯把王纲取钓翁。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 13
    },
    {
        "title": "三代门：再吟",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "东邻不事事西邻，御物卑和物自亲。",
            "天下言知天下者，兆人无主属贤人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 14
    },
    {
        "title": "三代门：又吟",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "千妖万态逞妍姿，破国亡家更是谁。",
            "匡政必能除苟媚，去邪当断勿狐疑。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 15
    },
    {
        "title": "三代门：子牙妻",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "陵柏无心竹变秋，不能同戚拟同休。",
            "岁寒焉在空垂涕，覆水如何欲再收。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 16
    },
    {
        "title": "三代门：周公",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "文武传芳百代基，几多贤哲守成规。",
            "仍闻吐握延儒素，犹恐民疵未尽知。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 17
    },
    {
        "title": "三代门：夷齐",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "让国由衷义亦乖，不知天命匹夫才。",
            "将除暴虐诚能阻，何异崎岖助纣来。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 18
    },
    {
        "title": "三代门：管蔡",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "伊商胡越尚同图，管蔡如何有异谟。",
            "不念祖宗危社稷，强于仁圣遣行诛。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 19
    },
    {
        "title": "三代门：成王",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "成王有过伯禽笞，圣惠能新日自奇。",
            "王道既成何所感，越裳呈瑞凤来仪。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 20
    },
    {
        "title": "三代门：幽王",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "狼烟篝火为边尘，烽候那宜悦妇人。",
            "厚德未闻闻厚色，不亡家国幸亡身。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 21
    },
    {
        "title": "三代门：平王",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "犬戎西集杀幽王，邦土何由不便亡。",
            "宜臼东来年更远，川流难绝信源长。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 22
    },
    {
        "title": "春秋战国门：祭足",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "吴鲁燕韩岂别宗，曾无外御但相攻。",
            "当时周郑谁为相，交质将何服远戎。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 23
    },
    {
        "title": "春秋战国门：再吟",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "周室衰微不共匡，干戈终日互争强。",
            "诸侯若解尊天子，列国何因次第亡。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 24
    },
    {
        "title": "春秋战国门：隐公",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "今古难堤是小人，苟希荣宠任相亲。",
            "陈谋不信怀忧惧，反间须防却害身。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 25
    },
    {
        "title": "春秋战国门：庄公",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "齐甲强临力有余，鲁庄为战念区区。",
            "鱼丽三鼓微曹刿，肉食安能暇远谟。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 26
    },
    {
        "title": "春秋战国门：哀公",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "贤为邻用国忧危，庙算无非委艳奇。",
            "两叶翠娥春乍展，一毛须去不难吹。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 27
    },
    {
        "title": "春秋战国门：再吟",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "好龙天为降真龙，及见真龙瘁厥躬。",
            "接下不勤徒好士，叶公何异鲁哀公。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 28
    },
    {
        "title": "春秋战国门：平公",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "鸿鹄轻腾万里高，何殊朝野得贤豪。",
            "能知翼戴穹苍力，不是蒙茸腹背毛。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 29
    },
    {
        "title": "春秋战国门：文公",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "灭虢吞虞未息兵，柔秦败楚霸威成。",
            "文公徒欲三强服，分晋元来是六卿。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 30
    },
    {
        "title": "春秋战国门：景公",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "觉病当宜早问师，病深难疗恨难追。",
            "晋侯徒有秦医缓，疾在膏肓救已迟。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 31
    },
    {
        "title": "春秋战国门：卫灵公",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "子鱼无隐欲源清，死不忘忠感卫灵。",
            "伯玉既亲知德润，残桃休吃悟兰馨。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 32
    },
    {
        "title": "春秋战国门：陈灵公",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "谁与陈君嫁祸来，孔宁行父夏姬媒。",
            "灵公徒认征舒面，至死何曾识祸胎。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 33
    },
    {
        "title": "春秋战国门：陈蔡君",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "楚聘宣尼欲道光，是时陈蔡畏邻强。",
            "庸谋但解遮贤路，不解迎贤谋自昌。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 34
    },
    {
        "title": "春秋战国门：楚惠王",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "芹中遇蛭强为吞，不欲缘微有害人。",
            "何事免成心腹疾，皇天惟德是相亲。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 35
    },
    {
        "title": "春秋战国门：楚怀王",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "不听陈轸信张仪，六里商于果见欺。",
            "既舍黔中西换得，又令生去益堪悲。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 36
    },
    {
        "title": "春秋战国门：再吟",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "不得商于又失齐，楚怀方寸一何迷。",
            "明知秦是虎狼国，更忍车轮独向西。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 37
    },
    {
        "title": "春秋战国门：韩惠王",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "韩惠开渠止暴秦，营田万顷饱秦人。",
            "何殊般肉供羸兽，兽壮安知不害身。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 38
    },
    {
        "title": "春秋战国门：顷襄王",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "秦陷荆王死不还，只缘偏听子兰言。",
            "顷襄还信子兰语，忍使江鱼葬屈原。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 39
    },
    {
        "title": "春秋战国门：武公",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "猛兽来兵只为文，岂宜凉德拟图尊。",
            "君看豹彩蒙麋质，人取无难必不存。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 40
    },
    {
        "title": "春秋战国门：华元",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "未知军法忌偏颇，徒解于思腹漫皤。",
            "昔日羊斟曾不预，今朝为政事如何。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 41
    },
    {
        "title": "春秋战国门：臧孙",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "诸孟憎吾似犬狞，贤臧哭孟倍伤情。",
            "季孙爱我如甘疾，疾足亡身药故宁。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 42
    },
    {
        "title": "春秋战国门：公叔",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "吴起南奔魏国荒，必听公叔失贤良。",
            "无谋纵欲离安邑，可免河沟徙大梁。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 43
    },
    {
        "title": "春秋战国门：庄辛",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "庄辛正谏谓妖词，兵及鄢陵始悔思。",
            "见兔必能知顾犬，亡羊补栈未为迟。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 44
    },
    {
        "title": "春秋战国门：孙膑",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "曾嫌胜己害贤人，钻火明知速自焚。",
            "断足尔能行不足，逢君谁肯不酬君。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 45
    },
    {
        "title": "春秋战国门：灵辄",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "失水枯鳞得再生，翳桑无地谢深情。",
            "朱轮未染酬恩血，公子何由见赤诚。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 46
    },
    {
        "title": "春秋战国门：郭开",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "秦袭邯郸岁月深，何人沾赠郭开金。",
            "廉颇还国李牧在，安得赵王为尔擒。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 47
    },
    {
        "title": "春秋战国门：乐羊",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "杯羹忍啜得非忠，巧佞胡为惑主聪。",
            "盈箧谤书能寝默，中山不是乐羊功。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 48
    },
    {
        "title": "春秋战国门：虞卿",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "割地求和国必危，安知坚守绝来思。",
            "年年来伐年年割，割尽邯郸何所之。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 49
    },
    {
        "title": "春秋战国门：豫让",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "门客家臣义莫俦，漆身吞炭不能休。",
            "中行智伯思何异，国士终期国士酬。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 50
    },
    {
        "title": "春秋战国门：毛遂",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "不识囊中颖脱锥，功成方信有英奇。",
            "平原门下三千客，得力何曾是素知。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 51
    },
    {
        "title": "春秋战国门：再吟",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "定获英奇不在多，然须设网遍山河。",
            "禽虽一目罗中得，岂可空张一目罗。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 52
    },
    {
        "title": "春秋战国门：田文",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "下客常才不足珍，谁为狗盗脱强秦。",
            "秦关若待鸡鸣出，笑杀临淄土偶人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 53
    },
    {
        "title": "春秋战国门：再吟",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "门下三千各自矜，频弹剑客独无能。",
            "田文不厌无能客，三窟全身果有凭。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 54
    },
    {
        "title": "春秋战国门：冯讙",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "兔窟穿成主再兴，辈流狐伏敢骄矜。",
            "冯讙不是无能者，要试君心欲展能。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 55
    },
    {
        "title": "春秋战国门：章子",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "在家能子必能臣，齐将功成以孝闻。",
            "改葬义无欺死父，临戎安肯背生君。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 56
    },
    {
        "title": "春秋战国门：卞和",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "磷磷谁为惑温温，至宝凡姿甚易分。",
            "自是时人多贵耳，目无明鉴使俱焚。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 57
    },
    {
        "title": "春秋战国门：季劄",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "吹毛霜刃过千金，生许徐君死挂林。",
            "宝剑徒称无价宝，行心更贵不欺心。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 58
    },
    {
        "title": "春秋战国门：孙武",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "理国无难似理兵，兵家法令贵遵行。",
            "行刑不避君王宠，一笑随刀八阵成。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 59
    },
    {
        "title": "春秋战国门：夫差",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "信听谗言疾不除，忠臣须杀竟何如。",
            "会稽既雪夫差死，泉下胡颜见子胥。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 60
    },
    {
        "title": "春秋战国门：少孺",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "宝贵亲仁与善邻，邻兵何要互相臻。",
            "螳螂定是遭黄雀，黄雀须防挟弹人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 61
    },
    {
        "title": "春秋战国门：苏厉",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "百步穿杨箭不移，养由堪教听弘规。",
            "身隆业著未知退，勿遣功名一旦隳。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 62
    },
    {
        "title": "春秋战国门：鬻拳",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "鬻拳强谏惧威刑，退省怀惭不顾生。",
            "双刖忍行留痛恨，惟君适足见忠诚。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 63
    },
    {
        "title": "春秋战国门：荆轲",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "反刃相酬是匹夫，安知突骑驾群胡。",
            "有心为报怀权略，可在于期与地图。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 64
    },
    {
        "title": "春秋战国门：再吟",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "几尺如霜利不群，恩仇未报反亡身。",
            "诚哉利器全由用，可惜吹毛不得人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 65
    },
    {
        "title": "春秋战国门：陈轸",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "丹青徒有逞喧哗，有足由来不是蛇。",
            "杀将破军为柱国，君今官极更何加。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 66
    },
    {
        "title": "春秋战国门：田饶",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "厨抛败肉士怀饥，仓烂余粮客未炊。",
            "临难欲行求死士，将何恩信致扶危。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 67
    },
    {
        "title": "春秋战国门：鲍叔",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "忠臣祝寿吐嘉词，鲍叔临轩酒一卮。",
            "安不忘危臣所愿，愿思危困必无危。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 68
    },
    {
        "title": "春秋战国门：晏婴",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "正人徒以刃相危，贪利忘忠死不为。",
            "麋鹿命悬当有处，驱车何必用宾士。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 69
    },
    {
        "title": "春秋战国门：再吟",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "下泽逢蛇盖是常，还如山上见豺狼。",
            "国中有怪非蛇兽，不用贤能是不祥。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 70
    },
    {
        "title": "春秋战国门：又吟",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "马毙厩人欲就刑，百年临尽一言生。",
            "赖逢贤相能匡救，仍免吾君播恶声。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 71
    },
    {
        "title": "春秋战国门：叔向",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "重禄存家不敢言，小臣忧祸亦如然。",
            "明开谏诤能无罪，只此宜为理国先。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 72
    },
    {
        "title": "春秋战国门：师旷",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "老能劝学照余生，似夜随灯到处明。",
            "往行前言如不见，暗中无烛若为行。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 73
    },
    {
        "title": "春秋战国门：智伯",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "三国连兵敌就擒，晋阳城下碧波深。",
            "风涛撼处看沈赵，舟楫不从翻自沈。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 74
    },
    {
        "title": "春秋战国门：再吟",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "攻城来下惜先分，一旦家邦属四邻。",
            "徒逞威强称智伯，不知权变是愚人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 75
    },
    {
        "title": "春秋战国门：襄子",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "君子常闻不迫危，城崩何用急重围。",
            "叛亡能退修文德，果见中牟以义归。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 76
    },
    {
        "title": "春秋战国门：杨回",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "三逐乡闾五去君，莫知何地可容身。",
            "杨回不是逢英鉴，白首无成一旅人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 77
    },
    {
        "title": "春秋战国门：颜回",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "陋巷箪瓢困有年，是时端木饫腥膻。",
            "宣尼行教何形迹，不肯分甘救子渊。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 78
    },
    {
        "title": "春秋战国门：子贡",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "救鲁亡吴事可伤，谁令利口说田常。",
            "吴亡必定由端木，鲁亦宜其运不长。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 79
    },
    {
        "title": "春秋战国门：再吟",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "一言能使定安危，安己危人是所宜。",
            "仁义不思垂教化，背恩亡德岂儒为。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 80
    },
    {
        "title": "春秋战国门：郑相",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "郑相清贤慎有余，好鱼鱼至竟何如。",
            "退鱼留得终身禄，禄在何忧不得鱼。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 81
    },
    {
        "title": "春秋战国门：子产",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "为政何门是化源，宽仁高下保安全。",
            "如嫌水德人多狎，拯溺宜将猛济宽。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 82
    },
    {
        "title": "春秋战国门：管仲",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "美酒浓馨客要沽，门深谁敢强提壶。",
            "苟非贤主询贤士，肯信沽人畏子獹。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 83
    },
    {
        "title": "春秋战国门：再吟",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "社鼠穿墙巧庇身，何由攻灌若为熏。",
            "能知窟穴依形势，不听谗邪是圣君。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 84
    },
    {
        "title": "春秋战国门：范蠡",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "西子能令转嫁吴，会稽知尔啄姑苏。",
            "迹高尘外功成处，一叶翩翩在五湖。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 85
    },
    {
        "title": "春秋战国门：屈原",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "满朝皆醉不容醒，众浊如何拟独清。",
            "江上流人真浪死，谁知浸润误深诚。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 86
    },
    {
        "title": "春秋战国门：黄歇",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "春申随质若王图，为主轻生大丈夫。",
            "女子异心安足听，功成何更用阴谟。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 87
    },
    {
        "title": "春秋战国门：王后",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "连环要解解非难，忽碎瑶阶一旦间。",
            "两国相持兵不解，会应俱碎似连环。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 88
    },
    {
        "title": "春秋战国门：樊姬",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "侧影频移未退朝，喜逢贤相日从高。",
            "当时不有樊姬问，令尹何由进叔敖。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 89
    },
    {
        "title": "春秋战国门：齐桓公",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "三往何劳万乘君，五来方见一微臣。",
            "微臣傲爵能轻主，霸主如何敢傲人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 90
    },
    {
        "title": "春秋战国门：中山君",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "敌临烹子一何庸，激怒来军速自攻。",
            "结怨岂思围不解，愚谋多以杀为雄。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 91
    },
    {
        "title": "春秋战国门：赵简子",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "简子雄心蓄霸机，贤愚聊欲试诸儿。",
            "假言藏宝非真宝，不是生知焉得知。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 92
    },
    {
        "title": "春秋战国门：再吟",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "谔谔能昌唯唯亡，亦由匡正得贤良。",
            "一从忠谠无周舍，吾过何人为短长。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 93
    },
    {
        "title": "春秋战国门：赵宣子",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "门人曾不有提弥，连嗾呀呀孰敢支。",
            "临难若教无苟免，乱朝争那以獒为。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 94
    },
    {
        "title": "春秋战国门：韩昭侯",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "去年秦伐我宜阳，今岁天灾旱且荒。",
            "对此不思人力困，楼门何可更高张。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 95
    },
    {
        "title": "春秋战国门：魏文侯",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "冒雨如何固出畋，虑乖群约失干干。",
            "文侯不是贪禽者，示信将为教化先。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 96
    },
    {
        "title": "春秋战国门：郤成子",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "陈乐无欢璧在隅，宰臣怀智有微谟。",
            "苟非成子当明哲，谁是仁人可托孤。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 97
    },
    {
        "title": "春秋战国门：秦武阳",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "丱岁徒闻有壮名，及令为副误荆卿。",
            "是时环柱能相副，谁谓燕囚事不成。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 98
    },
    {
        "title": "春秋战国门：田子方",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "太子无嫌礼乐亏，愿听贫富与安危。",
            "贱贫骄物贫终在，富贵骄人贵必隳。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 99
    },
    {
        "title": "春秋战国门：淳于髡",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "穰穰何祷手何赍，一呷村浆与只鸡。",
            "以少求多诚可笑，还如轻币欲全齐。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 100
    },
    {
        "title": "春秋战国门：再吟",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "戏问将何对所耽，滑稽无骨是常谭。",
            "昔时王者皆通四，近见君王只好三。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 101
    },
    {
        "title": "春秋战国门：再吟",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "曲突徙薪不谓贤，焦头烂额飨盘筵。",
            "时人多是轻先见，不独田家国亦然。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 102
    },
    {
        "title": "春秋战国门：田子奇",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "少年为吏虑非循，一骑奔追委使臣。",
            "使者不追何所对，车中缘见白头人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 103
    },
    {
        "title": "春秋战国门：百里奚",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "船骥由来是股肱，在虞虞灭在秦兴。",
            "裁量何异刀将尺，只系用之能不能。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 104
    },
    {
        "title": "春秋战国门：孙叔敖",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "童稚逢蛇叹不祥，虑悲来者为埋藏。",
            "是知阳报由阴施，天爵昭然契日彰。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 105
    },
    {
        "title": "春秋战国门：鲁仲连",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "昔迸烧牛发战机，夜奔惊火走燕师。",
            "今来跃马怀骄惰，十万如无一撮时。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 106
    },
    {
        "title": "春秋战国门：宋子罕",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "子怜温润欲归仁，吾贵坚廉是宝身。",
            "自有不贪身内宝，玉人徒献外来珍。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 107
    },
    {
        "title": "春秋战国门：宫之奇",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "虞虢相依自保安，谋臣吞度不为难。",
            "贪怜璧马迷香饵，肯信之奇谕齿寒。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 108
    },
    {
        "title": "春秋战国门：王孙满",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "九牧金熔物像成，辞昏去乱祚休明。",
            "兴亡在德不在鼎，楚子何劳问重轻。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 109
    },
    {
        "title": "春秋战国门：颜叔子",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "夜雨邻娃告屋倾，一宵从寄念悲惊。",
            "诚知独处从烧烛，君子行心要自明。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 110
    },
    {
        "title": "春秋战国门：张孟谭",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "强兵四合国将危，赖有谋臣为发挥。",
            "城内蒿铜诚自有，无谋谁解见玄机。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 111
    },
    {
        "title": "春秋战国门：公子无忌",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "按剑临笼震咄呼，鹞甘枭戮伏鸠辜。",
            "能怜钝拙诛豪俊，悯弱摧强真丈夫。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 112
    },
    {
        "title": "春秋战国门：再吟",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "赵解重围魏再昌，信陵贤德日馨芳。",
            "昏蒙愚主听谗说，公子云亡国亦亡。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 113
    },
    {
        "title": "春秋战国门：侯嬴朱亥",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "屠肆监门一贱微，信陵交结国人非。",
            "当时不是二君计，匹马那能解赵围。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 114
    },
    {
        "title": "春秋战国门：再吟",
        "author": "周昙",
        "biography": "",
        "paragraphs": [
            "走敌存亡义有余，全由雄勇与英谟。",
            "但如公子能交结，朱亥侯嬴何代无。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百二十八",
        "no#": 115
    }
]