[
    {
        "title": "奉和圣制从蓬莱向兴庆阁道中留春雨中春望之作应制",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "渭水自萦秦塞曲，黄山旧绕汉宫斜。銮舆迥出千门柳，",
            "为乘阳气行时令，不是宸游玩物华。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 1
    },
    {
        "title": "大同殿柱产玉芝龙池上有庆云神光照殿百官共覩圣恩便赐宴乐敢书即事",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "欲笑周文歌宴镐，遥轻汉武乐横汾。岂知玉殿生三秀，",
            "共欢天意同人意，万岁千秋奉圣君。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 2
    },
    {
        "title": "敕赐百官樱桃",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "芙蓉阙下会千官，紫禁朱樱出上阑。才是寝园春荐后，",
            "饱食不须愁内热，大官还有蔗浆寒。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 3
    },
    {
        "title": "敕借岐王九成宫避暑应教",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "帝子远辞丹凤阙，天书遥借翠微宫。隔窗云雾生衣上，",
            "仙家未必能胜此，何事吹笙向碧空。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 4
    },
    {
        "title": "和贾舍人早朝大明宫之作",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "绛帻鸡人送晓筹，尚衣方进翠云裘。九天阊阖开宫殿，",
            "朝罢须裁五色诏，佩声归向凤池头。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 5
    },
    {
        "title": "和太常韦主簿五郎温汤寓目之作",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "汉主离宫接露台，秦川一半夕阳开。青山尽是朱旗绕，",
            "闻道甘泉能献赋，悬知独有子云才。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 6
    },
    {
        "title": "苑舍人能书梵字兼达梵音，皆曲尽其妙，戏为之赠",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "名儒待诏满公车。才子为郎典石渠。莲花法藏心悬悟，",
            "故旧相望在三事，愿君莫厌承明庐。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 7
    },
    {
        "title": "重酬苑郎中",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "何幸含香奉至尊，多惭未报主人恩。草木尽能酬雨露，",
            "扬子解嘲徒自遣，冯唐已老复何论。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 8
    },
    {
        "title": "酬郭给事",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "洞门高阁霭余辉，桃李阴阴柳絮飞。禁里疏钟官舍晚，",
            "强欲从君无那老，将因卧病解朝衣。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 9
    },
    {
        "title": "出塞",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "居延城外猎天骄，白草连山野火烧。暮云空碛时驱马，",
            "玉靶角弓珠勒马，汉家将赐霍嫖姚。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 10
    },
    {
        "title": "既蒙宥罪旋复拜官伏感圣恩窃书鄙意兼奉简新除使君等诸公",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "忽蒙汉诏还冠冕，始觉殷王解网罗。日比皇明犹自暗，",
            "闻道百城新佩印，还来双阙共鸣珂。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 11
    },
    {
        "title": "送方尊师归嵩山",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "仙官欲往九龙潭，旄节朱幡倚石龛。山压天中半天上，",
            "借问迎来双白鹤，已曾衡岳送苏耽。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 12
    },
    {
        "title": "送杨少府贬郴州",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "明到衡山与洞庭，若为秋月听猿声。愁看北渚三湘远，",
            "长沙不久留才子，贾谊何须吊屈平。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 13
    },
    {
        "title": "过乘如禅师萧居士嵩丘兰若",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "无著天亲弟与兄，嵩丘兰若一峰晴。食随鸣磬巢乌下，",
            "深洞长松何所有，俨然天竺古先生。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 14
    },
    {
        "title": "春日与裴迪过新昌里访吕逸人不遇",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "桃源一向绝风尘，柳市南头访隐沦。到门不敢题凡鸟，",
            "闭户著书多岁月，种松皆老作龙鳞。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 15
    },
    {
        "title": "酌酒与裴迪",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "酌酒与君君自宽，人情翻覆似波澜。白首相知犹按剑，",
            "世事浮云何足问，不如高卧且加餐。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 16
    },
    {
        "title": "辋川别业",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "不到东山向一年，归来才及种春田。雨中草色绿堪染，",
            "披衣倒屣且相见，相欢语笑衡门前。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 17
    },
    {
        "title": "早秋山中作",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "无才不敢累明时，思向东溪守故篱。岂厌尚平婚嫁早，",
            "寂寞柴门人不到，空林独与白云期。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 18
    },
    {
        "title": "积雨辋川庄作",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "积雨空林烟火迟，蒸藜炊黍饷东菑。漠漠水田飞白鹭，",
            "野老与人争席罢，海鸥何事更相疑。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 19
    },
    {
        "title": "听百舌鸟",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "上兰门外草萋萋，未央宫中花里栖。亦有相随过御苑，",
            "万户千门应觉晓，建章何必听鸣鸡。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 20
    },
    {
        "title": "息夫人",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "莫以今时宠，难忘旧日恩。看花满眼泪，不共楚王言。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 21
    },
    {
        "title": "班婕妤三首",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "玉窗萤影度，金殿人声绝。秋夜守罗帷，孤灯耿不灭。",
            "宫殿生秋草，君王恩幸疏。那堪闻凤吹，门外度金舆。",
            "怪来妆阁闭，朝下不相迎。总向春园里，花间笑语声。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 22
    },
    {
        "title": "辋川集：孟城坳",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "新家孟城口，古木余衰柳。来者复为谁，空悲昔人有。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 23
    },
    {
        "title": "辋川集：华子冈",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "飞鸟去不穷，连山复秋色。上下华子冈，惆怅情何极。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 24
    },
    {
        "title": "辋川集：文杏馆",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "文杏裁为梁，香茅结为宇。不知栋里云，去作人间雨。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 25
    },
    {
        "title": "辋川集：斤竹岭",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "檀栾映空曲，青翠漾涟漪。暗入商山路，樵人不可知。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 26
    },
    {
        "title": "辋川集：鹿柴",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "空山不见人，但闻人语响。返景入深林，复照青苔上。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 27
    },
    {
        "title": "辋川集：木兰柴",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "秋山敛余照，飞鸟逐前侣。彩翠时分明，夕岚无处所。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 28
    },
    {
        "title": "辋川集：茱萸沜",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "结实红且绿，复如花更开。山中傥留客，置此芙蓉杯。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 29
    },
    {
        "title": "辋川集：宫槐陌",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "仄径荫宫槐，幽阴多绿苔。应门但迎扫，畏有山僧来。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 30
    },
    {
        "title": "辋川集：临湖亭",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "轻舸迎上客，悠悠湖上来。当轩对尊酒，四面芙蓉开。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 31
    },
    {
        "title": "辋川集：南垞",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "轻舟南垞去，北垞淼难即。隔浦望人家，遥遥不相识。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 32
    },
    {
        "title": "辋川集：欹湖",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "吹箫凌极浦，日暮送夫君。湖上一回首，青山卷白云。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 33
    },
    {
        "title": "辋川集：柳浪",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "分行接绮树，倒影入清漪。不学御沟上，春风伤别离。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 34
    },
    {
        "title": "辋川集：栾家濑",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "飒飒秋雨中，浅浅石溜泻。跳波自相溅，白鹭惊复下。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 35
    },
    {
        "title": "辋川集：金屑泉",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "日饮金屑泉，少当千余岁。翠凤翊文螭，羽节朝玉帝。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 36
    },
    {
        "title": "辋川集：白石滩",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "清浅白石滩，绿蒲向堪把。家住水东西，浣纱明月下。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 37
    },
    {
        "title": "辋川集：北垞",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "北垞湖水北，杂树映朱阑。逶迤南川水，明灭青林端。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 38
    },
    {
        "title": "辋川集：竹里馆",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "独坐幽篁里，弹琴复长啸。深林人不知，明月来相照。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 39
    },
    {
        "title": "辋川集：辛夷坞",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "木末芙蓉花，山中发红萼。涧户寂无人，纷纷开且落。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 40
    },
    {
        "title": "辋川集：漆园",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "古人非傲吏，自阙经世务。偶寄一微官，婆娑数株树。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 41
    },
    {
        "title": "辋川集：椒园",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "桂尊迎帝子，杜若赠佳人。椒浆奠瑶席，欲下云中君。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 42
    },
    {
        "title": "皇甫岳云溪杂题五首：鸟鸣涧",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "人闲桂花落，夜静春山空。月出惊山鸟，时鸣春涧中。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 43
    },
    {
        "title": "皇甫岳云溪杂题五首：莲花坞",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "日日采莲去，洲长多暮归。弄篙莫溅水，畏湿红莲衣。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 44
    },
    {
        "title": "皇甫岳云溪杂题五首：鸬鹚堰",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "乍向红莲没，复出清蒲飏。独立何褵褷，衔鱼古查上。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 45
    },
    {
        "title": "皇甫岳云溪杂题五首：上平田",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "朝耕上平田，暮耕上平田。借问问津者，宁知沮溺贤。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 46
    },
    {
        "title": "皇甫岳云溪杂题五首：萍池",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "春池深且广，会待轻舟回。靡靡绿萍合，垂杨扫复开。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 47
    },
    {
        "title": "答裴迪辋口遇雨忆终南山之作",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "淼淼寒流广，苍苍秋雨晦。君问终南山，心知白云外。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 48
    },
    {
        "title": "山中寄诸弟妹",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "山中多法侣，禅诵自为群。城郭遥相望，唯应见白云。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 49
    },
    {
        "title": "闻裴秀才迪吟诗因戏赠",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "猿吟一何苦，愁朝复悲夕。莫作巫峡声，肠断秋江客。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 50
    },
    {
        "title": "赠韦穆十八",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "与君青眼客，共有白云心。不向东山去，日令春草深。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 51
    },
    {
        "title": "送别",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "山中相送罢，日暮掩柴扉。春草明年绿，王孙归不归。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 52
    },
    {
        "title": "临高台送黎拾遗",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "相送临高台，川原杳何极。日暮飞鸟还，行人去不息。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 53
    },
    {
        "title": "别辋川别业",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "依迟动车马，惆怅出松萝。忍别青山去，其如绿水何。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 54
    },
    {
        "title": "崔九弟欲往南山马上口号与别",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "城隅一分手，几日还相见。山中有桂花，莫待花如霰。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 55
    },
    {
        "title": "题友人云母障子",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "君家云母障，时向野庭开。自有山泉入，非因采画来。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 56
    },
    {
        "title": "红牡丹",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "绿艳闲且静，红衣浅复深。花心愁欲断，春色岂知心。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 57
    },
    {
        "title": "左掖梨花",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "闲洒阶边草，轻随箔外风。黄莺弄不足，衔入未央宫。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 58
    },
    {
        "title": "菩提寺禁口号又示裴迪",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "安得舍罗网，拂衣辞世喧。悠然策藜杖，归向桃花源。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 59
    },
    {
        "title": "杂诗三首",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "家住孟津河，门对孟津口。常有江南船，寄书家中否。",
            "君自故乡来，应知故乡事。来日绮窗前，寒梅著花未。",
            "已见寒梅发，复闻啼鸟声。心心视春草，畏向阶前生。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 60
    },
    {
        "title": "崔兴宗写真咏",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "画君年少时，如今君已老。今时新识人，知君旧时好。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 61
    },
    {
        "title": "山茱萸",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "朱实山下开，清香寒更发。幸与丛桂花，窗前向秋月。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 62
    },
    {
        "title": "相思",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "红豆生南国，秋来发故枝。愿君多采撷，此物最相思。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 63
    },
    {
        "title": "书事",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "轻阴阁小雨，深院昼慵开。坐看苍苔色，欲上人衣来。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 64
    },
    {
        "title": "哭孟浩然",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "故人不可见，汉水日东流。借问襄阳老，江山空蔡州。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 65
    },
    {
        "title": "阙题二首",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "荆溪白石出，天寒红叶稀。山路元无雨，空翠湿人衣。",
            "相看不忍发，惨澹暮潮平。语罢更携手，月明洲渚生。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 66
    },
    {
        "title": "田园乐七首",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "厌见千门万户，经过北里南邻。",
            "官府鸣珂有底，崆峒散发何人。",
            "再见封侯万户，立谈赐璧一双。",
            "讵胜耦耕南亩，何如高卧东窗。",
            "采菱渡头风急，策杖林西日斜。",
            "杏树坛边渔父，桃花源里人家。",
            "萋萋春草秋绿，落落长松夏寒。",
            "牛羊自归村巷，童稚不识衣冠。",
            "山下孤烟远村，天边独树高原。",
            "一瓢颜回陋巷，五柳先生对门。",
            "桃红复含宿雨，柳绿更带朝烟。",
            "花落家童未扫，莺啼山客犹眠。",
            "酌酒会临泉水，抱琴好倚长松。",
            "南园露葵朝折，东谷黄粱夜舂。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 67
    },
    {
        "title": "少年行四首",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "新丰美酒斗十千，咸阳游侠多少年。",
            "相逢意气为君饮，系马高楼垂柳边。",
            "出身仕汉羽林郎，初随骠骑战渔阳。",
            "孰知不向边庭苦，纵死犹闻侠骨香。",
            "一身能擘两雕弧，虏骑千重只似无。",
            "偏坐金鞍调白羽，纷纷射杀五单于。",
            "汉家君臣欢宴终，高议云台论战功。",
            "天子临轩赐侯印，将军佩出明光宫。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 68
    },
    {
        "title": "赠裴旻将军",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "腰间宝剑七星文，臂上雕弓百战勋。",
            "见说云中擒黠虏，始知天上有将军。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 69
    },
    {
        "title": "九月九日忆山东兄弟",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "独在异乡为异客，每逢佳节倍思亲。",
            "遥知兄弟登高处，遍插茱萸少一人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 70
    },
    {
        "title": "送王尊师归蜀中拜扫",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "大罗天上神仙客，濯锦江头花柳春。",
            "不为碧鸡称使者，唯令白鹤报乡人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 71
    },
    {
        "title": "渭城曲",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "渭城朝雨浥轻尘，客舍青青杨柳春。",
            "劝君更尽一杯酒，西出阳关无故人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 72
    },
    {
        "title": "齐州送祖二",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "送君南浦泪如丝，君向东州使我悲。",
            "为报故人憔悴尽，如今不似洛阳时。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 73
    },
    {
        "title": "送韦评事",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "欲逐将军取右贤，沙场走马向居延。",
            "遥知汉使萧关外，愁见孤城落日边。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 74
    },
    {
        "title": "灵云池送从弟",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "金杯缓酌清歌转，画舸轻移艳舞回。",
            "自叹鹡鸰临水别，不同鸿雁向池来。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 75
    },
    {
        "title": "送沈子归江东",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "杨柳渡头行客稀，罟师荡桨向临圻。",
            "唯有相思似春色，江南江北送君归。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 76
    },
    {
        "title": "与卢员外象过崔处士兴宗林亭",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "绿树重阴盖四邻，青苔日厚自无尘。",
            "科头箕踞长松下，白眼看他世上人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 77
    },
    {
        "title": "寒食汜上作",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "广武城边逢暮春，汶阳归客泪沾巾。",
            "落花寂寂啼山鸟，杨柳青青渡水人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 78
    },
    {
        "title": "戏题辋川别业",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "柳条拂地不须折，松树披云从更长。",
            "藤花欲暗藏猱子，柏叶初齐养麝香。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 79
    },
    {
        "title": "戏题磐石",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "可怜磐石临泉水，复有垂杨拂酒杯。",
            "若道春风不解意，何因吹送落花来。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 80
    },
    {
        "title": "寄河上段十六",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "与君相见即相亲，闻道君家在孟津。",
            "为见行舟试借问，客中时有洛阳人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 81
    },
    {
        "title": "菩提寺禁裴迪来相看说逆贼等凝碧池上作音乐供奉人等举声便一时泪下私成口号诵示裴迪",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "万户伤心生野烟，百僚何日更朝天。",
            "秋槐叶落空宫里，凝碧池头奏管弦。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 82
    },
    {
        "title": "凉州赛神",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "凉州城外少行人，百尺峰头望虏尘。",
            "健儿击鼓吹羌笛，共赛城东越骑神。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 83
    },
    {
        "title": "戏嘲史寰",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "清风细雨湿梅花，骤马先过碧玉家。",
            "正值楚王宫里至，门前初下七香车。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 84
    },
    {
        "title": "叹白发",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "宿昔朱颜成暮齿，须臾白发变垂髫。",
            "一生几许伤心事，不向空门何处销。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 85
    },
    {
        "title": "伊州歌",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "清风明月苦相思，荡子从戎十载余。",
            "征人去日殷勤嘱，归雁来时数附书。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 86
    },
    {
        "title": "送殷四葬",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "送君返葬石楼山，松柏苍苍宾驭还。",
            "埋骨白云长已矣，空余流水向人间。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 87
    },
    {
        "title": "疑梦",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "莫惊宠辱空忧喜，莫计恩雠浪苦辛。",
            "黄帝孔丘何处问，安知不是梦中身。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 88
    },
    {
        "title": "句",
        "author": "王维",
        "biography": "",
        "paragraphs": [
            "人家在仙掌，云气欲生衣。",
            "知古斋主精校2000。01。22。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百二十八",
        "no#": 89
    }
]