[
    {
        "title": "晚秋拾遗朱放访山居",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "不逐时人后，终年独闭关。家中贫自乐，石上卧常闲。",
            "坠栗添新味，寒花带老颜。侍臣当献纳，那得到空山。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 1
    },
    {
        "title": "题女道士居",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "不饵住云溪，休丹罢药畦。杏花虚结子，石髓任成泥。",
            "扫地青牛卧，栽松白鹤栖。共知仙女丽，莫是阮郎妻。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 2
    },
    {
        "title": "山中枉张宙员外书期访衡门",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "常恨相知晚，朝来枉数行。卧云惊圣代，拂石候仙郎。",
            "时果连枝熟，春醪满瓮香。贫家仍有趣，山色满湖光。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 3
    },
    {
        "title": "山中赠张正则评事",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "终年常避喧，师事五千言。流水闲过院，春风与闭门。",
            "山茶邀上客，桂实落前轩。莫强教余起，微官不足论。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 4
    },
    {
        "title": "题镜湖野老所居",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "湖里寻君去，樵风往返吹。树喧巢鸟出，路细葑田移。",
            "沤苎成鱼网，枯根是酒卮。老年唯自适，生事任群儿。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 5
    },
    {
        "title": "早秋宿崔业居处",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "从来席不暖，为尔便淹留。鸡黍今相会，云山昔共游。",
            "上帘宜晚景，卧簟觉新秋。身事何须问，余心正四愁。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 6
    },
    {
        "title": "赠乌程杨苹明府",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "策杖政成时，清溪弄钓丝。当年潘子貌，避病沈侯诗。",
            "漉酒迎宾急，看花署字迟。杨梅今熟未，与我两三枝。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 7
    },
    {
        "title": "徐侍郎素未相识时携酒命馔兼命诸诗客同访会稽山居",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "忽道仙翁至，幽人学拜迎。华簪窥瓮牖，珍味代藜羹。",
            "洗砚鱼仍戏，移樽鸟不惊。兰亭攀叙却，会此越中营。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 8
    },
    {
        "title": "春日闲居三首",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "一似桃源隐，将令过客迷。碍冠门柳长，惊梦院莺啼。",
            "浇药泉流细，围棋日影低。举家无外事，共爱草萋萋。",
            "长谣朝复暝，幽独几人知。老鹤兼雏弄，丛篁带笋移。",
            "白云将袖拂，青镜出簷窥。邀取渔家叟，花间把酒卮。",
            "寂寂池亭里，轩窗间绿苔。游鱼牵荇没，戏鸟踏花摧。",
            "小径僧寻去，高峰鹿下来。中年曾屡辟，多病复迟回。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 9
    },
    {
        "title": "题石室山王宁所居",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "白云知所好，柏叶幸加餐。石镜妻将照，仙书我借看。",
            "鸟来翻药碗，猿饮怕鱼竿。借问簷前树，何枝曾挂冠。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 10
    },
    {
        "title": "送王道士",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "真人俄整舄，双鹤屡飞翔。恐入壶中住，须传肘后方。",
            "霓裳云气润，石径术苗香。一去何时见，仙家日月长。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 11
    },
    {
        "title": "将移耶溪旧居留赠严维秘书",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "鸡犬渔舟里，长谣任兴行。那邀落日醉，已被远山迎。",
            "书箧将非重，荷衣著甚轻。谢安无个事，忽起为苍生。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 12
    },
    {
        "title": "秋日过僧惟则故院",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "衰草经行处，微灯旧道场。门人失谭柄，野鸟上禅床。",
            "科斗书空古，栴檀钵自香。今朝数行泪，却洒约公房。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 13
    },
    {
        "title": "山中奉寄钱起员外兼简苗发员外",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "空山岁计是胡麻，穷海无梁泛一槎。稚子唯能觅梨栗，",
            "借问省中何水部，今人几个属诗家。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 14
    },
    {
        "title": "献薛仆射",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "由来那敢议轻肥，散发行歌自采薇。逋客未能忘野兴，",
            "更乞大贤容小隐，益看愚谷有光辉。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 15
    },
    {
        "title": "鲍防员外见寻因书情呈赠",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "少小为儒不自强，如今懒复见侯王。览镜已知身渐老，",
            "犹有郎官来问疾，时人莫道我佯狂。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 16
    },
    {
        "title": "寄浙东皇甫中丞",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "闲闲麋鹿或相随，一两年来鬓欲衰。琴砚共依春酒瓮，",
            "久带纱巾仍藉草，山中那得见朝仪。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 17
    },
    {
        "title": "题章野人山居",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "带郭茅亭诗兴饶，回看一曲倚危桥。门前山色能深浅，",
            "向此隐来经几载，如今已是汉家朝。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 18
    },
    {
        "title": "山中枉皇甫温大夫见招书",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "十年木屐步苔痕，石上松间水自喧。三辟草堂仍被褐，",
            "亚相已能怜潦倒，山花笑处莫啼猿。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 19
    },
    {
        "title": "题茅山李尊师山居",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "天师百岁少如童，不到山中竟不逢。洗药每临新瀑水，",
            "此去人寰今远近，回看去壑一重重。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 20
    },
    {
        "title": "耶溪书怀寄刘长卿员外",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "时人多笑乐幽栖，晚起闲行独杖藜。云色卷舒前后岭，",
            "拟共钓竿长往复，严陵滩上胜耶溪。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 21
    },
    {
        "title": "山中崔大夫有书相问",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "客在烟霞里，闲闲逐狎鸥。终年常裸足，连日半蓬头。",
            "带月乘渔艇，迎寒绽鹿裘。已于人事少，多被挂冠留。",
            "素业堆千卷，清风至一丘。苍黄倒藜杖，伛偻睹银钩。",
            "迹愧巢由隐，才非管乐俦。从来自多病，不是傲王侯。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 22
    },
    {
        "title": "张建封大夫奏系为校书郎因寄此作",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "久是烟霞客，潭深钓得鱼。不知芸阁上，遗校几多书。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 23
    },
    {
        "title": "会稽山居寄薛播侍郎袁高给事高参舍人",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "稷契今为相，明君复为尧。宁知买臣困，犹负会稽樵。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 24
    },
    {
        "title": "闲居览史",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "长策胸中不复论，荷衣蓝缕闭柴门。",
            "当时汉祖无三杰，争得咸阳与子孙。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 25
    },
    {
        "title": "山中赠耿拾遗𣲗兼两省故人",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "数片荷衣不蔽身，青山白鸟岂知贫。",
            "如今非是秦时世，更隐桃花亦笑人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 26
    },
    {
        "title": "秋日送僧志幽归山寺",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "禅室绳床在翠微，松间荷笠一僧归。",
            "磬声寂历宜秋夜，手冷灯前自衲衣。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 27
    },
    {
        "title": "题僧明惠房",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "簷前朝暮雨添花，八十真僧饭一麻。",
            "入定几时将出定，不知巢燕污袈裟。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 28
    },
    {
        "title": "答泉州薛播使君重阳日赠酒",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "欲强登高无力也，篱边黄菊为谁开。",
            "共知不是浔阳郡，那得王弘送酒来。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 29
    },
    {
        "title": "题洪道士山院",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "霞外主人门不扃，数株桃树药囊青。",
            "闲行池畔随孤鹤，若问多应道姓丁。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 30
    },
    {
        "title": "期王炼师不至",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "黄精蒸罢洗琼杯，林下从留石上苔。",
            "昨日围棋未终局，多乘白鹤下山来。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 31
    },
    {
        "title": "题赠张道士山居",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "磐石垂萝即是家，回头犹看五枝花。",
            "松间寂寂无烟火，应服朝来一片霞。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 32
    },
    {
        "title": "山中书怀寄张建封大夫",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "昨日年催白发新，身如麋鹿不知贫。",
            "时时亦被群儿笑，赖有南山四老人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 33
    },
    {
        "title": "山中赠诸暨丹丘明府",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "荷衣半破带莓苔，笑向陶潜酒瓮开。",
            "纵醉还须上山去，白云那肯下山来。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 34
    },
    {
        "title": "奉寄昼公",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "蓑笠双童傍酒船，湖山相引到房前。",
            "团蕉何事教人见，暂借空床守坐禅。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 35
    },
    {
        "title": "宿云门上方",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "禅室遥看峰顶头，白云东去水长流。",
            "松间倘许幽人住，不更将钱买沃州。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 36
    },
    {
        "title": "即事奉呈郎中韦使君",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "久卧云间已息机，青袍忽著狎鸥飞。",
            "诗兴到来无一事，郡中今有谢玄晖。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 37
    },
    {
        "title": "晓鸡",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "黯黯严城罢鼓鼙，数声相续出寒栖。",
            "不嫌惊破纱窗梦，却恐为妖半夜啼。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 38
    }
]