[
    {
        "title": "答王无功入长安咏秋蓬见示",
        "author": "辛学士",
        "biography": "",
        "paragraphs": [
            "托根虽异所，飘叶早相依。因风若有便，更共入云飞。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百八十三",
        "no#": 1
    },
    {
        "title": "哭李远",
        "author": "卢尚书",
        "biography": "",
        "paragraphs": [
            "昨日舟还浙水湄，今朝丹旐欲何为。才收北浦一竿钓，",
            "不堪旧里经行处，风木萧萧邻笛悲。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百八十三",
        "no#": 2
    },
    {
        "title": "湘中怨讽",
        "author": "郑仆射",
        "biography": "",
        "paragraphs": [
            "青鹢苦幽独，隔江相对稀。夜寒芦叶雨，空作一声归。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百八十三",
        "no#": 3
    },
    {
        "title": "登汾上阁",
        "author": "郑中丞",
        "biography": "",
        "paragraphs": [
            "汾桂秋水阔，宛似到阊门。惆怅江湖思，惟将南客论。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百八十三",
        "no#": 4
    },
    {
        "title": "为御史衔命出关谳狱，道中看华山有诗",
        "author": "韩常侍",
        "biography": "",
        "paragraphs": [
            "野麋蒙象暂如犀，心不惊鸥角骇鸡。",
            "一路好山无伴看，断肠烟景寄猿啼。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百八十三",
        "no#": 5
    },
    {
        "title": "寄织锦篇与薛郎中",
        "author": "韩常侍",
        "biography": "",
        "paragraphs": [
            "锦字龙梭织锦篇，凤凰文采间非烟。",
            "并他时世新花样，虚费工夫不直钱。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百八十三",
        "no#": 6
    },
    {
        "title": "和人忆鹤",
        "author": "韩常侍",
        "biography": "",
        "paragraphs": [
            "拂拂云衣冠紫烟，已为丁令一千年。",
            "留君且伴居山客，幸有松梢明月天。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百八十三",
        "no#": 7
    },
    {
        "title": "句",
        "author": "韩常侍",
        "biography": "",
        "paragraphs": [
            "情多不似家山水，夜夜声声旁枕流。（《忆山泉》，"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百八十三",
        "no#": 8
    },
    {
        "title": "赠米都知",
        "author": "梁补阙",
        "biography": "",
        "paragraphs": [
            "供奉三朝四十年，圣时流落发衰残。",
            "贪将乐府歌明代，不把清吟换好官。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百八十三",
        "no#": 9
    },
    {
        "title": "题安国观",
        "author": "卢尚书",
        "biography": "",
        "paragraphs": [
            "夕照纱窗起暗尘，青松绕殿不知春。",
            "君看白首诵经者，半是宫中歌舞人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百八十三",
        "no#": 10
    },
    {
        "title": "自商山宿隐居",
        "author": "苏广文",
        "biography": "",
        "paragraphs": [
            "闻道桃源堪避秦，寻幽数日不逢人。烟霞洞里无鸡犬，",
            "醉卧白云闲入梦，不知何物是吾身。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百八十三",
        "no#": 11
    },
    {
        "title": "夜归华川因寄幕府",
        "author": "苏广文",
        "biography": "",
        "paragraphs": [
            "山村寥落野人稀，竹里衡门掩翠微。溪路夜随明月入，",
            "汀畔数鸥闲不起，只应知我已忘机。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百八十三",
        "no#": 12
    },
    {
        "title": "春日过田明府遇焦山人",
        "author": "苏广文",
        "biography": "",
        "paragraphs": [
            "陶公归隐白云溪，买得春泉溉药畦。夜静林间风虎啸，",
            "相见只言秦汉事，武陵溪里草萋萋。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百八十三",
        "no#": 13
    },
    {
        "title": "题升山",
        "author": "任生",
        "biography": "",
        "paragraphs": [
            "城外升山寺，城中望宛然。及登无半日，欲到已经年。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百八十三",
        "no#": 14
    },
    {
        "title": "投曹文姬诗",
        "author": "任生",
        "biography": "",
        "paragraphs": [
            "玉皇前殿掌书仙，一染尘心下九天。",
            "莫怪浓香熏骨腻，云衣曾惹御炉烟。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百八十三",
        "no#": 15
    },
    {
        "title": "绝句",
        "author": "吴公",
        "biography": "",
        "paragraphs": [
            "去国投兹土，编茅隐旧踪。年年秋水上，独对数株松。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百八十三",
        "no#": 16
    },
    {
        "title": "献卢常侍",
        "author": "曹生",
        "biography": "",
        "paragraphs": [
            "拜玉亭前闲送客，此时孤恨感离乡。",
            "寻思往岁绝缨事，肯向朱门泣夜长。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百八十三",
        "no#": 17
    },
    {
        "title": "赋新月",
        "author": "缪氏子",
        "biography": "",
        "paragraphs": [
            "初月如弓未上弦，分明挂在碧霄边。",
            "时人莫道蛾眉小，三五团圆照满天。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百八十三",
        "no#": 18
    },
    {
        "title": "悼妓诗",
        "author": "京兆韦氏子",
        "biography": "",
        "paragraphs": [
            "惆怅金泥簇蝶裙，春来犹见伴行云。",
            "不教布施刚留得，浑似初逢李少君。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百八十三",
        "no#": 19
    }
]