[
    {
        "title": "寓言",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "暖风迟日柳初含，顾影看身又自惭。",
            "何事明朝独惆怅，杏花时节在江南。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 1
    },
    {
        "title": "猿",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "月白烟青水暗流，孤猿衔恨叫中秋。",
            "三声欲断疑肠断，饶是少年今白头。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 2
    },
    {
        "title": "怀归",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "尘埃终日满窗前，水态云容思浩然。",
            "争得便归湘浦去，却持竿上钓鱼船。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 3
    },
    {
        "title": "边上晚秋",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "黑山南面更无州，马放平沙夜不收。",
            "风送孤城临晚角，一声声入客心愁。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 4
    },
    {
        "title": "伤友人悼吹箫妓",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "玉箫声断没流年，满目春愁陇树烟。",
            "艳质已随云雨散，凤楼空锁月明天。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 5
    },
    {
        "title": "访许颜",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "门近寒溪窗近山，枕山流水日潺潺。",
            "长嫌世上浮云客，老向尘中不解颜。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 6
    },
    {
        "title": "春日古道傍作",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "万古荣华旦暮齐，楼台春尽草萋萋。",
            "君看陌上何人墓，旋化红尘送马蹄。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 7
    },
    {
        "title": "青冢",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "青冢前头陇水流，燕支山上暮云秋。",
            "蛾眉一坠穷泉路，夜夜孤魂月下愁。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 8
    },
    {
        "title": "大梦上人自庐峰回",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "行脚寻常到寺稀，一枝藜杖一禅衣。",
            "开门满院空秋色，新向庐峰过夏归。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 9
    },
    {
        "title": "洛中二首",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "柳动晴风拂路尘，年年宫阙锁浓春。",
            "一从翠辇无巡幸，老却蛾眉几许人。",
            "风吹柳带摇晴绿，蝶绕花枝恋暖香。",
            "多把芳菲泛春酒，直教愁色对愁肠。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 10
    },
    {
        "title": "边上闻笳三首",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "何处吹笳薄暮天，塞垣高鸟没狼烟。",
            "游人一听头堪白，苏武争禁十九年。",
            "海路无尘边草新，荣枯不见绿杨春。",
            "白沙日暮愁云起，独感离乡万里人。",
            "胡雏吹笛上高台，寒雁惊飞去不回。",
            "尽日春风吹不散，只应分付客愁来。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 11
    },
    {
        "title": "春日寄许浑先辈",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "蓟北雁初去，湘南春又归。水流沧海急，人到白头稀。",
            "塞路尽何处，我愁当落晖。终须接鸳鹭，霄汉共高飞。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 12
    },
    {
        "title": "经阖闾城",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "遗踪委衰草，行客思悠悠。昔日人何处，终年水自流。",
            "孤烟村戍远，乱雨海门秋。吟罢独归去，烟云尽惨愁。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 13
    },
    {
        "title": "并州道中",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "行役我方倦，苦吟谁复闻。戍楼春带雪，边角暮吹云。",
            "极目无人迹，回头送雁群。如何遣公子，高卧醉醺醺。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 14
    },
    {
        "title": "别怀",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "相别徒成泣，经过总是空。劳生惯离别，夜梦苦西东。",
            "去路三湘浪，归程一片风。他年寄消息，书在鲤鱼中。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 15
    },
    {
        "title": "渔父",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "白发沧浪上，全忘是与非。秋潭垂钓去，夜月叩船归。",
            "烟影侵芦岸，潮痕在竹扉。终年狎鸥鸟，来去且无机。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 16
    },
    {
        "title": "秋梦",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "寒空动高吹，月色满清砧。残梦夜魂断，美人边思深。",
            "孤鸿秋出塞，一叶暗辞林。又寄征衣去，迢迢天外心。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 17
    },
    {
        "title": "早秋客舍",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "风吹一片叶，万物已惊秋。独夜他乡泪，年年为客愁。",
            "别离何处尽，摇落几时休。不及磻溪叟，身闲长自由。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 18
    },
    {
        "title": "逢故人",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "故交相见稀，相见倍依依。尘路事不尽，云岩闲好归。",
            "投人销壮志，徇俗变真机。又落他乡泪，风前一满衣。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 19
    },
    {
        "title": "秋晚江上遣怀",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "孤舟天际外，去路望中赊。贫病远行客，梦魂多在家。",
            "蝉吟秋色树，鸦噪夕阳沙。不拟彻双鬓，他方掷岁华。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 20
    },
    {
        "title": "长安夜月",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "寒光垂静夜，皓彩满重城。万国尽分照，谁家无此明。",
            "古槐疏影薄，仙桂动秋声。独有长门里，蛾眉对晓晴。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 21
    },
    {
        "title": "云",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "东西那有碍，出处岂虚心。晓入洞庭阔，暮归巫峡深。",
            "渡江随鸟影，拥树隔猿吟。莫隐高唐去，枯苗待作霖。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 22
    },
    {
        "title": "春怀",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "年光何太急，倏忽又青春。明月谁为主，江山暗换人。",
            "莺花潜运老，荣乐渐成尘。遥忆朱门柳，别离应更频。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 23
    },
    {
        "title": "逢故人",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "年年不相见，相见却成悲。教我泪如霰，嗟君发似丝。",
            "正伤携手处，况值落花时。莫惜今宵醉，人间忽忽期。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 24
    },
    {
        "title": "闲题",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "男儿所在即为家，百镒黄金一朵花。",
            "借问春风何处好，绿杨深巷马头斜。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 25
    },
    {
        "title": "金谷园",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "繁华事散逐香尘，流水无情草自春。",
            "日暮东风怨啼鸟，落花犹似堕楼人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 26
    },
    {
        "title": "重登科",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "星汉离宫月出轮，满街含笑绮罗春。",
            "花前每被青蛾问，何事重来只一人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 27
    },
    {
        "title": "游边",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "黄沙连海路无尘，边草长枯不见春。",
            "日暮拂云堆下过，马前逢著射雕人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 28
    },
    {
        "title": "将赴池州道中作",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "青阳云水去年寻，黄绢歌诗出翰林。投辖暂停留酒客，",
            "南去南来尽乡国，月明秋水只沉沉。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 29
    },
    {
        "title": "隋宫春",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "龙舟东下事成空，蔓草萋萋满故宫。",
            "亡国亡家为颜色，露桃犹自恨春风。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 30
    },
    {
        "title": "蛮中醉",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "瘴塞蛮江入洞流，人家多在竹棚头。",
            "青山海上无城郭，唯见松牌出象州。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 31
    },
    {
        "title": "寓题",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "把酒直须判酩酊，逢花莫惜暂淹留。",
            "假如三万六千日，半是悲哀半是愁。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 32
    },
    {
        "title": "送赵十二赴举",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "省事却因多事力，无心翻似有心来。",
            "秋风郡阁残花在，别后何人更一杯。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 33
    },
    {
        "title": "偶呈郑先辈",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "不语亭亭俨薄妆，画裙双凤郁金香。",
            "西京才子旁看取，何似乔家那窈娘。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 34
    },
    {
        "title": "《子规",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "蜀地曾闻子规鸟，宣城又见杜鹃花。",
            "一叫一回肠一断，三春三月忆三巴。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 35
    },
    {
        "title": "江楼",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "独酌芳春酒，登楼已半醺。谁惊一行雁，冲断过江云。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 36
    },
    {
        "title": "旅宿",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "旅馆无良伴，凝情自悄然。寒灯思旧事，断雁警愁眠。",
            "远梦归侵晓，家书到来年。湘江好烟月，门系钓鱼船。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 37
    },
    {
        "title": "杜鹃",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "杜宇竟何冤，年年叫蜀门。至今衔积恨，终古吊残魂。",
            "芳草迷肠结，红花染血痕。山川尽春色，呜咽复谁论。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 38
    },
    {
        "title": "闻蝉",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "火云初似灭，晓角欲微清。故国行千里，新蝉忽数声。",
            "时行仍仿佛，度日更分明。不敢频倾耳，唯忧白发生。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 39
    },
    {
        "title": "送友人",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "十载名兼利，人皆与命争。青春留不住，白发自然生。",
            "夜雨滴乡思，秋风从别情。都门五十里，驰马逐鸡声。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 40
    },
    {
        "title": "旅情",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "窗虚枕簟凉，寝倦忆潇湘。山色几时老，人心终日忙。",
            "松风半夜雨，帘月满堂霜。匹马好归去，江头橘正香。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 41
    },
    {
        "title": "晓望",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "独起望山色，水鸡鸣蓼洲。房星随月晓，楚木向云秋。",
            "曲渚疑江尽，平沙似浪浮。秦原在何处，泽国碧悠悠。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 42
    },
    {
        "title": "贻友人",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "自是东西客，逢人又送人。不应相见老，只是别离频。",
            "度日还知暮，平生未识春。傥无迁谷分，归去养天真。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 43
    },
    {
        "title": "书事",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "自笑走红尘，流年旧复新。东风半夜雨，南国万家春。",
            "失计抛鱼艇，何门化涸鳞。是谁添岁月，老却暗投人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 44
    },
    {
        "title": "别鹤",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "分飞共所从，六翮势催风。声断碧云外，影孤明月中。",
            "青田归路远，丹桂旧巢空。矫翼知何处，天涯不可穷。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 45
    },
    {
        "title": "晚泊",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "帆湿去悠悠，停桡宿渡头。乱烟迷野岸，独鸟出中流。",
            "篷雨延乡梦，江风阻暮秋。傥无身外事，甘老向扁舟。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 46
    },
    {
        "title": "山寺",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "峭壁引行径，截溪开石门。泉飞溅虚槛，云起涨河轩。",
            "隔水看来路，疏篱见定猿。未闲难久住，归去复何言。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 47
    },
    {
        "title": "早行",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "垂鞭信马行，数里未鸡鸣。林下带残梦，叶飞时忽惊。",
            "霜凝孤鹤迥，月晓远山横。僮仆休辞险，时平路复平。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 48
    },
    {
        "title": "秋日偶题",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "荷花兼柳叶，彼此不胜秋。玉露滴初泣，金风吹更愁。",
            "绿眉甘弃坠，红脸恨飘流。叹息是游子，少年还白头。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 49
    },
    {
        "title": "忆归",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "新城非故里，终日想柴扃。兴罢花还落，愁来酒欲醒。",
            "何人初发白，几处乱山青。远忆湘江上，渔歌对月听。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 50
    },
    {
        "title": "偶见",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "朔风高紧掠河楼，白鼻䯄郎白罽裘。",
            "有个当垆明似月，马鞭斜揖笑回头。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 51
    },
    {
        "title": "醉倒",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "日晴空乐下仙云，俱在凉亭送使君。",
            "莫辞一盏即相请，还是三年更不闻。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 52
    },
    {
        "title": "酬许十三秀才兼依来韵",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "多为裁诗步竹轩，有时凝思过朝昏。篇成敢道怀金璞，",
            "凭君把卷侵寒烛，丽句时传画戟门。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 53
    },
    {
        "title": "后池泛舟送王十秀才",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "城日晚悠悠，弦歌在碧流。夕风飘度曲，烟屿隐行舟。",
            "问拍拟新令，怜香占彩球。当筵虽一醉，宁复缓离愁。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 54
    },
    {
        "title": "书情",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "谁家洛浦神，十四五来人。媚发轻垂额，香衫软著身。",
            "摘莲红袖湿，窥渌翠蛾频。飞鹊徒来往，平阳公主亲。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 55
    },
    {
        "title": "兵部尚书席上作",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "华堂今日绮筵开，谁唤分司御史来。",
            "偶发狂言惊满坐，三重粉面一时回。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 56
    },
    {
        "title": "骕骦阪",
        "author": "杜牧",
        "biography": "",
        "paragraphs": [
            "荆州一万里，不如蒯易度。仰首望飞鸣，伊人何异趣。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百二十五",
        "no#": 57
    }
]