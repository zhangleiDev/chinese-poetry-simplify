[
    {
        "title": "题玉真观公主山池院",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "香殿留遗影，春朝玉户开。羽衣重素几，珠网俨轻埃。",
            "石自蓬山得，泉经太液来。柳丝遮绿浪，花粉落青苔。",
            "镜掩鸾空在，霞消凤不回。唯余古桃树，传是上仙栽。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 1
    },
    {
        "title": "送永阳崔明府",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "古国群舒地，前当桐柏关。连绵江上雨，稠叠楚南山。",
            "沙馆行帆息，枫洲候吏还。乘篮若有暇，精舍在林间。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 2
    },
    {
        "title": "送曹三同猗游山寺",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "山蹋青芜尽，凉秋古寺深。何时得连策，此夜更闻琴。",
            "穷水云同穴，过僧虎共林。殷勤如念我，遗尔挂冠心。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 3
    },
    {
        "title": "送崔校书赴梓幕",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "碧峰天柱下，鼓角镇南军。管记催飞檄，蓬莱辍校文。",
            "栈霜朝似雪，江雾晚成云。想出褒中望，巴庸方路分。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 4
    },
    {
        "title": "送夔州班使君",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "鱼国巴庸路，麾幢汉守过。晓樯争市隘，夜鼓祭神多。",
            "云白当山雨，风清满峡波。夷陵旧人吏，犹诵两岐歌。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 5
    },
    {
        "title": "送菊潭王明府",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "业成洙泗客，皓发著儒衣。一与游人别，仍闻带印归。",
            "林多宛地古，云尽汉山稀。莫爱浔阳隐，嫌官计亦非。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 6
    },
    {
        "title": "送太易上人赴东洛",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "遥见登山处，青芜雪后春。云深岳庙火，寺宿洛阳人。",
            "饵药将斋折，唯诗与道亲。凡经几回别，麈尾不离身。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 7
    },
    {
        "title": "和王卿立秋即事",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "秋宜何处看，试问白云官。暗入蝉鸣树，微侵蝶绕兰。",
            "向风凉稍动，近日暑犹残。九陌浮埃减，千峰爽气攒。",
            "换衣防竹暮，沈果讶泉寒。宫响传花杵，天清出露盘。",
            "高禽当侧弁，游鲔对凭栏。一奏招商曲，空令继唱难。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 8
    },
    {
        "title": "和李员外与舍人咏玫瑰花寄徐侍郎",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "仙吏紫薇郎，奇花共玩芳。攒星排绿蒂，照眼发红光。",
            "暗妒翻阶药，遥连直署香。游枝蜂绕易，碍刺鸟衔妨。",
            "露湿凝衣粉，风吹散蕊黄。蒙茏珠树合，焕烂锦屏张。",
            "留客胜看竹，思人比爱棠。如传采苹咏，远思满潇湘。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 9
    },
    {
        "title": "冬夜耿拾遗王秀才就宿因伤故人",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "旧时闻笛泪，今夜重沾衣。方恨同人少，何堪相见稀。",
            "竹烟凝涧壑，林雪似芳菲。多谢劳车马，应怜独掩扉。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 10
    },
    {
        "title": "早春游慈恩南池",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "山寺临池水，春愁望远生。蹋桥逢鹤起，寻竹值泉横。",
            "新柳丝犹短，轻苹叶未成。还如虎溪上，日暮伴僧行。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 11
    },
    {
        "title": "雨夜见投之作",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "出户繁星尽，池塘暗不开。动衣凉气度，遰树远声来。",
            "灯外初行电，城隅偶隐雷。因知谢文学，晓望比尘埃。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 12
    },
    {
        "title": "龙池寺望月寄韦使君阎别驾",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "清光此夜中，万古望应同。当野山沈雾，低城树有风。",
            "花宫纷共邃，水府皓相空。遥想高楼上，唯君对庾公。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 13
    },
    {
        "title": "秋夜忆兴善院寄苗发",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "右军多住寺，此夜后池秋。自与山僧伴，那因洛客愁。",
            "卷帘霜霭霭，满目水悠悠。若有诗相赠，期君忆惠休。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 14
    },
    {
        "title": "病中寄郑十六兄",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "倦枕欲徐行，开帘秋月明。手便筇杖冷，头喜葛巾轻。",
            "绿草前侵水，黄花半上城。虚消此尘景，不见十年兄。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 15
    },
    {
        "title": "卫明府寄枇杷叶以诗答",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "倾筐呈绿叶，重叠色何鲜。讵是秋风里，犹如晓露前。",
            "仙方当见重，消疾本应便。全胜甘蕉赠，空投谢氏篇。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 16
    },
    {
        "title": "过庆宝寺",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "黄叶前朝寺，无僧寒殿开。池晴龟出曝，松暮鹤飞回。",
            "古井碑横草，阴廊画杂苔。禅宫亦销歇，尘世转堪哀。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 17
    },
    {
        "title": "奉和张大夫酬高山人",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "野客居铃阁，重门将校稀。豸冠亲谷弁，龟印识荷衣。",
            "座右寒飙爽，谈余暮角微。苍生须太傅，山在岂容归。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 18
    },
    {
        "title": "送严使君游山",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "家楚依三户，辞州选一钱。酒杯同寄世，客棹任销年。",
            "赤烧兼山远，青芜与浪连。青春明月夜，知上鄂君船。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 19
    },
    {
        "title": "送柳震归蜀",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "白日双流静，西看蜀国春。桐花能乳鸟，竹节竞祠神。",
            "蹇步徒相望，先鞭不可亲。知从江仆射，登榻更何人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 20
    },
    {
        "title": "送乐平苗明府",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "天际山多处，东安古邑深。绿田通竹里，白浪隔枫林。",
            "诗有江僧和，门唯越客寻。应将放鱼化，一境表吾心。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 21
    },
    {
        "title": "赠送郑钱二郎中",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "梅含柳已动，昨日起东风。惆怅心徒壮，无如鬓作翁。",
            "百年飘若水，万绪尽归空。何可宗禅客，迟回岐路中。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 22
    },
    {
        "title": "酬郑十四望驿不得同宿见赠因寄张参军",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "逢君喜成泪，暂似故乡中。谪宦犹多惧，清宵不得终。",
            "月烟高有鹤，宿草净无虫。明日郤超会，应思下客同。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 23
    },
    {
        "title": "暮春野望寄钱起",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "草长花落树，羸病强寻春。无复少年意，空余华发新。",
            "青原高见水，白社静逢人。寄谢南宫客，轩车不可亲。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 24
    },
    {
        "title": "送王使君小子孝廉登科归省",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "年少通经学，登科尚佩觿。张冯本名士，蔡廓是佳儿。",
            "鞍马临岐路，龙钟对别离。寄书胡太守，请与故人知。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 25
    },
    {
        "title": "云阳寺石竹花",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "一自幽山别，相逢此寺中。高低俱出叶，深浅不分丛。",
            "野蝶难争白，庭榴暗让红。谁怜芳最久，春露到秋风。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 26
    },
    {
        "title": "送高胜重谒曹王",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "江上青枫岸，阴阴万里春。朝辞郢城酒，暮见洞庭人。",
            "兴比乘舟访，恩怀倒屣亲。想君登旧榭，重喜扫芳尘。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 27
    },
    {
        "title": "闲园书事招畅当",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "闻蝉昼眠后，欹枕对蓬蒿。羸病懒寻戴，田园方咏陶。",
            "傍簷虫挂静，出树蝶飞高。惆怅临清镜，思君见鬓毛。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 28
    },
    {
        "title": "过钱员外",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "为郎头已白，迹向市朝稀。移病居荒宅，安贫著败衣。",
            "野园随客醉，雪寺伴僧归。自说东峰下，松萝满故扉。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 29
    },
    {
        "title": "赠庾侍御",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "年少身无累，相逢忆此时。雪过云寺宿，酒向竹园期。",
            "白发今催老，清琴但起悲。唯应逐宗炳，内学愿为师。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 30
    },
    {
        "title": "赠李端",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "共忆南浮日，登高望若何。楚田湖草远，江寺海榴多。",
            "载酒寻山宿，思人带雪过。东西几回别，此会各蹉跎。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 31
    },
    {
        "title": "送流人",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "闻说南中事，悲君重窜身。山村枫子鬼，江庙石郎神。",
            "童稚留荒宅，图书托故人。青门好风景，为尔一沾巾。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 32
    },
    {
        "title": "过胡居士睹王右丞遗文",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "旧日相知尽，深居独一身。闭门空有雪，看竹永无人。",
            "每许前山隐，曾怜陋巷贫。题诗今尚在，暂为拂流尘。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 33
    },
    {
        "title": "送郎使君赴郢州",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "使君持节去，云水满前程。楚寺多连竹，江樯远映城。",
            "登楼向月望，赛庙傍山行。若动思乡咏，应贻谢步兵。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 34
    },
    {
        "title": "贼平后送人北归",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "世乱同南去，时清独北还。他乡生白发，旧国见青山。",
            "晓月过残垒，繁星宿故关。寒禽与衰草，处处伴愁颜。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 35
    },
    {
        "title": "观猎骑",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "缠臂绣纶巾，貂裘窄称身。射禽风助箭，走马雪翻尘。",
            "金埒争开道，香车为驻轮。翩翩不知处，传是霍家亲。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 36
    },
    {
        "title": "同苗员外宿荐福常师房",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "浮生共多故，聚宿喜君同。人息时闻磬，灯摇乍有风。",
            "霜阶疑水际，夜木似山中。一愿持如意，长来事远公。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 37
    },
    {
        "title": "送乔广下第归淮南",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "遥想长淮尽，荒堤楚路斜。戍旌标白浪，罟网入青葭。",
            "啼鸟仍临水，愁人更见花。东堂一枝在，为子惜年华。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 38
    },
    {
        "title": "风筝",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "高风吹玉柱，万籁忽齐飘。飒树迟难度，萦空细渐销。",
            "松泉鹿门夜，笙鹤洛滨朝。坐与真僧听，支颐向寂寥。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 39
    },
    {
        "title": "闲居寄苗发",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "渐向浮生老，前期竟若何。独身居处静，永夜坐时多。",
            "厌逐青林客，休吟白雪歌。支公有遗寺，重与谢安过。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 40
    },
    {
        "title": "送王先生归南山",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "儒中年最老，独有济南生。爱子方传业，无官自耦耕。",
            "竹通山舍远，云接雪田平。愿作门人去，相随隐姓名。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 41
    },
    {
        "title": "寄天台秀师",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "天台瀑布寺，传有白头师。幻迹示羸病，空门无住持。",
            "雪晴看鹤去，海夜与龙期。永愿亲瓶屦，呈功得问疑。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 42
    },
    {
        "title": "送夏侯审赴宁国",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "青圻连白浪，晓日渡南津。山叠陵阳树，舟多建业人。",
            "烟霞高占寺，枫竹暗停神。如接玄晖集，江丞独见亲。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 43
    },
    {
        "title": "云阳馆与韩绅宿别",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "故人江海别，几度隔山川。乍见翻疑梦，相悲各问年。",
            "孤灯寒照雨，湿竹暗浮烟。更有明朝恨，离杯惜共传。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 44
    },
    {
        "title": "送卢使君赴夔州",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "铙管随旌旆，高秋远上巴。白波连雾雨，青壁断蒹葭。",
            "凭几双童静，登楼万井斜。政成知变俗，当应画轮车。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 45
    },
    {
        "title": "夜闻回雁",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "雁响天边过，高高望不分。飕飗传细雨，嘹唳隔长云。",
            "散向谁家尽，归来几客闻。还将今夜意，西海话苏君。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 46
    },
    {
        "title": "赋得的的帆向浦",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "向浦参差去，随波远近还。初移芳草里，正在夕阳间。",
            "隐映回孤驿，微明出乱山。向空看不尽，归思满江关。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 47
    },
    {
        "title": "秋思呈尹植裴说",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "静向懒相偶，年将衰共催。前途欢不集，往事恨空来。",
            "昼景委红叶，月华销绿苔。沈思竟何有，坐结玉琴哀。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 48
    },
    {
        "title": "闲园即事寄暕公",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "欲就东林寄一身，尚怜儿女未成人。柴门客去残阳在，",
            "深山兰若何时到，羡与闲云作四邻。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 49
    },
    {
        "title": "题暕上人院",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "闭门不出自焚香，拥褐看山岁月长。雨后绿苔生石井，",
            "更说本师同学在，几时携手见衡阳。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 50
    },
    {
        "title": "长安晓望寄程补阙",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "迢递山河拥帝京，参差宫殿接云平。风吹晓漏经长乐，",
            "独有浅才甘未达，多惭名在鲁诸生。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 51
    },
    {
        "title": "下第日书情寄上叔父",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "微才空觉滞京师，末学曾为叔父知。雪里题诗偏见赏，",
            "欲归江海寻山去，愿报何人得桂枝。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 52
    },
    {
        "title": "南原望汉宫",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "荒原空有汉宫名，衰草茫茫雉堞平。连雁下时秋水在，",
            "故事悠悠不可问，寒禽野水自纵横。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 53
    },
    {
        "title": "早夏寄元校书",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "独游野径送芳菲，高竹林居接翠微。绿岸草深虫入遍，",
            "蓬荜永无车马到，更当斋夜忆玄晖。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 54
    },
    {
        "title": "赠衡岳隐禅师",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "拥褐安居南岳头，白云高寺见衡州。石窗湖水摇寒月，",
            "垂垂身老将传法，因下人间遂北游。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 55
    },
    {
        "title": "题凌云寺",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "春山古寺绕沧波，石磴盘空鸟道过。百丈金身开翠壁，",
            "不与方袍同结社，下归尘世竟如何。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 56
    },
    {
        "title": "晦日益州北池陪宴",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "临泛从公日，仙舟翠幕张。七桥通碧沼，双树接花塘。",
            "玉烛收寒气，金波隐夕光。野闻歌管思，水静绮罗香。",
            "游骑萦林远，飞桡截岸长。郊原怀灞浐，陂溠写江潢。",
            "常侍传花诏，偏裨问羽觞。岂令南岘首，千载播余芳。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 57
    },
    {
        "title": "送曲山人之衡州",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "白石先生眉发光，已分甜雪饮红浆。衣巾半染烟霞气，",
            "千年城郭如相问，华表峨峨有夜霜。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 58
    },
    {
        "title": "立秋日",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "律变新秋至，萧条自此初。花酣莲报谢，叶在柳呈疏。",
            "澹日非云映，清风似雨余。卷帘凉暗度，迎扇暑先除。",
            "草静多翻燕，波澄乍露鱼。今朝散骑省，作赋兴何如。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 59
    },
    {
        "title": "咏古寺花",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "共爱芳菲此树中，千跗万萼裹枝红。",
            "迟迟欲去犹回望，覆地无人满寺风。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 60
    },
    {
        "title": "酬张芬有赦后见赠",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "紫凤朝衔五色书，阳春忽布网罗除。已将心变寒灰后，",
            "劳君故有诗相赠，欲报琼瑶恨不如。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 61
    },
    {
        "title": "哭苗员外呈张参军",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "思君宁家宅，久接竹林期。尝值偷琴处，亲闻比玉时。",
            "高人不易合，弱冠早相知。试艺临诸友，能文即我师。",
            "凌寒松未老，先暮槿何衰。季子生前别，羊昙醉后悲。",
            "寿堂乖一恸，奠席阻长辞。因沥殊方泪，遥成墓下诗。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 62
    },
    {
        "title": "金陵怀古",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "辇路江枫暗，宫庭野草春。伤心庾开府，老作北朝臣。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 63
    },
    {
        "title": "发渝州却寄韦判官",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "红烛津亭夜见君，繁弦急管两纷纷。",
            "平明分手空江转，唯有猿声满水云。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 64
    },
    {
        "title": "送卢彻之太原谒马尚书",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "榆落雕飞关塞秋，黄云画角见并州。",
            "翩翩羽骑双旌后，上客亲随郭细侯。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 65
    },
    {
        "title": "峡口送友人",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "峡口花飞欲尽春，天涯去住泪沾巾。",
            "来时万里同为客，今日翻成送故人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 66
    },
    {
        "title": "故郭婉仪挽歌",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "一日辞秦镜，千秋别汉宫。岂唯泉路掩，长使月轮空。",
            "苦色凝朝露，悲声切暝风。婉仪余旧德，仍载礼经中。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 67
    },
    {
        "title": "送翰林张学士岭南勒圣碑",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "汉恩天外洽，周颂日边称。文独司空羡，书兼太尉能。",
            "出关逢北雁，度岭逐南鹏。使者翰林客，余春归灞陵。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 68
    },
    {
        "title": "送吉校书东归",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "少年芸阁吏，罢直暂归休。独与亲知别，行逢江海秋。",
            "听猿看楚岫，随雁到吴洲。处处园林好，何人待子猷。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 69
    },
    {
        "title": "早春游望",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "东风春未足，试望秦城曲。青草状寒芜，黄花似秋菊。",
            "壮将欢共去，老与悲相逐。独作游社人，暮过威辇宿。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 70
    },
    {
        "title": "秋日趋府上张大夫",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "重城洞启肃秋烟，共说羊公在镇年。鞞鼓暗惊林叶落，",
            "谪吏何能沐风化，空将歌颂拜车前。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 71
    },
    {
        "title": "竹里径",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "幽径行迹稀，清阴苔色古。萧萧风欲来，乍似蓬山雨。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 72
    },
    {
        "title": "黄子陂",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "岸芳春色晓，水影夕阳微。寂寂深烟里，渔舟夜不归。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 73
    },
    {
        "title": "田鹤",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "散下渚田中，隐见菰蒲里。哀鸣自相应，欲作凌风起。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 74
    },
    {
        "title": "药园",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "春园芳已遍，绿蔓杂红英。独有深山客，时来辨药名。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 75
    },
    {
        "title": "石井",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "苔色遍春石，桐阴入寒井。幽人独汲时，先乐残阳影。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 76
    },
    {
        "title": "板桥",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "横遮野水石，前带荒村道。来往见愁人，清风柳阴好。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 77
    },
    {
        "title": "石莲花",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "今逢石上生，本自波中有。红艳秋风里，谁怜众芳后。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 78
    },
    {
        "title": "远寺钟",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "杳杳疏钟发，因风清复引。中宵独听之，似与东林近。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 79
    },
    {
        "title": "松下雪",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "不随晴野尽，独向深松积。落照入寒光，偏能伴幽寂。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 80
    },
    {
        "title": "新柳",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "全欺芳蕙晚，似妒寒梅疾。撩乱发青条，春风来几日。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 81
    },
    {
        "title": "唐昌公主院看花",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "遗殿空长闭，乘鸾自不回。至今荒草上，寥落旧花开。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 82
    },
    {
        "title": "别张赞",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "今日山晴后，残蝉菊发时。登楼见秋色，何处最相思。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 83
    },
    {
        "title": "晚思",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "蛩余窗下月，草湿阶前露。晚景凄我衣，秋风入庭树。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 84
    },
    {
        "title": "留卢秦卿",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "知有前期在，难分此夜中。无将故人酒，不及石尤风。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 85
    },
    {
        "title": "登岘亭",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "岘山回首望秦关，南向荆州几日还。",
            "今日登临唯有泪，不知风景在何山。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 86
    },
    {
        "title": "哭麹山人",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "忆昔秋风起，君曾叹逐臣。何言芳草日，自作九泉人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 87
    },
    {
        "title": "过坚上人故院与李端同赋",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "旧依支遁宿，曾与戴颙来。今日空林下，唯知见绿苔。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 88
    },
    {
        "title": "病中嫁女妓",
        "author": "司空曙",
        "biography": "",
        "paragraphs": [
            "万事伤心在目前，一身垂泪对花筵。",
            "黄金用尽教歌舞，留与他人乐少年。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百九十二",
        "no#": 89
