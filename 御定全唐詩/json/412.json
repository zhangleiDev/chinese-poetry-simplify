[
    {
        "title": "雪后宿同轨店，上法护寺钟楼望月",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "满山残雪满山风，野寺无门院院空。",
            "烟火渐稀孤店静，月明深夜古楼中。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十二",
        "no#": 1
    },
    {
        "title": "陪韦尚书丈归履信宅，因赠韦氏兄弟",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "紫垣驺骑入华居，公子文衣护锦舆。",
            "眠阁书生复何事，也骑羸马从尚书。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十二",
        "no#": 2
    },
    {
        "title": "永贞二年正月二日上御丹凤楼赦天下予与李公垂庾顺之闲行曲江不及盛观",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "春来饶梦慵朝起，不看千官拥御楼。",
            "却著闲行是忙事，数人同傍曲江头。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十二",
        "no#": 3
    },
    {
        "title": "韦居守晚岁常言退休之志因署其居曰大隐洞命予赋诗因赠绝句",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "谢公潜有东山意，已向朱门启洞门。",
            "大隐犹疑恋朝市，不知名作罢归园。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十二",
        "no#": 4
    },
    {
        "title": "赠李十二牡丹花片，因以饯行",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "莺涩余声絮堕风，牡丹花尽叶成丛。",
            "可怜颜色经年别，收取朱阑一片红。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十二",
        "no#": 5
    },
    {
        "title": "题李十一修行里居壁",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "云阙朝回尘骑合，杏花春尽曲江闲。",
            "怜君虽在城中住，不隔人家便是山。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十二",
        "no#": 6
    },
    {
        "title": "靖安穷居",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "喧静不由居远近，大都车马就权门。",
            "野人住处无名利，草满空阶树满园。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十二",
        "no#": 7
    },
    {
        "title": "赠乐天",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "等闲相见销长日，也有闲时更学琴。",
            "不是眼前无外物，不关心事不经心。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十二",
        "no#": 8
    },
    {
        "title": "使东川：骆口驿二首",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "邮亭壁上数行字，崔李题名王白诗。",
            "尽日无人共言语，不离墙下至行时。",
            "二星徼外通蛮服，五夜灯前草御文。",
            "我到东川恰相半，向南看月北看云。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十二",
        "no#": 9
    },
    {
        "title": "使东川：清明日",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "常年寒食好风轻，触处相随取次行。",
            "今日清明汉江上，一身骑马县官迎。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十二",
        "no#": 10
    },
    {
        "title": "使东川：亚枝红",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "平阳池上亚枝红，怅望山邮事事同。",
            "还向万竿深竹里，一枝浑卧碧流中。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十二",
        "no#": 11
    },
    {
        "title": "使东川：梁州梦",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "梦君同绕曲江头，也向慈恩院院游。",
            "亭吏呼人排去马，所惊身在古梁州。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十二",
        "no#": 12
    },
    {
        "title": "使东川：南秦雪",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "帝城寒尽临寒食，骆谷春深未有春。才见岭头云似盖，",
            "飞鸟不飞猿不动，青骢御史上南秦。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十二",
        "no#": 13
    },
    {
        "title": "使东川：江楼月",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "嘉陵江岸驿楼中，江在楼前月在空。月色满床兼满地，",
            "万一帝乡还洁白，几人潜傍杏园东。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十二",
        "no#": 14
    },
    {
        "title": "使东川：惭问囚",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "司马子微坛上头，与君深结白云俦。尚平村落拟连买，",
            "那知今日蜀门路，带月夜行缘问囚。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十二",
        "no#": 15
    },
    {
        "title": "使东川：江上行",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "闷见汉江流不息，悠悠漫漫竟何成。",
            "江流不语意相问，何事远来江上行。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十二",
        "no#": 16
    },
    {
        "title": "使东川：汉江上笛",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "小年为写游梁赋，最说汉江闻笛愁。",
            "今夜听时在何处，月明西县驿南楼。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十二",
        "no#": 17
    },
    {
        "title": "使东川：邮亭月",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "君多务实我多情，大抵偏嗔步月明。",
            "今夜山邮与蛮嶂，君应坚卧我还行。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十二",
        "no#": 18
    },
    {
        "title": "使东川：嘉陵驿二首",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "嘉陵驿上空床客，一夜嘉陵江水声。",
            "仍对墙南满山树，野花撩乱月胧明。",
            "墙外花枝压短墙，月明还照半张床。",
            "无人会得此时意，一夜独眠西畔廊。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十二",
        "no#": 19
    },
    {
        "title": "使东川：百牢关",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "嘉陵江上万重山，何事临江一破颜。",
            "自笑只缘任敬仲，等闲身度百牢关。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十二",
        "no#": 20
    },
    {
        "title": "使东川：江花落",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "日暮嘉陵江水东，梨花万片逐江风。",
            "江花何处最肠断，半落江流半在空。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十二",
        "no#": 21
    },
    {
        "title": "使东川：嘉陵江二首",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "秦人惟识秦中水，长想吴江与蜀江。",
            "今日嘉川驿楼下，可怜如练绕明窗。",
            "千里嘉陵江水声，何年重绕此江行。",
            "只应添得清宵梦，时见满江流月明。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十二",
        "no#": 22
    },
    {
        "title": "使东川：西县驿",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "去时楼上清明夜，月照楼前撩乱花。",
            "今日成阴复成子，可怜春尽未还家。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十二",
        "no#": 23
    },
    {
        "title": "使东川：望喜驿",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "满眼文书堆案边，眼昏偷得暂时眠。",
            "子规惊觉灯又灭，一道月光横枕前。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十二",
        "no#": 24
    },
    {
        "title": "使东川：好时节",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "身骑骢马峨眉下，面带霜威卓氏前。",
            "虚度东川好时节，酒楼元被蜀儿眠。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十二",
        "no#": 25
    },
    {
        "title": "使东川：夜深行",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "夜深犹自绕江行，震地江声似鼓声。",
            "渐见戍楼疑近驿，百牢关吏火前迎。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十二",
        "no#": 26
    },
    {
        "title": "使东川：望驿台",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "可怜三月三旬足，怅望江边望驿台。",
            "料得孟光今日语，不曾春尽不归来。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十二",
        "no#": 27
    },
    {
        "title": "赠咸阳少府萧郎",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "莫怪逢君泪每盈，仲由多感有深情。陆家幼女托良婿，",
            "别时何处最肠断，日暮渭阳驱马行。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十二",
        "no#": 28
    },
    {
        "title": "赠吕三校书",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "同年同拜校书郎，触处潜行烂熳狂。共占花园争赵辟，",
            "语到欲明欢又泣，傍人相笑两相伤。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十二",
        "no#": 29
    },
    {
        "title": "封书",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "鹤台南望白云关，城市犹存暂一还。",
            "书出步虚三百韵，蕊珠文字在人间。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十二",
        "no#": 30
    },
    {
        "title": "仁风李著作园醉后寄李十",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "胧明春月照花枝，花下音声是管儿。",
            "却笑西京李员外，五更骑马趁朝时。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十二",
        "no#": 31
    },
    {
        "title": "灯影",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "洛阳昼夜无车马，漫挂红纱满树头。",
            "见说平时灯影里，玄宗潜伴太真游。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十二",
        "no#": 32
    },
    {
        "title": "贬江陵途中寄乐天杓直杓直以员外郎判盐铁乐天以拾遗在翰林",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "想到江陵无一事，酒杯书卷缀新文。紫芽嫩茗和枝采，",
            "算缗草诏终须解，不敢将心远羡君。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十二",
        "no#": 33
    },
    {
        "title": "渡汉江",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "嶓冢去年寻漾水，襄阳今日渡江𣸣。山遥远树才成点，",
            "鲵鲸归穴东溟溢，又作波涛随伍员。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十二",
        "no#": 34
    },
    {
        "title": "哀病骢呈致用",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "枥上病骢啼褭褭，江边废宅路迢迢。自经梅雨长垂耳，",
            "曾听禁漏惊衙鼓，惯蹋康衢怕小桥。半夜雄嘶心不死，"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十二",
        "no#": 35
    },
    {
        "title": "送岭南崔侍御",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "我是北人长北望，每嗟南雁更南飞。君今又作岭南别，",
            "黄家贼用镩刀利，白水郎行旱地稀。蜃吐朝光楼隐隐，",
            "火布垢尘须火浣，木绵温软当绵衣。桄榔面碜槟榔涩，",
            "此中无限相忧事，请为殷勤事事依。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十二",
        "no#": 36
    },
    {
        "title": "酬乐天八月十五夜禁中独直玩月见寄",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "一年秋半月偏深，况就烟霄极赏心。金凤台前波漾漾，",
            "何意枚皋正承诏，瞥然尘念到江阴。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十二",
        "no#": 37
    },
    {
        "title": "予病瘴乐天寄通中散碧腴垂云膏仍题四韵以慰远怀开坼之间因有酬答",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "紫河变炼红霞散，翠液煎研碧玉英。金籍真人天上合，",
            "唯有思君治不得，膏销雪尽意还生。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十二",
        "no#": 38
    }
]