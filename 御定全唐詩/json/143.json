[
    {
        "title": "朝来曲",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "月昃鸣珂动，花连绣户春。盘龙玉台镜，唯待画眉人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 1
    },
    {
        "title": "从军行",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "大将军出战，白日暗榆关。三面黄金甲，单于破胆还。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 2
    },
    {
        "title": "答武陵田太守",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "仗剑行千里，微躯感一言。曾为大梁客，不负信陵恩。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 3
    },
    {
        "title": "题灞池二首",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "腰镰欲何之，东园刈秋韭。世事不复论，悲歌和樵叟。",
            "开门望长川，薄暮见渔者。借问白头翁，垂纶几年也。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 4
    },
    {
        "title": "题僧房",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "棕榈花满院，苔藓入闲房。彼此名言绝，空中闻异香。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 5
    },
    {
        "title": "击磬老人",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "双峰褐衣久，一磬白眉长。谁识野人意，徒看春草芳。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 6
    },
    {
        "title": "送胡大",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "荆门不堪别，况乃潇湘秋。何处遥望君，江边明月楼。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 7
    },
    {
        "title": "送郭司仓",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "映门淮水绿，留骑主人心。明月随良掾，春潮夜夜深。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 8
    },
    {
        "title": "送李十五",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "怨别秦楚深，江中秋云起。天长杳无隔，月影在寒水。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 9
    },
    {
        "title": "送张四",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "枫林已愁暮，楚水复堪悲。别后冷山月，清猿无断时。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 10
    },
    {
        "title": "武陵田太守席送司马卢溪",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "诸侯分楚郡，饮饯五溪春。山水清晖远，俱怜一逐臣。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 11
    },
    {
        "title": "送谭八之桂林",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "客心仍在楚，江馆复临湘。别意猿鸟外，天寒桂水长。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 12
    },
    {
        "title": "送刘十五之郡",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "平明江雾寒，客马江上发。扁舟事洛阳，窅窅含楚月。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 13
    },
    {
        "title": "从军行七首",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "烽火城西百尺楼，黄昏独上海风秋。",
            "更吹羌笛关山月，无那金闺万里愁。",
            "琵琶起舞换新声，总是关山旧别情。",
            "撩乱边愁听不尽，高高秋月照长城。",
            "关城榆叶早疏黄，日暮云沙古战场。",
            "表请回军掩尘骨，莫教兵士哭龙荒。",
            "青海长云暗雪山，孤城遥望玉门关。",
            "黄沙百战穿金甲，不破楼兰终不还。",
            "大漠风尘日色昏，红旗半卷出辕门。",
            "前军夜战洮河北，已报生擒吐谷浑。",
            "胡瓶落膊紫薄汗，碎叶城西秋月团。",
            "明敕星驰封宝剑，辞君一夜取楼兰。",
            "玉门山嶂几千重，山北山南总是烽。",
            "人依远戍须看火，马踏深山不见踪。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 14
    },
    {
        "title": "出塞二首",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "秦时明月汉时关，万里长征人未还。",
            "但使龙城飞将在，不教胡马度阴山。",
            "骝马新跨白玉鞍，战罢沙场月色寒。",
            "城头铁鼓声犹振，匣里金刀血未干。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 15
    },
    {
        "title": "采莲曲二首",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "吴姬越艳楚王妃，争弄莲舟水湿衣。",
            "来时浦口花迎入，采罢江头月送归。",
            "荷叶罗裙一色裁，芙蓉向脸两边开。",
            "乱入池中看不见，闻歌始觉有人来。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 16
    },
    {
        "title": "殿前曲二首",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "贵人妆梳殿前催，香风吹入殿后来。",
            "仗引笙歌大宛马，白莲花发照池台。",
            "胡部笙歌西殿头，梨园弟子和凉州。",
            "新声一段高楼月，圣主千秋乐未休。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 17
    },
    {
        "title": "春宫曲",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "昨夜风开露井桃，未央前殿月轮高。",
            "平阳歌舞新承宠，帘外春寒赐锦袍。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 18
    },
    {
        "title": "西宫春怨",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "西宫夜静百花香，欲卷珠帘春恨长。",
            "斜抱云和深见月，朦胧树色隐昭阳。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 19
    },
    {
        "title": "西宫秋怨",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "芙蓉不及美人妆，水殿风来珠翠香。",
            "谁分含啼掩秋扇，空悬明月待君王。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 20
    },
    {
        "title": "长信秋词五首",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "金井梧桐秋叶黄，珠帘不卷夜来霜。",
            "熏笼玉枕无颜色，卧听南宫清漏长。",
            "高殿秋砧响夜阑，霜深犹忆御衣寒。",
            "银灯青琐裁缝歇，还向金城明主看。",
            "奉帚平明金殿开，且将团扇暂裴回。",
            "玉颜不及寒鸦色，犹带昭阳日影来。",
            "真成薄命久寻思，梦见君王觉后疑。",
            "火照西宫知夜饮，分明复道奉恩时。",
            "长信宫中秋月明，昭阳殿下捣衣声。",
            "白露堂中细草迹，红罗帐里不胜情。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 21
    },
    {
        "title": "青楼曲二首",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "白马金鞍从武皇，旌旗十万宿长杨。",
            "楼头小妇鸣筝坐，遥见飞尘入建章。",
            "驰道杨花满御沟，红妆缦绾上青楼。",
            "金章紫绶千余骑，夫婿朝回初拜侯。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 22
    },
    {
        "title": "青楼怨",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "香帏风动花入楼，高调鸣筝缓夜愁。",
            "肠断关山不解说，依依残月下帘钩。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 23
    },
    {
        "title": "浣纱女",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "钱塘江畔是谁家，江上女儿全胜花。",
            "吴王在时不得出，今日公然来浣纱。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 24
    },
    {
        "title": "闺怨",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "闺中少妇不曾愁，春日凝妆上翠楼。",
            "忽见陌头杨柳色，悔教夫婿觅封侯。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 25
    },
    {
        "title": "甘泉歌",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "乘舆执玉已登坛，细草沾衣春殿寒。",
            "昨夜云生拜初月，万年甘露水晶盘。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 26
    },
    {
        "title": "萧驸马宅花烛",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "青鸾飞入合欢宫，紫凤衔花出禁中。",
            "可怜今夜千门里，银汉星回一道通。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 27
    },
    {
        "title": "观猎",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "角鹰初下秋草稀，铁骢抛鞚去如飞。",
            "少年猎得平原兔，马后横捎意气归。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 28
    },
    {
        "title": "寄穆侍御出幽州",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "一从恩谴度潇湘，塞北江南万里长。",
            "莫道蓟门书信少，雁飞犹得到衡阳。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 29
    },
    {
        "title": "寄陶副使",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "闻道将军破海门，如何远谪渡湘沅。",
            "春来明主封西岳，自有还君紫绶恩。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 30
    },
    {
        "title": "至南陵答皇甫岳",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "与君同病复漂沦，昨夜宣城别故人。",
            "明主恩深非岁久，长江还共五溪滨。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 31
    },
    {
        "title": "西江寄越弟",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "南浦逢君岭外还，沅溪更远洞庭山。",
            "尧时恩泽如春雨，梦里相逢同入关。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 32
    },
    {
        "title": "李四仓曹宅夜饮",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "霜天留后故情欢，银烛金炉夜不寒。",
            "欲问吴江别来意，青山明月梦中看。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 33
    },
    {
        "title": "宴春源",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "源向春城花几重，江明深翠引诸峰。",
            "与君醉失松溪路，山馆寥寥传暝钟。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 34
    },
    {
        "title": "龙标野宴",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "沅溪夏晚足凉风，春酒相携就竹丛。",
            "莫道弦歌愁远谪，青山明月不曾空。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 35
    },
    {
        "title": "听流人水调子",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "孤舟微月对枫林，分付鸣筝与客心。",
            "岭色千重万重雨，断弦收与泪痕深。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 36
    },
    {
        "title": "梁苑",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "梁园秋竹古时烟，城外风悲欲暮天。",
            "万乘旌旗何处在，平台宾客有谁怜。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 37
    },
    {
        "title": "武陵龙兴观黄道士房问易因题",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "斋心问易太阳宫，八卦真形一气中。",
            "仙老言余鹤飞去，玉清坛上雨蒙蒙。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 38
    },
    {
        "title": "送魏二",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "醉别江楼橘柚香，江风引雨入舟凉。",
            "忆君遥在潇湘月，愁听清猿梦里长。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 39
    },
    {
        "title": "别李浦之京",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "故园今在灞陵西，江畔逢君醉不迷。",
            "小弟邻庄尚渔猎，一封书寄数行啼。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 40
    },
    {
        "title": "送狄宗亨",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "秋在水清山暮蝉，洛阳树色鸣皋烟。",
            "送君归去愁不尽，又惜空度凉风天。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 41
    },
    {
        "title": "送薛大赴安陆",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "津头云雨暗湘山，迁客离忧楚地颜。",
            "遥送扁舟安陆郡，天边何处穆陵关。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 42
    },
    {
        "title": "芙蓉楼送辛渐二首",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "寒雨连天夜入湖，平明送客楚山孤。",
            "洛阳亲友如相问，一片冰心在玉壶。",
            "丹阳城南秋海阴，丹阳城北楚云深。",
            "高楼送客不能醉，寂寂寒江明月心。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 43
    },
    {
        "title": "重别李评事",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "莫道秋江离别难，舟船明日是长安。",
            "吴姬缓舞留君醉，随意青枫白露寒。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 44
    },
    {
        "title": "别陶副使归南海",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "南越归人梦海楼，广陵新月海亭秋。",
            "宝刀留赠长相忆，当取戈船万户侯。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 45
    },
    {
        "title": "送人归江夏",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "寒江绿水楚云深，莫道离忧迁远心。",
            "晓夕双帆归鄂渚，愁将孤月梦中寻。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 46
    },
    {
        "title": "送李五",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "玉碗金罍倾送君，江西日入起黄云。",
            "扁舟乘月暂来去，谁道沧浪吴楚分。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 47
    },
    {
        "title": "送十五舅",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "深林秋水近日空，归棹演漾清阴中。",
            "夕浦离觞意何已，草根寒露悲鸣虫。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 48
    },
    {
        "title": "留别郭八",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "长亭驻马未能前，井邑苍茫含暮烟。",
            "醉别何须更惆怅，回头不语但垂鞭。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 49
    },
    {
        "title": "送窦七",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "清江月色傍林秋，波上荧荧望一舟。",
            "鄂渚轻帆须早发，江边明月为君留。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 50
    },
    {
        "title": "巴陵送李十二",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "摇曳巴陵洲渚分，清江传语便风闻。",
            "山长不见秋城色，日暮蒹葭空水云。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 51
    },
    {
        "title": "送裴图南",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "黄河渡头归问津，离家几日茱萸新。",
            "漫道闺中飞破镜，犹看陌上别行人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 52
    },
    {
        "title": "留别司马太守",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "辰阳太守念王孙，远谪沅溪何可论。",
            "黄鹤青云当一举，明珠吐著报君恩。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 53
    },
    {
        "title": "卢溪主人",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "武陵溪口驻扁舟，溪水随君向北流。",
            "行到荆门上三峡，莫将孤月对猿愁。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 54
    },
    {
        "title": "送程六",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "冬夜伤离在五溪，青鱼雪落鲙橙齑。",
            "武冈前路看斜月，片片舟中云向西。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 55
    },
    {
        "title": "送朱越",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "远别舟中蒋山暮，君行举首燕城路。",
            "蓟门秋月隐黄云，期向金陵醉江树。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 56
    },
    {
        "title": "别辛渐",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "别馆萧条风雨寒，扁舟月色渡江看。",
            "酒酣不识关西道，却望春江云尚残。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 57
    },
    {
        "title": "送柴侍御",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "流水通波接武冈，送君不觉有离伤。",
            "青山一道同云雨，明月何曾是两乡。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 58
    },
    {
        "title": "送万大归长沙",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "桂阳秋水长沙县，楚竹离声为君变。",
            "青山隐隐孤舟微，白鹤双飞忽相见。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 59
    },
    {
        "title": "送吴十九往沅陵",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "沅江流水到辰阳，溪口逢君驿路长。",
            "远谪谁知望雷雨，明年春水共还乡。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 60
    },
    {
        "title": "别皇甫五",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "溆浦潭阳隔楚山，离尊不用起愁颜。",
            "明祠灵响期昭应，天泽俱从此路还。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 61
    },
    {
        "title": "送崔参军往龙溪",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "龙溪只在龙标上，秋月孤山两相向。",
            "谴谪离心是丈夫，鸿恩共待春江涨。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 62
    },
    {
        "title": "送郑判官",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "东楚吴山驿树微，轺车衔命奉恩辉。",
            "英僚携出新丰酒，半道遥看骢马归。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 63
    },
    {
        "title": "送姚司法归吴",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "吴掾留觞楚郡心，洞庭秋雨海门阴。",
            "但令意远扁舟近，不道沧江百丈深。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 64
    },
    {
        "title": "送高三之桂林",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "留君夜饮对潇湘，从此归舟客梦长。",
            "岭上梅花侵雪暗，归时还拂桂花香。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 65
    },
    {
        "title": "旅望",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "白花原头望京师，黄河水流无尽时。",
            "穷秋旷野行人绝，马首东来知是谁。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 66
    },
    {
        "title": "题朱炼师山房",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "叩齿焚香出世尘，斋坛鸣磬步虚人。",
            "百花仙酝能留客，一饭胡麻度几春。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 67
    },
    {
        "title": "武陵开元观黄炼师院三首",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "松间白发黄尊师，童子烧香禹步时。",
            "欲访桃源入溪路，忽闻鸡犬使人疑。",
            "先贤盛说桃花源，尘忝何堪武陵郡。",
            "闻道秦时避地人，至今不与人通问。",
            "山观空虚清静门，从官役吏扰尘喧。",
            "暂因问俗到真境，便欲投诚依道源。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 68
    },
    {
        "title": "河上老人歌",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "河上老人坐古槎，合丹只用青莲花。",
            "至今八十如四十，口道沧溟是我家。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 69
    },
    {
        "title": "春怨",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "音书杜绝白狼西，桃李无颜黄鸟啼。",
            "寒雁春深归去尽，出门肠断草萋萋。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 70
    },
    {
        "title": "句",
        "author": "王昌龄",
        "biography": "",
        "paragraphs": [
            "朝荐抱良策，独倚江城楼。",
            "昨从金陵邑，远谪沅溪滨。",
            "娟魄已三孕。。",
            "驾幸温泉日，严霜子月初。",
            "长亭酒未醒，千里风动地。",
            "苍荻寒沧江，石头岸边饮。",
            "天仗森森练雪凝，身骑铁骢自臂鹰。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷一百四十三",
        "no#": 71
    }
]