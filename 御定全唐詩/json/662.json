[
    {
        "title": "夜泊义兴戏呈邑宰",
        "author": "罗隐",
        "biography": "",
        "paragraphs": [
            "溪畔维舟问戴星，此中三害有图经。",
            "长桥可避南山远，却恐难防是最灵。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百六十二",
        "no#": 1
    },
    {
        "title": "听琵琶",
        "author": "罗隐",
        "biography": "",
        "paragraphs": [
            "香筵酒散思朝散，偶向梧桐暗处闻。",
            "大底曲中皆有恨，满楼人自不知君。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百六十二",
        "no#": 2
    },
    {
        "title": "经耒阳杜工部墓",
        "author": "罗隐",
        "biography": "",
        "paragraphs": [
            "紫菊馨香覆楚醪，奠君江畔雨萧骚。旅魂自是才相累，",
            "屈原宋玉邻君处，几驾青螭缓郁陶。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百六十二",
        "no#": 3
    },
    {
        "title": "题袁溪张逸人所居",
        "author": "罗隐",
        "biography": "",
        "paragraphs": [
            "蒲梢猎猎燕差差，数里溪光日落时。芳树文君机上锦，",
            "若使浮名拘绊得，世间何处有男儿。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百六十二",
        "no#": 4
    },
    {
        "title": "升平公主旧第",
        "author": "罗隐",
        "biography": "",
        "paragraphs": [
            "乘凤仙人降此时，玉篇才罢到文词。两轮水硙光明照，",
            "坛场客散香街暝，惆怅齐竽取次吹。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百六十二",
        "no#": 5
    },
    {
        "title": "寄黔中王从事",
        "author": "罗隐",
        "biography": "",
        "paragraphs": [
            "故人刀笔事军书，南转黔江半月余。别后乡关情几许，",
            "今日举觞君莫问，生涯牢落鬓萧疏。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百六十二",
        "no#": 6
    },
    {
        "title": "关亭春望",
        "author": "罗隐",
        "biography": "",
        "paragraphs": [
            "关畔春云拂马头，马前春事共悠悠。风摇岸柳长条困，",
            "未知至竟将何用，渭水泾川一向流。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百六十二",
        "no#": 7
    },
    {
        "title": "寄徐济进士",
        "author": "罗隐",
        "biography": "",
        "paragraphs": [
            "往年疏懒共江湖，月满花香记得无。霜压楚莲秋后折，",
            "出得函关抽得手，从来不及阮元瑜。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百六十二",
        "no#": 8
    },
    {
        "title": "寄韦赡",
        "author": "罗隐",
        "biography": "",
        "paragraphs": [
            "石城蓑笠阻心期，落尽山花有所思。羸马二年蓬转后，",
            "禅智阑干市桥酒，纵然相见只相悲。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百六十二",
        "no#": 9
    },
    {
        "title": "霅溪晚泊寄裴庶子",
        "author": "罗隐",
        "biography": "",
        "paragraphs": [
            "溪风如扇雨如丝，闲步闲吟柳恽诗。杯酒疏狂非曩日，",
            "满眼云山莫相笑，与君俱是受深知。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百六十二",
        "no#": 10
    },
    {
        "title": "送姚安之赴任秋浦",
        "author": "罗隐",
        "biography": "",
        "paragraphs": [
            "官罢春坊地象雷，片帆高指贵池开。五侯水暖鱼鳞去，",
            "到头称意须年少，赢得时光向酒杯。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百六十二",
        "no#": 11
    },
    {
        "title": "寄乔逸人",
        "author": "罗隐",
        "biography": "",
        "paragraphs": [
            "南经湘浦北扬州，别后风帆几度游。春酒谁家禁烂漫，",
            "长短此行须入手，更饶君占一年秋。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百六十二",
        "no#": 12
    },
    {
        "title": "塞外",
        "author": "罗隐",
        "biography": "",
        "paragraphs": [
            "塞外偷儿塞内兵，圣君宵旰望升平。碧幢未作朝廷计，",
            "汉王第宅秦田土，今日将军已自荣。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百六十二",
        "no#": 13
    },
    {
        "title": "裴庶子除太仆卿因贺",
        "author": "罗隐",
        "biography": "",
        "paragraphs": [
            "楚珪班序未为轻，莫惜良途副圣明。宫省旧推皇甫谧，",
            "应笑马安虚巧宦，四回迁转始为卿。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百六十二",
        "no#": 14
    },
    {
        "title": "咏史",
        "author": "罗隐",
        "biography": "",
        "paragraphs": [
            "蠹简遗编试一寻，寂寥前事似如今。徐陵笔砚珊瑚架，",
            "九原郝泚何由起，虚误西蕃八尺金。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百六十二",
        "no#": 15
    },
    {
        "title": "中元夜泊淮口",
        "author": "罗隐",
        "biography": "",
        "paragraphs": [
            "木叶回飘水面平，偶因孤棹已三更。秋凉雾露侵灯下，",
            "锦帆天子狂魂魄，应过扬州看月明。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百六十二",
        "no#": 16
    },
    {
        "title": "寄池州郑员外",
        "author": "罗隐",
        "biography": "",
        "paragraphs": [
            "兽绕朱轮酒满船，郡城萧洒贵池边。衣同莱子曾分笔，",
            "陵阳百姓将何福，社舞村歌又一年。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百六十二",
        "no#": 17
    },
    {
        "title": "归梦",
        "author": "罗隐",
        "biography": "",
        "paragraphs": [
            "陆海波涛渐渐深，一回归梦抵千金。路傍草色休多事，",
            "贪财败阵谁相悉，鲍叔如今不可寻。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百六十二",
        "no#": 18
    },
    {
        "title": "送溪州使君",
        "author": "罗隐",
        "biography": "",
        "paragraphs": [
            "兵寇伤残国力衰，就中南土藉良医。凤衔泥诏辞丹阙，",
            "灞桥酒醆黔巫月，从此江心两所思。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百六十二",
        "no#": 19
    },
    {
        "title": "送霅川郑员外",
        "author": "罗隐",
        "biography": "",
        "paragraphs": [
            "明时塞诏列分麾，东拥朱轮出帝畿。铜虎贵提天子印，",
            "知有掖垣南步在，可能须待政成归。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百六十二",
        "no#": 20
    },
    {
        "title": "酬寄右司李员外",
        "author": "罗隐",
        "biography": "",
        "paragraphs": [
            "当年忆见桂枝春，自此清途未四旬。左省望高推健笔，",
            "犹把随和向泥滓，应怜疏散任天真。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百六十二",
        "no#": 21
    },
    {
        "title": "莲塘驿",
        "author": "罗隐",
        "biography": "",
        "paragraphs": [
            "莲塘馆东初日明，莲塘馆西行人行。隔林啼鸟似相应，",
            "莫言来去只如此，君看鬓边霜几茎。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百六十二",
        "no#": 22
    },
    {
        "title": "甘露寺火后",
        "author": "罗隐",
        "biography": "",
        "paragraphs": [
            "六朝胜事已尘埃，犹有闲人怅望来。只道鬼神能护物，",
            "还识平泉故侯否，一生踪迹比楼台。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百六十二",
        "no#": 23
    },
    {
        "title": "春日登上元石头故城",
        "author": "罗隐",
        "biography": "",
        "paragraphs": [
            "万里伤心极目春，东南王气只逡巡。野花相笑落满地，",
            "太平寺主惟轻薄，却把三公与贼臣。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百六十二",
        "no#": 24
    },
    {
        "title": "送宣武徐巡官",
        "author": "罗隐",
        "biography": "",
        "paragraphs": [
            "傲睨公卿二十年，东来西去只悠然。白知关畔元非马，",
            "临行不惜刀圭便，愁杀长安买笑钱。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百六十二",
        "no#": 25
    },
    {
        "title": "冬暮寄裴郎中",
        "author": "罗隐",
        "biography": "",
        "paragraphs": [
            "晓发星星入镜宜，早年容易近年悲。敢言得事时将晚，",
            "仙郎旧有黄金约，沥胆隳肝更祷祈。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百六十二",
        "no#": 26
    },
    {
        "title": "中元甲子以辛丑驾幸蜀四首",
        "author": "罗隐",
        "biography": "",
        "paragraphs": [
            "子仪不起浑瑊亡，西幸谁人从武皇。四海为家虽未远，",
            "应感两朝巡狩迹，绿槐端正驿荒凉。",
            "爪牙柱石两俱销，一点渝尘九土摇。敢恨甲兵为弃物，",
            "跪望嵕山重启告，可能余烈不胜妖。",
            "邪气奔屯瑞气移，清平过尽到艰危。纵饶犬彘迷常理，",
            "可怜一曲还京乐，重对红蕉教蜀儿。",
            "白丁攘臂犯长安，翠辇苍黄路屈盘。丹凤有情尘外远，",
            "不将不侯何计是，钓鱼船上泪阑干。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百六十二",
        "no#": 27
    },
    {
        "title": "题润州妙善前石羊",
        "author": "罗隐",
        "biography": "",
        "paragraphs": [
            "紫髯桑盖此沈吟，很石犹存事可寻。汉鼎未安聊把手，",
            "还有市鄽沽酒客，雀喧鸠聚话蹄涔。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百六十二",
        "no#": 28
    },
    {
        "title": "登宛陵条风楼寄窦常侍",
        "author": "罗隐",
        "biography": "",
        "paragraphs": [
            "乱罹时节懒登临，试借条风半日吟。只有远山含暖律，",
            "自笑疏慵似麋鹿，也教台上费黄金。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百六十二",
        "no#": 29
    },
    {
        "title": "台城",
        "author": "罗隐",
        "biography": "",
        "paragraphs": [
            "晚云阴映下空城，六代累累夕照明。玉井已干龙不起，",
            "深谷作陵山作海，茂弘流辈莫伤情。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百六十二",
        "no#": 30
    },
    {
        "title": "甘露寺看雪上周相公",
        "author": "罗隐",
        "biography": "",
        "paragraphs": [
            "筛寒洒白乱溟蒙，祷请功兼造化功。光薄乍迷京口月，",
            "一种为祥君看取，半禳灾沴半年丰。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百六十二",
        "no#": 31
    },
    {
        "title": "寄京阙陆郎中昆仲",
        "author": "罗隐",
        "biography": "",
        "paragraphs": [
            "柏台兰署四周旋，宾榻何妨雁影连。才见玳簪欹细柳，",
            "争奈乱罹人渐少，麦城新赋许谁传。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百六十二",
        "no#": 32
    },
    {
        "title": "偶题",
        "author": "罗隐",
        "biography": "",
        "paragraphs": [
            "钟陵醉别十余春，重见云英掌上身。",
            "我未成名君未嫁，可能俱是不如人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百六十二",
        "no#": 33
    },
    {
        "title": "故都",
        "author": "罗隐",
        "biography": "",
        "paragraphs": [
            "江南江北两风流，一作迷津一拜侯。",
            "至竟不如隋炀帝，破家犹得到扬州。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百六十二",
        "no#": 34
    },
    {
        "title": "董仲舒",
        "author": "罗隐",
        "biography": "",
        "paragraphs": [
            "灾变儒生不合闻，谩将刀笔指乾坤。",
            "偶然留得阴阳术，闭却南门又北门。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百六十二",
        "no#": 35
    },
    {
        "title": "献尚父大王",
        "author": "罗隐",
        "biography": "",
        "paragraphs": [
            "数年铁甲定东瓯，夜渡江山瞻斗牛。",
            "今日朱方平殄后，虎符龙节十三州。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百六十二",
        "no#": 36
    },
    {
        "title": "蜂",
        "author": "罗隐",
        "biography": "",
        "paragraphs": [
            "不论平地与山尖，无限风光尽被占。",
            "采得百花成蜜后，为谁辛苦为谁甜。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百六十二",
        "no#": 37
    },
    {
        "title": "帘二首",
        "author": "罗隐",
        "biography": "",
        "paragraphs": [
            "叠影重纹映画堂，玉钩银烛共荧煌。",
            "会应得见神仙在，休下真珠十二行。",
            "翡翠佳名世共稀，玉堂高下巧相宜。",
            "殷勤为嘱纤纤手，卷上银钩莫放垂。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百六十二",
        "no#": 38
    }
]