[
    {
        "title": "伤小女",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "一岁犹未满，九泉何太深。唯余卷书草，相对共伤心。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十五",
        "no#": 1
    },
    {
        "title": "秋江晓望",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "万顷湖天碧，一星飞鹭白。此时放怀望，不厌为浮客。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十五",
        "no#": 2
    },
    {
        "title": "闲夜酒醒",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "醒来山月高，孤枕群书里。酒渴漫思茶，山童呼不起。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十五",
        "no#": 3
    },
    {
        "title": "和鲁望风人诗三首",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "刻石书离恨，因成别后悲。莫言春茧薄，犹有万重思。",
            "镂出容刀饰，亲逢巧笑难。日中骚客佩，争奈即阑干。",
            "江上秋声起，从来浪得名。逆风犹挂席，若不会凡情。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十五",
        "no#": 4
    },
    {
        "title": "古函关",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "破落古关城，犹能扼帝京。今朝行客过，不待晓鸡鸣。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十五",
        "no#": 5
    },
    {
        "title": "聪明泉",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "一勺如琼液，将愚拟望贤。欲知心不变，还似饮贪泉。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十五",
        "no#": 6
    },
    {
        "title": "史处士",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "山期须早赴，世累莫迟留。忽遇狂风起，闲心不自由。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十五",
        "no#": 7
    },
    {
        "title": "芳草渡",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "溪南越乡音，古柳渡江深。日晚无来客，闲船系绿阴。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十五",
        "no#": 8
    },
    {
        "title": "古宫词三首",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "楼殿倚明月，参差如乱峰。宫花半夜发，不待景阳钟。",
            "闲骑小步马，独绕万年枝。尽日看花足，君王自不知。",
            "玉枕寐不足，宫花空触簷。梁间燕不睡，应怪夜明帘。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十五",
        "no#": 9
    },
    {
        "title": "春日陪崔谏议樱桃园宴",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "万树香飘水麝风，蜡熏花雪尽成红。",
            "夜深欢态状不得，醉客图开明月中。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十五",
        "no#": 10
    },
    {
        "title": "松江早春",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "松陵清净雪消初，见底新安恐未如。",
            "稳凭船舷无一事，分明数得鲙残鱼。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十五",
        "no#": 11
    },
    {
        "title": "女坟湖",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "万贵千奢已寂寥，可怜幽愤为谁娇。",
            "须知韩重相思骨，直在芙蓉向下消。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十五",
        "no#": 12
    },
    {
        "title": "泰伯庙",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "一庙争祠两让君，几千年后转清芬。",
            "当时尽解称高义，谁敢教他莽卓闻。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十五",
        "no#": 13
    },
    {
        "title": "宿木兰院",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "木兰院里双栖鹤，长被金钲聒不眠。",
            "今夜宿来还似尔，到明无计梦云泉。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十五",
        "no#": 14
    },
    {
        "title": "重题蔷薇",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "浓似猩猩初染素，轻如燕燕欲凌空。",
            "可怜细丽难胜日，照得深红作浅红。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十五",
        "no#": 15
    },
    {
        "title": "春夕酒醒",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "四弦才罢醉蛮奴，醽醁余香在翠炉。",
            "夜半醒来红蜡短，一枝寒泪作珊瑚。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十五",
        "no#": 16
    },
    {
        "title": "青门闲泛",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "青翰虚徐夏思清，愁烟漠漠荇花平。",
            "醉来欲把田田叶，尽裹当时醒酒鲭。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十五",
        "no#": 17
    },
    {
        "title": "木兰后池三咏：重台莲花",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "欹红婑媠力难任，每叶头边半米金。",
            "可得教他水妃见，两重元是一重心。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十五",
        "no#": 18
    },
    {
        "title": "木兰后池三咏：浮萍",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "嫩似金脂飏似烟，多情浑欲拥红莲。",
            "明朝拟附南风信，寄与湘妃作翠钿。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十五",
        "no#": 19
    },
    {
        "title": "木兰后池三咏：白莲",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "但恐醍醐难并洁，只应薝卜可齐香。",
            "半垂金粉知何似，静婉临溪照额黄。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十五",
        "no#": 20
    },
    {
        "title": "重题后池",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "细雨阑珊眠鹭觉，钿波悠漾并鸳娇。",
            "适来会得荆王意，只为莲茎重细腰。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十五",
        "no#": 21
    },
    {
        "title": "庭中初植松桂，鲁望偶题，奉和次韵",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "毵毵绿发垂轻露，猎猎丹华动细风。",
            "恰似青童君欲会，俨然相向立庭中。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十五",
        "no#": 22
    },
    {
        "title": "鲁望戏题书印囊奉和次韵",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "金篆方圆一寸余，可怜银艾未思渠。",
            "不知夫子将心印，印破人间万卷书。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十五",
        "no#": 23
    },
    {
        "title": "馆娃宫怀古五绝",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "绮合飘香下太湖，乱兵侵晓上姑苏。",
            "越王大有堪羞处，只把西施赚得吴。",
            "郑妲无言下玉墀，夜来飞箭满罘罳。越王定指高台笑，却见当时金镂楣。",
            "半夜娃宫作战场，血腥犹杂宴时香。",
            "西施不及烧残蜡，犹为君王泣数行。",
            "素袜虽遮未掩羞，越兵犹怕伍员头。",
            "吴王恨魄今如在，只合西施濑上游。",
            "响屟廊中金玉步，采苹山上绮罗身。",
            "不知水葬今何处，溪月弯弯欲效颦。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十五",
        "no#": 24
    },
    {
        "title": "虎丘寺西小溪闲泛三绝",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "鼓子花明白石岸，桃枝竹覆翠岚溪。",
            "分明似对天台洞，应厌顽仙不肯迷。",
            "绝壑只怜白羽傲，穷溪唯觉锦鳞痴。",
            "更深尚有通樵处，或是秦人未可知。",
            "高下不惊红翡翠，浅深还碍白蔷薇。",
            "船头系个松根上，欲待逢仙不拟归。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十五",
        "no#": 25
    },
    {
        "title": "天竺寺八月十五日夜桂子",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "玉颗珊珊下月轮，殿前拾得露华新。",
            "至今不会天中事，应是嫦娥掷与人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十五",
        "no#": 26
    },
    {
        "title": "钓侣二章",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "趁眠无事避风涛，一斗霜鳞换浊醪。",
            "惊怪儿童呼不得，尽冲烟雨漉车螯。",
            "严陵滩势似云崩，钓具归来放石层。",
            "烟浪溅篷寒不睡，更将枯蚌点渔灯。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十五",
        "no#": 27
    },
    {
        "title": "寄同年韦校书",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "二年疏放饱江潭，水物山容尽足耽。",
            "唯有故人怜未替，欲封干鲙寄终南。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十五",
        "no#": 28
    },
    {
        "title": "初冬偶作",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "豹皮茵下百余钱，刘堕闲沽尽醉眠。",
            "酒病校来无一事，鹤亡松老似经年。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十五",
        "no#": 29
    },
    {
        "title": "醉中寄鲁望一壶并一绝",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "门巷寥寥空紫苔，先生应渴解酲杯。",
            "醉中不得亲相倚，故遣青州从事来。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十五",
        "no#": 30
    },
    {
        "title": "更次来韵寄鲁望",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "萧萧红叶掷苍苔，玄晏先生欠一杯。",
            "从此问君还酒债，颜延之送几钱来。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十五",
        "no#": 31
    },
    {
        "title": "重玄寺双矮桧",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "扑地枝回是翠钿，碧丝笼细不成烟。",
            "应如天竺难陀寺，一对狻猊相枕眠。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十五",
        "no#": 32
    },
    {
        "title": "奉酬鲁望醉中戏赠",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "秦吴风俗昔难同，唯有才情事事通。",
            "刚恋水云归不得，前身应是太湖公。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十五",
        "no#": 33
    },
    {
        "title": "皋桥",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "皋桥依旧绿杨中，闾里犹生隐士风。",
            "唯我到来居上馆，不知何道胜梁鸿。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十五",
        "no#": 34
    },
    {
        "title": "军事院霜菊盛开，因书一绝寄上谏议",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "金华千点晓霜凝，独对壶觞又不能。",
            "已过重阳三十日，至今犹自待王弘。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十五",
        "no#": 35
    },
    {
        "title": "悼鹤",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "莫怪朝来泪满衣，坠毛犹傍水花飞。",
            "辽东旧事今千古，却向人间葬令威。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十五",
        "no#": 36
    },
    {
        "title": "醉中先起李縠戏赠走笔奉酬",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "麝烟苒苒生银兔，蜡泪涟涟滴绣闺。",
            "舞袖莫欺先醉去，醒来还解验金泥。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十五",
        "no#": 37
    },
    {
        "title": "奉和鲁望招润卿博士辞以道侣将至之作",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "瘿木樽前地胏图，为君偏辍俗功夫。",
            "灵真散尽光来此，莫恋安妃在后无。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十五",
        "no#": 38
    },
    {
        "title": "奉和再招",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "飙御已应归杳眇，博山犹自对氛氲。",
            "不知入夜能来否，红蜡先教刻五分。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十五",
        "no#": 39
    },
    {
        "title": "酒病偶作",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "郁林步障昼遮明，一炷浓香养病酲。",
            "何事晚来还欲饮，隔墙闻卖蛤蜊声。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十五",
        "no#": 40
    },
    {
        "title": "润卿鲁望寒夜见访，各惜其志，遂成一绝",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "世外为交不是亲，醉吟俱岸白纶巾。",
            "清风月白更三点，未放华阳鹤上人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十五",
        "no#": 41
    },
    {
        "title": "奉和鲁望玩金㶉𫛶戏赠",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "镂羽雕毛迥出群，温黁飘出麝脐熏。",
            "夜来曾吐红茵畔，犹似溪边睡不闻。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十五",
        "no#": 42
    },
    {
        "title": "友人许惠酒以诗征之",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "野客萧然访我家，霜威白菊两三花。",
            "子山病起无余事，只望蒲台酒一车。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十五",
        "no#": 43
    },
    {
        "title": "寒夜文宴润卿有期不至",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "草堂虚洒待高真，不意清斋避世尘。",
            "料得焚香无别事，存心应降月夫人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十五",
        "no#": 44
    },
    {
        "title": "汴河怀古二首",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "万艘龙舸绿丝间，载到扬州尽不还。",
            "应是天教开汴水，一千余里地无山。",
            "尽道隋亡为此河，至今千里赖通波。",
            "若无水殿龙舟事，共禹论功不较多。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十五",
        "no#": 45
    },
    {
        "title": "寄题天台国清寺齐梁体",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "十里松门国清路，饭猿台上菩提树。",
            "怪来烟雨落晴天，元是海风吹瀑布。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十五",
        "no#": 46
    },
