[
    {
        "title": "送崔判官使太原",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "劳君车马此逡巡，我与刘君本世亲。两地山河分节制，",
            "报主由来须尽敌，相期万里宝刀新。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 1
    },
    {
        "title": "幕中诸公有观猎之作因继之",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "刀州城北剑山东，甲士屯云骑散风。旌旆遍张林岭动，",
            "为报府中诸从事，燕然未勒莫论功。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 2
    },
    {
        "title": "同幕中诸公送李侍御归朝",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "昔年专席奉清朝，今日持书即旧僚。珠履会中箫管思，",
            "岁月不堪相送尽，颓颜更被别离凋。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 3
    },
    {
        "title": "送张六谏议归朝",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "诏书前日下丹霄，头戴儒冠脱皂貂。笛怨柳营烟漠漠，",
            "归去朝端如有问，玉关门外老班超。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 4
    },
    {
        "title": "酬严司空荆南见寄",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "金貂再领三公府，玉帐连封万户侯。帘卷青山巫峡晓，",
            "白雪调高歌不得，美人南国翠蛾愁。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 5
    },
    {
        "title": "南徐别业早春有怀",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "生涯扰扰竟何成，自爱深居隐姓名。远雁临空翻夕照，",
            "虚度年华不相见，离肠怀土并关情。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 6
    },
    {
        "title": "摩诃池宴",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "摩诃池上春光早，爱水看花日日来。秾李雪开歌扇掩，",
            "昼短欲将清夜继，西园自有月裴回。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 7
    },
    {
        "title": "至栎阳崇道寺闻严十少府趋侍",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "云连万木夕沉沉，草色泉声古院深。闻说羊车趋盛府，",
            "惆怅送君身未达，不堪摇落听秋砧。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 8
    },
    {
        "title": "春暮郊居寄朱舍人",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "幽深不让子真居，度日闲眠世事疏。春水满池新雨霁，",
            "回首知音青琐闼，何时一为荐相如。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 9
    },
    {
        "title": "送温况游蜀",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "游人西去客三巴，身逐孤蓬不定家。山近峨眉飞暮雨，",
            "应到严君开卦处，将余一为问生涯。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 10
    },
    {
        "title": "崔敷叹春物将谢恨不同览时余方为事牵束及往寻不遇题之留赠",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "九陌迟迟丽景斜，禁街西访隐沦赊。门依高柳空飞絮，",
            "残阳寂寞东城去，惆怅春风落尽花。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 11
    },
    {
        "title": "秋灯对雨寄史近崔积",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "坐听宫城传晚漏，起看衰叶下寒枝。空庭绿草结离念，",
            "相逢莫厌尊前醉，春去秋来自不知。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 12
    },
    {
        "title": "春题龙门香山寺",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "众香天上梵仙宫，钟磬寥寥半碧空。清景乍开松岭月，",
            "欲尽出寻那可得，三千世界本无穷。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 13
    },
    {
        "title": "酬陆三与邹十八侍御",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "城分流水郭连山，拂露开怀一解颜。令尹关中仙史会，",
            "莫恨殷勤留此地，东崖桂树昔同攀。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 14
    },
    {
        "title": "酬谈校书长安秋夜对月寄诸故旧",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "故园千里渺遐情，黄叶萧条白露生。惊鹊绕枝风满幌，",
            "莫怪孔融悲岁序，五侯门馆重娄卿。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 15
    },
    {
        "title": "送田三端公还鄂州",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "孤云迢递恋沧洲，劝酒梨花对白头。南陌送归车骑合，",
            "君去庾公应借问，驰心千里大江流。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 16
    },
    {
        "title": "送李秀才赴滑州诣大夫舅",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "陌头车马去翩翩，白面怀书美少年。东武扬公姻娅重，",
            "知己满朝留不住，贵臣河上拥旌旃。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 17
    },
    {
        "title": "秋日书怀",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "金貂玉铉奉君恩，夜漏晨钟老掖垣。参决万机空有愧，",
            "倦鸟不知归去日，青芜白露满郊园。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 18
    },
    {
        "title": "南昌滩",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "渠江明净峡逶迤，船到名滩拽𥮘迟。橹窡动摇妨作梦，",
            "物色可怜心莫限，此行都是独行时。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 19
    },
    {
        "title": "奉和圣制丰年多庆九日示怀",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "令节寰宇泰，神都佳气浓。赓歌禹功盛，击壤尧年丰。",
            "九奏碧霄里，千官皇泽中。南山澄凝黛，曲水清涵空。",
            "金玉美王度，欢康谣国风。睿文垂日月，永与天无穷。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 20
    },
    {
        "title": "夏日陪冯许二侍郎与严秘书游昊天观览旧题寄同里杨华州中丞",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "三伏草木变，九城车马烦。碧霄回骑射，丹洞入桃源。",
            "台殿云浮栋，緌缨鹤在轩。莫将真破妄，聊用静持喧。",
            "石甃古苔冷，水筠凉簟翻。黄公垆下叹，旌旆国东门。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 21
    },
    {
        "title": "奉和圣制重阳日即事",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "玉烛降寒露，我皇歌古风。重阳德泽展，万国欢娱同。",
            "绮陌拥行骑，香尘凝晓空。神都自蔼蔼，佳气助葱葱。",
            "律吕阴阳畅，景光天地通。徒然被鸿霈，无以报玄功。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 22
    },
    {
        "title": "秋日台中寄怀简诸僚",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "宪府日多事，秋光照碧林。干云岩翠合，布石地苔深。",
            "忧悔耿遐抱，尘埃缁素襟。物情牵局促，友道旷招寻。",
            "颓节风霜变，流年芳景侵。池荷足幽气，烟竹又繁阴。",
            "簪组赤墀恋，池鱼沧海心。涤烦滞幽赏，永度瑶华音。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 23
    },
    {
        "title": "奉酬淮南中书相公见寄",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "扬州隋故都，竹使汉名儒。翊圣恩华异，持衡节制殊。",
            "朝廷连受脤，台座接吁谟。金玉裁王度，丹书奉帝俞。",
            "九重辞象魏，千里握兵符。铁马秋临塞，虹旌夜渡泸。",
            "江长梅笛怨，天远桂轮孤。浩叹烟霜晓，芳期兰蕙芜。",
            "雅言书一劄，宾海雁东隅。岁月奔波尽，音徽雾雨濡。",
            "蜀江分井络，锦浪入淮湖。独抱相思恨，关山不可逾。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 24
    },
    {
        "title": "甫构西亭偶题因呈监军及幕中诸公",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "瀛海无因泛，昆丘岂易寻。数峰聊在目，一境暂清心。",
            "悦彼松柏性，爱兹桃李阴。列芳凭有土，丛干聚成林。",
            "信矣子牟恋，归欤尼父吟。暗香兰露滴，空翠蕙楼深。",
            "负鼎位尝忝，荷戈年屡侵。百城烦鞅掌，九仞喜岖嵚。",
            "巴汉溯沿楫，岷峨千万岑。恩偏不敢去，范蠡畏熔金。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 25
    },
    {
        "title": "和杨弘微春日曲江南望",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "迟景霭悠悠，伤春南陌头。暄风一澹荡，遐思几殷优。",
            "龙去空仙沼，鸾飞掩妓楼。芳菲余雨露，冠盖旧公侯。",
            "朱戟千门闭，黄鹂百啭愁。烟蒙宫树晚，花咽石泉流。",
            "寒谷律潜应，中林兰自幽。商山将避汉，晋室正藩周。",
            "黍稷闻兴叹，琼瑶畏见投。君心即吾事，微向在沧洲。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 26
    },
    {
        "title": "长安秋夜怀陈京昆季",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "钟鼓九衢绝，出门千里同。远情高枕夜，秋思北窗空。",
            "静见烟凝烛，闲听叶坠桐。玉壶思洞彻，琼树忆葱笼。",
            "萤影疏帘外，鸿声暗雨中。羁愁难会面，懒慢责微躬。",
            "甲乙科攀桂，图书阁践蓬。一瓢非可乐，六翮未因风。",
            "寥落悲秋尽，蹉跎惜岁穷。明朝不相见，流泪菊花丛。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 27
    },
    {
        "title": "冬日，汉江南行将赴夏口，途次江陵界，寄裴尚书",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "五部拥双旌，南依墨客卿。关山迥梁甸，波浪接湓城。",
            "烟景迷时候，云帆渺去程。蛤珠冯月吐，芦雁触罗惊。",
            "浦树凝寒晦，江天湛镜清。赏心随处惬，壮志逐年轻。",
            "舟楫不可驻，提封如任情。向方曾指路，射策许言兵。",
            "兰渚歇芳意，菱歌非应声。元戎武昌守，羊祜幸连营。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 28
    },
    {
        "title": "奉酬中书李相公早朝于中书候传点，偶书所怀",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "寥落曙钟断，微明烟月沉。翠霞仙仗合，清漏掖垣深。",
            "北极星遥拱，南山阙迥临。兰釭竟晓焰，琪树欲秋阴。",
            "霄汉惭联步，貂蝉愧并簪。德容温比玉，王度式如金。",
            "鱼水千年运，箫韶九奏音。代天惊度日，掷地喜开襟。",
            "文武时方泰，唐虞道可寻。忝陪申及甫，清净奉尧心。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 29
    },
    {
        "title": "甲午岁相国李公有北园寄赠之作吟玩历时屡促酬答机务不暇未及报章今古遽分电波增感留墓剑而心许感邻笛而意伤寓哀冥寞以广遗韵云",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "机事劳西掖，幽怀寄北园。鹤巢深更静，蝉噪断犹喧。",
            "仙酝百花馥，艳歌双袖翻。碧云诗变雅，皇泽叶流根。",
            "未报雕龙赠，俄伤泪剑痕。佳城关白日，哀挽向青门。",
            "礼命公台重，烟霜陇树繁。天高不可问，空使辅星昏。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 30
    },
    {
        "title": "和杨三舍人晚秋与崔二舍人张秘监苖考功同游昊天观时中书寓直不得陪随因追往年曾与旧僚联游此观纪题在壁已有沦亡书事感怀辄以呈寄兼呈东省三给事之作杨君见征鄙词因以继和",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "瑶圃高秋会，金闺奉诏辰。朱轮天上客，白石洞中人。",
            "佩响泉声杂，朝衣羽服亲。九重青琐闭，三秀紫芝新。",
            "化药秦方士，偷桃汉侍臣。玉笙王子驾，辽鹤令威身。",
            "叹逝颓波速，缄词丽曲春。重将凄恨意，苔壁问遗尘。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 31
    },
    {
        "title": "酬李十一尚书西亭暇日书怀见寄十二韵之作",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "鼎铉昔云忝，西南分主忧。烟尘开僰道，旌节护蛮陬。",
            "任重功无立，力微恩未酬。据鞍惭齿发，责帅惧春秋。",
            "高德闻郑履，俭居称晏裘。三刀君入梦，九折我回辀。",
            "时景屡迁易，兹言期退休。方追故山事，岂谓台阶留。",
            "遐抱清净理，眷言兰杜幽。一缄琼玖赠，万里别离愁。",
            "巴岭云外没，蜀江天际流。怀贤耿遥思，相望凤池头。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 32
    },
    {
        "title": "秋怀奉寄朱补阙",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "上苑繁霜降，骚人起恨初。白云深陋巷，衰草遍闲居。",
            "暮色秋烟重，寒声牖叶虚。潘生秋思苦，陶令世情疏。",
            "已制归田赋，犹陈谏猎书。不知青琐客，投分竟何如。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 33
    },
    {
        "title": "奉酬中书相公至日圜丘行事合于中书宿斋移止于集贤院叙情见寄之什",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "郊庙祗严祀，斋庄觌上玄。别开金虎观，不离紫微天。",
            "树古长杨接，池清太液连。仲山方补衮，文举自伤年。",
            "风溢铜壶漏，香凝绮阁烟。仍闻白雪唱，流咏满鶤弦。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 34
    },
    {
        "title": "途次",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "去国策羸马，劳歌行路难。地崇秦制险，人乐汉恩宽。",
            "御沼澄泉碧，宫梨佛露丹。鼎成仙驭远，龙化宿云残。",
            "不问三苗宠，谁陪万国欢。至今松桂色，长助玉楼寒。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 35
    },
    {
        "title": "题故蔡国公主九华观上池院",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "朱门临九衢，云木蔼仙居。曲沼天波接，层台凤舞余。",
            "曙烟深碧筱，香露湿红蕖。瑶瑟含风韵，纱窗积翠虚。",
            "秦楼今寂寞，真界竟何如。不与蓬瀛异，迢迢远玉除。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 36
    },
    {
        "title": "酬严维秋夜见寄",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "遥夜思悠悠，闻钟远梦休。乱林萤烛暗，零露竹风秋。",
            "启户云归栋，褰帘月上钩。昭明逢圣代，羁旅别沧洲。",
            "骑省潘郎思，衡闱宋玉愁。神仙惭李郭，词赋谢曹刘。",
            "松柏应无变，琼瑶不可酬。谁堪此时景，寂寞下高楼。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 37
    },
    {
        "title": "途次近蜀驿蒙恩赐宝刀及飞龙厩马使还奉寄中书李郑二公",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "草草事行役，迟迟违故关。碧帏遥隐雾，红旆渐依山。",
            "感激酬恩泪，星霜去国颜。捧刀金锡字，归马玉连环。",
            "威凤翔双阙，征夫纵百蛮。应怜宣室召，温树不同攀。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 38
    },
    {
        "title": "归燕",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "春色遍芳菲，闲簷双燕归。还同旧侣至，来绕故巢飞。",
            "敢望烟霄达，多惭羽翮微。衔泥傍金砌，拾蕊到荆扉。",
            "云海经时别，雕梁长日依。主人能一顾，转盻自光辉。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 39
    },
    {
        "title": "送许著作分司东都",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "瑶瑟激凄响，征鸿翻夕阳。署分刊竹简，书蠹护芸香。",
            "马色关城晓，蝉声驿路长。石渠荣正礼，兰室重元方。",
            "不作经年别，离魂亦暂伤。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 40
    },
    {
        "title": "闻相公三兄小园置宴以元衡寓直因寄上兼呈中书三兄",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "休沐限中禁，家山传胜游。露寒潘省夜，木落庾园秋。",
            "兰菊回幽步，壶觞洽旧俦。位高天禄阁，词异畔牢愁。",
            "孤思琴先觉，驰晖水竞流。明朝不相见，清祀在圜丘。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 41
    },
    {
        "title": "酬陆员外歙州许员外郢州二使君",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "吴洲云海接，楚驿梦林长。符节分忧重，鹓鸿去路翔。",
            "艳歌愁翠黛，宝瑟韵清商。洲草遥池合，春风晓旆张。",
            "晋臣多乐广，汉主识冯唐。不作经年别，离魂亦未伤。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 42
    },
    {
        "title": "渐至涪州先寄王使君",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "治教通夷俗，均输问大田。江分巴字水，树入夜郎烟。",
            "毒雾含秋气，阴岩蔽曙天。路难空计日，身老不由年。",
            "将命宁知远，归心讵可传。星郎复何事，出守五溪边。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 43
    },
    {
        "title": "石州城",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "丈夫心爱横行，报国知嫌命轻。楼兰径百战，"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 44
    },
    {
        "title": "寒食下第",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "柳挂九衢丝，花飘万家雪。如何憔悴人，对此芳菲节。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 45
    },
    {
        "title": "途中即事",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "南征复北还，扰扰百年间。自笑红尘里，生涯不暂闲。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 46
    },
    {
        "title": "春日偶作",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "纵横桃李枝，淡荡春风吹。美人歌白苎，万恨在蛾眉。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 47
    },
    {
        "title": "夏夜作",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "夜久喧暂息，池台惟月明。无因驻清景，日出事还生。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 48
    },
    {
        "title": "左掖梨花",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "巧笑解迎人，晴雪香堪惜。随风蝶影翻，误点朝衣赤。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 49
    },
    {
        "title": "同陈六侍御寒食游禅定藏山上人院",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "年少轻行乐，东城南陌头。与君寂寞意，共作草堂游。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 50
    },
    {
        "title": "赠佳人",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "步摇金翠玉搔头，倾国倾城胜莫愁。",
            "若逞仙姿游洛浦，定知神女谢风流。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 51
    },
    {
        "title": "休暇日，中书相公致斋禁省，因以寄赠",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "尝闻圣主得贤臣，三接能令四海春。",
            "月满禁垣斋沐夜，清吟属和更何人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 52
    },
    {
        "title": "春兴",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "杨柳阴阴细雨晴，残花落尽见流莺。",
            "春风一夜吹香梦，梦逐春风到洛城。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 53
    },
    {
        "title": "酬裴起居西亭留题",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "艳歌能起关山恨，红烛偏凝寒塞情。",
            "况是池塘风雨夜，不堪丝管尽离声。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 54
    },
    {
        "title": "送张侍御赴京",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "江南烟雨塞鸿飞，西府文章谢掾归。",
            "相送汀州兰棹晚，菱歌一曲泪沾衣。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 55
    },
    {
        "title": "鄂渚送友",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "云帆淼淼巴陵渡，烟树苍苍故郢城。",
            "江上梅花无数落，送君南浦不胜情。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 56
    },
    {
        "title": "送裴戡行军",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "珠履三千醉不欢，玉人犹苦夜冰寒。",
            "送君偏有无言泪，天下关山行路难。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 57
    },
    {
        "title": "宿青阳驿",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "空山摇落三秋暮，萤过疏帘月露团。",
            "寂寞银灯愁不寐，萧萧风竹夜窗寒。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 58
    },
    {
        "title": "送崔舍人起居",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "赤墀同拜紫泥封，驷牡连征侍九重。",
            "惟有白须张司马，不言名利尚相从。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 59
    },
    {
        "title": "路岐重赋",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "芳郊欲别阑干泪，故国难期聚散云。",
            "分手更逢江驿暮，马嘶猿叫不堪闻。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 60
    },
    {
        "title": "重送卢三十一起居",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "相如拥传有光辉，何事阑干泪湿衣。",
            "旧府东山余妓在，重将歌舞送君归。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 61
    },
    {
        "title": "送张谏议",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "汉庭从事五人来，回首疆场独未回。",
            "今日送君魂断处，寒云寥落数株梅。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 62
    },
    {
        "title": "同诸公夜宴监军玩花之作",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "五侯门馆百花繁，红烛摇风白雪翻。",
            "不似凤凰池畔见，飘扬今隔上林园。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 63
    },
    {
        "title": "郊居寓目偶题",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "晨趋禁掖暮郊园，松桂苍苍烟露繁。",
            "明月上时群动息，雪峰高处正当轩。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 64
    },
    {
        "title": "题嘉陵驿",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "悠悠风旆绕山川，山驿空蒙雨似烟。",
            "路半嘉陵头已白，蜀门西上更青天。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 65
    },
    {
        "title": "送柳郎中裴起居",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "望乡台上秦人在，学射山中杜魄哀。",
            "落日河桥千骑别，春风寂寞旆旌回。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 66
    },
    {
        "title": "秋日出游偶作",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "黄花丹叶满江城，暂爱江头风景清。",
            "闲步欲舒山野性，貔貅不许独行人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 67
    },
    {
        "title": "夏夜饯裴行军赴朝命",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "三年同看锦城花，银烛连宵照绮霞。",
            "报国从来先意气，临岐不用重咨嗟。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 68
    },
    {
        "title": "摩诃池送李侍御之凤翔",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "柳暗花明池上山，高楼歌酒换离颜。",
            "他时欲寄相思字，何处黄云是陇间。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 69
    },
    {
        "title": "送魏正则擢第归江陵",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "商山路接玉山深，古木苍然尽合阴。",
            "会府登筵君最少，江城秋至肯惊心。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 70
    },
    {
        "title": "登阖闾古城",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "登高望远自伤情，柳发花开映古城。",
            "全盛已随流水去，黄鹂空啭旧春声。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 71
    },
    {
        "title": "寻三藏上人",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "北风吹雪暮萧萧，问法寻僧上界遥。",
            "临水手持筇竹杖，逢君不语指芭蕉。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 72
    },
    {
        "title": "山居",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "身依泉壑将时背，路入烟萝得地深。",
            "终岁不知城郭事，手栽林竹尽成阴。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 73
    },
    {
        "title": "长安贼中寄题江南所居茱萸树",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "手种茱萸旧井傍，几回春露又秋霜。",
            "今来独向秦中见，攀折无时不断肠。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 74
    },
    {
        "title": "春斋夜雨忆郭通微",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "桃源在在阻风尘，世事悠悠又遇春。",
            "雨滴闲阶清夜久，焚香偏忆白云人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 75
    },
    {
        "title": "送严秀才",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "灞浐别离肠已断，江山迢递信仍稀。",
            "送君偏下临岐泪，家在南州身未归。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 76
    },
    {
        "title": "春日酬熊执易南亭花发见赠",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "千株桃杏参差发，想见花时人却愁。",
            "曾忝陆机琴酒会，春亭惟愿一淹留。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 77
    },
    {
        "title": "中春亭雪夜寄西邻韩李二舍人",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "广庭飞雪对愁人，寒谷由来不悟春。",
            "却笑山阴乘兴夜，何如今日戴家邻。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 78
    },
    {
        "title": "立秋日与陆华原于县界南馆送邹十八",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "风入昭阳池馆秋，片云孤鹤两难留。",
            "明朝独向青山郭，唯有蝉声催白头。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 79
    },
    {
        "title": "酬韦胄曹登天长寺上方见寄",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "青门几度沾襟泪，并在东林雪外峰。",
            "今日重烦相忆处，春光知绕凤池浓。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 80
    },
    {
        "title": "陌上暮春",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "青青南陌柳如丝，柳色莺声晚日迟。",
            "何处最伤游客思，春风三月落花时。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 81
    },
    {
        "title": "春日偶作",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "飞花寂寂燕双双，南客衡门对楚江。",
            "惆怅管弦何处发，春风吹到读书窗。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 82
    },
    {
        "title": "春暮寄杜嘉兴昆弟",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "柳色千家与万家，轻风细雨落残花。",
            "数枝琼玉无由见，空掩柴扉度岁华。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 83
    },
    {
        "title": "渡淮",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "暮涛凝雪长淮水，细雨飞梅五月天。",
            "行子不须愁夜泊，绿杨多处有人烟。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 84
    },
    {
        "title": "与崔十五同访裴校书不遇",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "梨花落尽柳花时，庭树流莺日过迟。",
            "几度相思不相见，春风何处有佳期。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 85
    },
    {
        "title": "夏日寄陆三达陆四逢并王念八仲周",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "士衡兄弟旧齐名，还似当年在洛城。",
            "闻说重门方隐相，古槐高柳夏阴清。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 86
    },
    {
        "title": "秋原寓目",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "木落风高天宇开，秋原一望思悠哉。",
            "边城今少射雕骑，连雁嗷嗷何处来。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 87
    },
    {
        "title": "赠歌人",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "林莺一哢四时春，蝉翼罗衣白玉人。",
            "曾逐使君歌舞地，清声长啸翠眉颦。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 88
    },
    {
        "title": "同苗郎中送严侍御赴黔中，因访仙源之事",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "武陵源在朗江东，流水飞花仙洞中。",
            "莫问阮郎千古事，绿杨深处翠霞空。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 89
    },
    {
        "title": "使次盘豆驿望永乐县",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "山川不记何年别，城郭应非昔所经。",
            "欲驻征车终日望，天河云雨晦冥冥。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 90
    },
    {
        "title": "缑山道中口号",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "秋山寂寂秋水清，寒郊木叶飞无声。",
            "王子白云仙去久，洛滨行路夜吹笙。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 91
    },
    {
        "title": "岁暮送舍人",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "边城岁暮望乡关，身逐戎旌未得还。",
            "欲别临岐无限泪，故园花发寄君攀。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 92
    },
    {
        "title": "寓兴呈崔员外诸公",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "三月杨花飞满空，飘飖十里雪如风。",
            "不知何处香醪熟，愿醉佳园芳树中。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 93
    },
    {
        "title": "单于晓角",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "胡儿吹角汉城头，月皎霜寒大漠秋。",
            "三奏未终天便晓，何人不起望乡愁。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 94
    },
    {
        "title": "汴河闻笳",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "何处金笳月里悲，悠悠边客梦先知。",
            "单于城下关山曲，今日中原总解吹。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 95
    },
    {
        "title": "塞上春怀",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "东风河外五城喧，南客征袍满泪痕。",
            "愁至独登高处望，蔼然云树重伤魂。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 96
    },
    {
        "title": "塞外月夜寄荆南熊侍御",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "南依刘表北刘琨，征战年年箫鼓喧。",
            "云雨一乖千万里，长城秋月洞庭猿。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 97
    },
    {
        "title": "单于罢战却归题善阳馆",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "单于南去善阳关，身逐归云到处闲。",
            "曾是五年莲府客，每闻胡虏哭阴山。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 98
    },
    {
        "title": "韦常侍以宾客致仕，同诸公题壁",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "孤云永日自徘徊，岩馆苍苍遍绿苔。",
            "望苑忽惊新诏下，彩鸾归处玉笼开。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 99
    },
    {
        "title": "学仙难",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "玉殿笙歌汉帝愁，鸾龙俨驾望瀛洲。",
            "黄金化尽方士死，青天欲上无缘由。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 100
    },
    {
        "title": "唐昌观玉蕊花",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "琪树芊芊玉蕊新，洞宫长闭彩霞春。",
            "日暮落英铺地雪，献花应过九天人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 101
    },
    {
        "title": "春晓闻莺",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "寥寥兰台晓梦惊，绿林残月思孤莺。",
            "犹疑蜀魄千年恨，化作冤禽万啭声。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 102
    },
    {
        "title": "闻严秘书与正字及诸客夜会因寄",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "衡门寥落岁阴穷，露湿莓苔叶厌风。",
            "闻道今宵阮家会，竹林明月七人同。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 103
    },
    {
        "title": "戏赠韩二秀才",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "名高折桂方年少，心苦为文命未通。",
            "闻说东堂今有待，飞鸣何处及春风。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 104
    },
    {
        "title": "闻王仲周所居牡丹花发，因戏赠",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "闻说庭花发暮春，长安才子看须频。",
            "花开花落无人见，借问何人是主人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 105
    },
    {
        "title": "酬王十八见招",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "王昌家直在城东，落尽庭花昨夜风。",
            "高兴不辞千日醉，随君走马向新丰。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 106
    },
    {
        "title": "赠道者",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "麻衣如雪一枝梅，笑掩微妆入梦来。",
            "若到越溪逢越女，红莲池里白莲开。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 107
    },
    {
        "title": "重送白将军",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "红烛芳筵惜夜分，歌楼管咽思难闻。",
            "早知怨别人间世，不下青山老白云。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 108
    },
    {
        "title": "和李丞题李将军林园",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "落英飘蕊雪纷纷，啼鸟如悲霍冠军。",
            "逝水不回弦管绝，玉楼迢递锁浮云。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 109
    },
    {
        "title": "同张惟送霍总",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "春风箫管怨津楼，三奏行人醉不留。",
            "别后相思江上岸，落花飞处杜鹃愁。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 110
    },
    {
        "title": "赠别崔起居",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "三十年前会府同，红颜销尽两成翁。",
            "别泪共将何处洒，锦江南渡足春风。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 111
    },
    {
        "title": "春日偶题",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "山川百战古刀州，龙节来分圣主忧。",
            "静守化条无一事，春风独上望京楼。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 112
    },
    {
        "title": "听歌",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "月上重楼丝管秋，佳人夜唱古梁州。",
            "满堂谁是知音者，不惜千金与莫愁。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 113
    },
    {
        "title": "酬韦胄曹",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "相逢异县蹉跎意，无复少年容易欢。",
            "桃李美人攀折尽，何如松柏四时寒。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 114
    },
    {
        "title": "同幕府夜宴惜花",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "芳草落花明月榭，朝云暮雨锦城春。",
            "莫愁红艳风前散，自有青蛾镜里人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 115
    },
    {
        "title": "代佳人赠张郎中",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "洛阳佳丽本神仙，冰雪颜容桃李年。",
            "心爱阮郎留不住，独将珠泪湿红铅。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 116
    },
    {
        "title": "饯裴行军赴朝命",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "来时圣主假光辉，心恃朝恩计日归。",
            "谁料忽成云雨别，独将边泪洒戎衣。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 117
    },
    {
        "title": "秋日经潼关感寓",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "昔年曾逐汉征东，三授兵符百战中。",
            "力保山河嗟下世，秋风牢落故营空。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 118
    },
    {
        "title": "赠歌人",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "仙歌静转玉箫催，疑是流莺禁苑来。",
            "他日相思梦巫峡，莫教云雨晦阳台。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 119
    },
    {
        "title": "见郭侍郎题壁",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "万里枫江偶问程，青苔壁上故人名。",
            "悠悠身世限南北，一别十年空复情。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 120
    }
]
