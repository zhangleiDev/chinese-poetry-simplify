[
    {
        "title": "梦后吟",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "醉中还有梦，身外已无心。明镜唯知老，青山何处深。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 1
    },
    {
        "title": "题灵山寺",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "觉地本随身，灵山重结因。如何战鸟佛，不化捕鱼人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 2
    },
    {
        "title": "题元阳观旧读书房赠李范",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "此观十年游，此房千里宿。还来旧窗下，更取君书读。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 3
    },
    {
        "title": "永嘉",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "东瓯传旧俗，风日江边好。何处乐神声，夷歌出烟岛。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 4
    },
    {
        "title": "青弋江",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "凄清回泊夜，沦波激石响。村边草市桥，月下罟师网。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 5
    },
    {
        "title": "听山鹧鸪",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "谁家无春酒，何处无春鸟。夜宿桃花村，踏歌接天晓。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 6
    },
    {
        "title": "山径柳",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "宛转若游丝，浅深栽绿崦。年年立春后，即被啼莺占。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 7
    },
    {
        "title": "石上藤",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "空山无鸟迹，何物如人意。委曲结绳文，离披草书字。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 8
    },
    {
        "title": "薜荔庵",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "薜荔作禅庵，重叠庵边树。空山径欲绝，也有人知处。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 9
    },
    {
        "title": "芙蓉榭",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "风摆莲衣干，月背鸟巢寒。文鱼翻乱叶，翠羽上危栏。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 10
    },
    {
        "title": "欹松漪",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "湛湛碧涟漪，老松欹侧卧。悠扬绿萝影，下拂波纹破。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 11
    },
    {
        "title": "焙茶坞",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "新茶已上焙，旧架忧生醭。旋旋续新烟，呼儿劈寒木。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 12
    },
    {
        "title": "弹琴谷",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "谷中谁弹琴，琴响谷冥寂。因君扣商调，草虫惊暗壁。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 13
    },
    {
        "title": "白鹭汀",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "靃靡汀草碧，淋森鹭毛白。夜起沙月中，思量捕鱼策。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 14
    },
    {
        "title": "千松岭",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "终日吟天风，有时天籁止。问渠何旨意，恐落凡人耳。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 15
    },
    {
        "title": "黄菊湾",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "时菊凝晓露，露华滴秋湾。仙人酿酒熟，醉里飞空山。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 16
    },
    {
        "title": "临平湖",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "采藕平湖上，藕泥封藕节。船影入荷香，莫冲莲柄折。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 17
    },
    {
        "title": "山春洞",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "引烛踏仙泥，时时乱乳燕。不知何道士，手把灵书卷。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 18
    },
    {
        "title": "石窦泉",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "吹沙复喷石，曲折仍圆旋。野客漱流时，杯粘落花片。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 19
    },
    {
        "title": "古仙坛",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "远山谁放烧，疑是坛边醮。仙人错下山，拍手坛边笑。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 20
    },
    {
        "title": "题山顶寺",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "遥闻林下语，知是经行所。日暮香风时，诸天散花雨。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 21
    },
    {
        "title": "天宝题壁",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "五十余年别，伶俜道不行。却来书处在，惆怅似前生。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 22
    },
    {
        "title": "哭李别驾",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "故人行迹灭，秋草向南悲。不欲频回步，孀妻正哭时。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 23
    },
    {
        "title": "春雨不闻百舌",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "百舌春来哑，愁人共待晴。不关秋水事，饮恨亦无声。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 24
    },
    {
        "title": "忆鄱阳旧游",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "悠悠南国思，夜向江南泊。楚客断肠时，月明枫子落。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 25
    },
    {
        "title": "春怀",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "园莺啼已倦，树树陨香红。不是春相背，当由己自翁。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 26
    },
    {
        "title": "洛阳陌二首",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "莺声满御堤，堤柳拂丝齐。风送名花落，香红衬马蹄。",
            "珂佩逐鸣驺，王孙结伴游。金丸落飞鸟，乘兴醉青楼。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 27
    },
    {
        "title": "寄淮上柳十三",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "苇萧中辟户，相映绿淮流。莫讶春潮阔，鸥边可泊舟。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 28
    },
    {
        "title": "送李泌",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "昔别吴堤雨，春帆去较迟。江波千里绿，□□□□□。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 29
    },
    {
        "title": "山中夜宿",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "凉月挂层峰，萝林落叶重。掩关深畏虎，风起撼长松。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 30
    },
    {
        "title": "登楼",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "高阁成长望，江流雁叫哀。凄凉故吴事，麋鹿走荒台。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 31
    },
    {
        "title": "江上",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "江清白鸟斜，荡桨罥苹花。听唱菱歌晚，回塘月照沙。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 32
    },
    {
        "title": "溪上",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "采莲溪上女，舟小怯摇风。惊起鸳鸯宿，水云撩乱红。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 33
    },
    {
        "title": "田家",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "带水摘禾穗，夜捣具晨炊。县帖取社长，嗔怪见官迟。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 34
    },
    {
        "title": "宿山中僧",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "不𦶟香炉烟，蒲团坐如铁。尝想同夜禅，风堕松顶雪。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 35
    },
    {
        "title": "梅湾",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "白石盘盘磴，清香树树梅。山深不吟赏，辜负委苍苔。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 36
    },
    {
        "title": "思归",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "不能经纶大经，甘作草莽闲臣。",
            "青琐应须长别，白云漫与相亲。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 37
    },
    {
        "title": "归山作",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "心事数茎白发，生涯一片青山。",
            "空林有雪相待，古道无人独还。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 38
    },
    {
        "title": "过山农家",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "板桥人渡泉声，茅簷日午鸡鸣。",
            "莫嗔焙茶烟暗，却喜晒谷天晴。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 39
    },
    {
        "title": "代佳人赠别",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "万里行人欲渡溪，千行珠泪滴为泥。",
            "已成残梦随君去，犹有惊乌半夜啼。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 40
    },
    {
        "title": "忆故园",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "惆怅多山人复稀，杜鹃啼处泪沾衣。",
            "故园此去千余里，春梦犹能夜夜归。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 41
    },
    {
        "title": "题叶道士山房",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "水边垂柳赤栏桥，洞里仙人碧玉箫。",
            "近得麻姑音信否，浔阳江上不通潮。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 42
    },
    {
        "title": "送李秀才入京",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "五湖秋叶满行船，八月灵槎欲上天。",
            "君向长安余适越，独登秦望望秦川。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 43
    },
    {
        "title": "越中席上看弄老人",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "不到山阴十二春，镜中相见白头新。",
            "此生不复为年少，今日从他弄老人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 44
    },
    {
        "title": "听刘安唱歌",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "子夜新声何处传，悲翁更忆太平年。",
            "即今法曲无人唱，已逐霓裳飞上天。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 45
    },
    {
        "title": "樱桃曲",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "百舌犹来上苑花，游人独自忆京华。",
            "遥知寝庙尝新后，敕赐樱桃向几家。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 46
    },
    {
        "title": "山中",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "野人爱向山中宿，况在葛洪丹井西。",
            "庭前有个长松树，夜半子规来上啼。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 47
    },
    {
        "title": "赠朱放",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "野客归时无四邻，黔娄别久案常贫。",
            "渔樵旧路不堪入，何处空山犹有人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 48
    },
    {
        "title": "江村乱后",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "江村日暮寻遗老，江水东流横浩浩。",
            "竹里闲窗不见人，门前旧路生青草。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 49
    },
    {
        "title": "望简寂观",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "青嶂青溪直复斜，白鸡白犬到人家。",
            "仙人住在最高处，向晚春泉流白花。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 50
    },
    {
        "title": "五两歌送张夏",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "竿头五两风褭褭，水上云帆逐飞鸟。",
            "送君初出扬州时，霭霭曈曈江溢晓。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 51
    },
    {
        "title": "临海所居三首",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "此是昔年征战处，曾经永日绝人行。",
            "千家寂寂对流水，唯有汀洲春草生。",
            "此去临溪不是遥，楼中望见赤城标。",
            "不知叠嶂重霞里，更有何人度石桥。",
            "家在双峰兰若边，一声秋磬发孤烟。",
            "山连极浦鸟飞尽，月上青林人未眠。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 52
    },
    {
        "title": "听角思归",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "故园黄叶满青苔，梦后城头晓角哀。",
            "此夜断肠人不见，起行残日影徘徊。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 53
    },
    {
        "title": "酬柳相公",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "天下如今已太平，相公何事唤狂生。",
            "个身恰似笼中鹤，东望沧溟叫数声。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 54
    },
    {
        "title": "题明霞台",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "野人本自不求名，欲向山中过一生。",
            "莫嫌憔悴无知己，别有烟霞似弟兄。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 55
    },
    {
        "title": "哭绚法师",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "楚客停桡欲问谁，白沙江草麹尘丝。",
            "生公手种殿前树，唯有花开𫛸鴂悲。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 56
    },
    {
        "title": "送柳宜城葬",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "鸣笳已逐春风咽，匹马犹依旧路嘶。",
            "遥望柳家门外树，恐闻黄鸟向人啼。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 57
    },
    {
        "title": "宫词五首",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "禁柳烟中闻晓乌，风吹玉漏尽铜壶。",
            "内官先向蓬莱殿，金合开香泻御炉。",
            "玉楼天半起笙歌，风送宫嫔笑语和。",
            "月殿影开闻夜漏，水精帘卷近银河。",
            "玉阶容卫宿千官，风猎青旗晓仗寒。",
            "侍女先来荐琼蕊，露浆新下九霄盘。",
            "九重天乐降神仙，步舞分行踏锦筵。",
            "嘈囋一声钟鼓歇，万人楼下拾金钱。",
            "金吾持戟护新簷，天乐声传万姓瞻。",
            "楼上美人相倚看，红妆透出水精帘。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 58
    },
    {
        "title": "寻桃花岭潘三姑台",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "桃花岭上觉天低，人上青山马隔溪。",
            "行到三姑学仙处，还如刘阮二郎迷。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 59
    },
    {
        "title": "夜中望仙观",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "日暮衔花飞鸟还，月明溪上见青山。",
            "遥知玉女窗前树，不是仙人不得攀。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 60
    },
    {
        "title": "送李侍御往吴兴",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "世间只有情难说，今夜应无不醉人。",
            "若向洞庭山下过，暗知浇沥圣姑神。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 61
    },
    {
        "title": "奉和韩晋公晦日呈诸判官",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "江南无处不闻歌，晦日中军乐更多。",
            "不是风光催柳色，却缘威令动阳和。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 62
    },
    {
        "title": "子规",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "杜宇冤亡积有时，年年啼血动人悲。",
            "若教恨魄皆能化，何树何山著子规。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 63
    },
    {
        "title": "海鸥咏",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "万里飞来为客鸟，曾蒙丹凤借枝柯。",
            "一朝凤去梧桐死，满目鸱鸢奈尔何。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 64
    },
    {
        "title": "赠韦清将军",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "身执金吾主禁兵，腰间宝剑重横行。",
            "接舆亦是狂歌者，更就将军乞一声。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 65
    },
    {
        "title": "赠僧二首",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "家住义兴东舍溪，溪边莎草雨无泥。",
            "上人一向心入定，春鸟年年空自啼。",
            "出头皆是新年少，何处能容老病翁。",
            "更把浮荣喻生灭，世间无事不虚空。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 66
    },
    {
        "title": "登楼望水",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "鸟啼花发柳含烟，掷却风光忆少年。",
            "更上高楼望江水，故乡何处一归船。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 67
    },
    {
        "title": "湖中",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "青草湖边日色低，黄茅嶂里鹧鸪啼。",
            "丈夫飘荡今如此，一曲长歌楚水西。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 68
    },
    {
        "title": "岁日作",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "不觉老将春共至，更悲携手几人全。",
            "还丹寂寞羞明镜，手把屠苏让少年。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 69
    },
    {
        "title": "山中赠客",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "山中好处无人别，涧梅伪作山中雪。",
            "野客相逢夜不眠，山中童子烧松节。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 70
    },
    {
        "title": "王郎中妓席五咏：箜篌",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "玉作搔头金步摇，高张苦调响连宵。",
            "欲知写尽相思梦，度水寻云不用桥。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 71
    },
    {
        "title": "王郎中妓席五咏：舞",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "汗浥新装画不成，丝催急节舞衣轻。",
            "落花绕树疑无影，回雪从风暗有情。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 72
    },
    {
        "title": "王郎中妓席五咏：歌",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "柳拂青楼花满衣，能歌宛转世应稀。",
            "空中几处闻清响，欲绕行云不遣飞。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 73
    },
    {
        "title": "王郎中妓席五咏：筝",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "秦声楚调怨无穷，陇水胡笳咽复通。",
            "莫遣黄莺花里啭，参差撩乱妒春风。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 74
    },
    {
        "title": "王郎中妓席五咏：笙",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "欲写人间离别心，须听鸣凤似龙吟。",
            "江南曲尽归何处，洞水山云知浅深。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 75
    },
    {
        "title": "送李山人还玉溪",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "好鸟共鸣临水树，幽人独欠买山钱。",
            "若为种得千竿竹，引取君家一眼泉。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 76
    },
    {
        "title": "送少微上人还鹿门",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "少微不向吴中隐，为个生缘在鹿门。",
            "行入汉江秋月色，襄阳耆旧几人存。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 77
    },
    {
        "title": "宿昭应",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "武帝祈灵太乙坛，新丰树色绕千官。",
            "那知今夜长生殿，独闭山门月影寒。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 78
    },
    {
        "title": "题琅邪上方",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "东晋王家在此溪，南朝树色隔窗低。",
            "碑沈字灭昔人远，谷鸟犹向寒花啼。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 79
    },
    {
        "title": "安仁港口望仙人城",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "楼台采翠远分明，闻说仙家在此城。",
            "欲上仙城无路上，水边花里有人声。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 80
    },
    {
        "title": "寄秘书包监",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "一别长安路几千，遥知旧日主人怜。",
            "贾生只是三年谪，独自无才已四年。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 81
    },
    {
        "title": "小孤山",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "古庙枫林江水边，寒鸦接饭雁横天。",
            "大孤山远小孤出，月照洞庭归客船。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 82
    },
    {
        "title": "送李秀才游嵩山",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "嵩山石壁挂飞流，无限神仙在上头。",
            "采得新诗题石壁，老人惆怅不同游。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 83
    },
    {
        "title": "从剡溪至赤城",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "灵溪宿处接灵山，窈映高楼向月闲。",
            "夜半鹤声残梦里，犹疑琴曲洞房间。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 84
    },
    {
        "title": "叶上题诗从苑中流出",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "花落深宫莺亦悲，上阳宫女断肠时。",
            "君恩不闭东流水，叶上题诗寄与谁。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 85
    },
    {
        "title": "崦里桃花",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "崦里桃花逢女冠，林间杏叶落仙坛。",
            "老人方授上清箓，夜听步虚山月寒。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 86
    },
    {
        "title": "听子规",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "栖霞山中子规鸟，口边血出啼不了。",
            "山僧后夜初出定，闻似不闻山月晓。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 87
    },
    {
        "title": "竹枝曲",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "帝子苍梧不复归，洞庭叶下荆云飞。",
            "巴人夜唱竹枝后，肠断晓猿声渐稀。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 88
    },
    {
        "title": "寻僧二首",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "方丈玲珑花竹闲，已将心印出人间。",
            "家家门外长安道，何处相逢是宝山。",
            "弥天释子本高情，往往山中独自行。",
            "莫怪狂人游楚国，莲花只在淤泥生。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 89
    },
    {
        "title": "桃花曲",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "魏帝宫人舞凤楼，隋家天子泛龙舟。",
            "君王夜醉春眠晏，不觉桃花逐水流。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 90
    },
    {
        "title": "赠远",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "暂出河边思远道，却来窗下听新莺。",
            "故人一别几时见，春草还从旧处生。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 91
    },
    {
        "title": "早春思归有唱竹枝歌者坐中下泪",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "渺渺春生楚水波，楚人齐唱竹枝歌。",
            "与君皆是思归客，拭泪看花奈老何。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 92
    },
    {
        "title": "送郭秀才",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "故人曾任丹徒令，买得青山拟独耕。",
            "不作草堂招远客，却将垂柳借啼莺。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 93
    },
    {
        "title": "宫词",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "长乐宫连上苑春，玉楼金殿艳歌新。",
            "君门一入无由出，唯有宫莺得见人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 94
    },
    {
        "title": "悼稚",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "稚子比来骑竹马，犹疑只在屋东西。",
            "莫言道者无悲事，曾听巴猿向月啼。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 95
    },
    {
        "title": "山僧兰若",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "绝顶茅庵老此生，寒云孤木伴经行。",
            "世人那得知幽径，遥向青峰礼磬声。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 96
    },
    {
        "title": "句",
        "author": "顾况",
        "biography": "",
        "paragraphs": [
            "崦合桃花水，窗鸣柳谷泉。",
            "颓垣化为陂，陆地堪乘舟。",
            "汀洲渺渺江篱短，疑是疑非两断肠。",
            "巫峡朝云暮不归，洞庭春水晴空满。",
            "龙吟四泽欲兴雨，凤引九雏警宿乌。",
            "新妇矶边月明，女儿浦口潮平。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十七",
        "no#": 97
    }
]