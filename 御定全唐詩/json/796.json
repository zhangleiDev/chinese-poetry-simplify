[
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "都来消帝力，全不用兵防。"
        ],
        "notes": [
            "（齐己《风骚旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 1
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "道远擎空钵，深山踏落花。"
        ],
        "notes": [
            "（齐己《风骚旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 2
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "船中江上景，晚泊早行时。"
        ],
        "notes": [
            "（齐己《风骚旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 3
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "山寺钟楼月，江城鼓角风。"
        ],
        "notes": [
            "（齐己《风骚旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 4
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "雨过花争出，云空月半生。"
        ],
        "notes": [
            "（齐己《风骚旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 5
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "鸟正啼隋柳，人须入楚山。"
        ],
        "notes": [
            "（齐己《风骚旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 6
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "高松飘雨雪，一室掩香灯。"
        ],
        "notes": [
            "（齐己《风骚旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 7
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "遂有冥心者，还寻此境来。"
        ],
        "notes": [
            "（齐己《风骚旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 8
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "又因风雨夜，重到古松门。"
        ],
        "notes": [
            "（齐己《风骚旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 9
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "道晦金鸡伏，时来木马鸣。"
        ],
        "notes": [
            "（齐己《风骚旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 10
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "谁来看山寺，自去扫松门。"
        ],
        "notes": [
            "（齐己《风骚旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 11
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "此生还自喜，余事不相侵。"
        ],
        "notes": [
            "（齐己《风骚旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 12
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "白鬓无心镊，青山得意多。"
        ],
        "notes": [
            "（齐己《风骚旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 13
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "一春能几日，无雨亦多风。"
        ],
        "notes": [
            "（齐己《风骚旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 14
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "苦雨秋涛涨，狂风野烧翻。"
        ],
        "notes": [
            "（齐己《风骚旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 15
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "须知三尺剑，只为不平磨。"
        ],
        "notes": [
            "（齐己《风骚旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 16
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "要路争先进，闲门肯暂过。"
        ],
        "notes": [
            "（齐己《风骚旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 17
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "已难消永夜，况复听秋霖。"
        ],
        "notes": [
            "（齐己《风骚旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 18
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "大雪路亦宿，深山水也吞。"
        ],
        "notes": [
            "（赠僧，齐己《风骚旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 19
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "卷帘黄叶落，锁印子规啼。"
        ],
        "notes": [
            "（齐己《风骚旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 20
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "亡国空流水，孤祠掩落花。"
        ],
        "notes": [
            "（齐己《风骚旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 21
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "山魅隔窗舞，𫛳鸟入帘飞。"
        ],
        "notes": [
            "（齐己《风骚旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 22
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "万里八九月，一身西北风。"
        ],
        "notes": [
            "（齐己《风骚旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 23
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "磴危攀薜荔，石滑疏莓苔。"
        ],
        "notes": [
            "（齐己《风骚旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 24
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "日落无行客，天寒有去鸿。"
        ],
        "notes": [
            "（齐己《风骚旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 25
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "正思浮世事，又到古城边。"
        ],
        "notes": [
            "（齐己《风骚旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 26
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "寻常风雨夜，应有鬼神看。"
        ],
        "notes": [
            "（齐己《风骚旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 27
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "寄宿山中寺，相辞海上僧。"
        ],
        "notes": [
            "（齐己《风骚旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 28
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "风吹榆荚叶，雨打木瓜花。"
        ],
        "notes": [
            "（炙毂子《诗格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 29
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "是星皆拱北，无水不朝东。"
        ],
        "notes": [
            "（梅尧臣《续金针诗格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 30
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "西风起边雁，一一向潇湘。"
        ],
        "notes": [
            "（梅尧臣《续金针诗格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 31
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "那堪怀远路，犹自上高楼。"
        ],
        "notes": [
            "（梅尧臣《续金针诗格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 32
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "白云风散尽，红叶水流来。"
        ],
        "notes": [
            "（徐寅《雅道机要》）"
        ],
        "volume": "卷七百九十六",
        "no#": 33
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "妾如无异意，君弃也甘心。"
        ],
        "notes": [
            "（徐寅《雅道机要》）"
        ],
        "volume": "卷七百九十六",
        "no#": 34
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "野过云根阔，山高树影长。"
        ],
        "notes": [
            "（徐寅《雅道机要》）"
        ],
        "volume": "卷七百九十六",
        "no#": 35
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "古迹应重别，生缘不暂停。"
        ],
        "notes": [
            "（徐寅《雅道机要》）"
        ],
        "volume": "卷七百九十六",
        "no#": 36
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "一个字未稳，数宵心不闲。"
        ],
        "notes": [
            "（徐寅《雅道机要》）"
        ],
        "volume": "卷七百九十六",
        "no#": 37
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "别地叶频落，去程山已寒。"
        ],
        "notes": [
            "（徐寅《雅道机要》）"
        ],
        "volume": "卷七百九十六",
        "no#": 38
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "但取诗名得，何论下第频。"
        ],
        "notes": [
            "（徐寅《雅道机要》）"
        ],
        "volume": "卷七百九十六",
        "no#": 39
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "早起赴前程，邻鸡尚未鸣。"
        ],
        "notes": [
            "（文彧《诗格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 40
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "白地一回雨，儿孙拾得金。"
        ],
        "notes": [
            "（文彧《诗格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 41
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "轩车在何处，雨雪满前山。"
        ],
        "notes": [
            "（文彧《诗格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 42
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "灯微山馆雨，角咽海城秋。"
        ],
        "notes": [
            "（文彧《诗格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 43
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "去帆看已远，临水立多时。"
        ],
        "notes": [
            "（《桂林诗评》）"
        ],
        "volume": "卷七百九十六",
        "no#": 44
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "家贫为客早，路远得书稀。"
        ],
        "notes": [
            "（《桂林诗评》）"
        ],
        "volume": "卷七百九十六",
        "no#": 45
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "瀑布五千仞，草堂瀑布边。"
        ],
        "notes": [
            "（《桂林诗评》）"
        ],
        "volume": "卷七百九十六",
        "no#": 46
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "我忆云门寺，门前千万峰。"
        ],
        "notes": [
            "（《桂林诗评》）"
        ],
        "volume": "卷七百九十六",
        "no#": 47
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "一瓶居无外，万木老禅中。"
        ],
        "notes": [
            "（《桂林诗评》）"
        ],
        "volume": "卷七百九十六",
        "no#": 48
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "秋江洗一钵，寒日晒三衣。"
        ],
        "notes": [
            "（《桂林诗评》）"
        ],
        "volume": "卷七百九十六",
        "no#": 49
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "空山行客少，独树晚蝉多。"
        ],
        "notes": [
            "（《桂林诗评》）"
        ],
        "volume": "卷七百九十六",
        "no#": 50
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "一片月生海，几家人上楼。"
        ],
        "notes": [
            "（待月，王正字《诗中旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 51
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "窗寒风渐紧，灯落夜将深。"
        ],
        "notes": [
            "（秋日言怀，王正字诗中旨格）"
        ],
        "volume": "卷七百九十六",
        "no#": 52
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "云梦几行去，潇湘一夜空。"
        ],
        "notes": [
            "（送雁，王正字《诗中旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 53
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "无风来竹院，有月在莎庭。"
        ],
        "notes": [
            "（蟋蟀，王正字《诗中旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 54
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "故园从小别，夜雨近秋闻。"
        ],
        "notes": [
            "（王正字《诗中旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 55
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "书剑别南州，炎荒万里游。"
        ],
        "notes": [
            "（王正字《诗中旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 56
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "山盘樵径上，人到雪房迟。"
        ],
        "notes": [
            "（王正字《诗中旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 57
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "路经秋雨后，人入乱峰来。"
        ],
        "notes": [
            "（王正字《诗中旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 58
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "独树知秋早，孤舟觉夜寒。"
        ],
        "notes": [
            "（王正字《诗中旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 59
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "马饥餐落叶，鹤宿晒残阳。"
        ],
        "notes": [
            "（王正字《诗中旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 60
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "路远春行尽，家贫愁到时。"
        ],
        "notes": [
            "（《吟窗杂录》）"
        ],
        "volume": "卷七百九十六",
        "no#": 61
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "临水通宵坐，知君此兴同。"
        ],
        "notes": [
            "（《吟窗杂录》）"
        ],
        "volume": "卷七百九十六",
        "no#": 62
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "江声兼小雨，暝色入啼猿。"
        ],
        "notes": [
            "（《树萱录》）"
        ],
        "volume": "卷七百九十六",
        "no#": 63
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "藕隐玲珑玉，花藏缥缈容。"
        ],
        "notes": [
            "（《树萱录》）"
        ],
        "volume": "卷七百九十六",
        "no#": 64
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "红树醉秋色，碧溪弹夜弦。"
        ],
        "notes": [
            "（《树萱录》）"
        ],
        "volume": "卷七百九十六",
        "no#": 65
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "网断蛛犹织，梁空燕不归。"
        ],
        "notes": [
            "（《树萱录》）"
        ],
        "volume": "卷七百九十六",
        "no#": 66
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "倒身无著处，呵手不成温。"
        ],
        "notes": [
            "（《苦寒》见《事文类聚》）"
        ],
        "volume": "卷七百九十六",
        "no#": 67
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "几多诗弟子，无限酒知闻。"
        ],
        "notes": [
            "（《悼方干》见《海录碎事》）"
        ],
        "volume": "卷七百九十六",
        "no#": 68
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "床上小熏笼，韶州新退红。"
        ],
        "notes": [
            "（《乐府》见陆务观集注）"
        ],
        "volume": "卷七百九十六",
        "no#": 69
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "看时人步涩，展处蝶争来。"
        ],
        "notes": [
            "（《题于兢画牡丹》，"
        ],
        "volume": "卷七百九十六",
        "no#": 70
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "兢字德源，长安人，梁宰相也。"
        ],
        "notes": [
            "见《见闻志》）"
        ],
        "volume": "卷七百九十六",
        "no#": 71
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "酒味杂莲气，香冷胜于冰。"
        ],
        "notes": [
            "轮囷如象鼻，潇洒绝青蝇。（《碧筩杯》，见杨慎《艺林伐山》）"
        ],
        "volume": "卷七百九十六",
        "no#": 72
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "太宗皇帝真长策，赚得英雄尽白头。"
        ],
        "notes": [
            "（《国史补》曰：进士科得之艰难，其有老死于文场者，亦无所恨，故诗云。《画墁录》以为赵嘏作）"
        ],
        "volume": "卷七百九十六",
        "no#": 73
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "曾题名处添前字，送出城人乞旧衣。"
        ],
        "notes": [
            "（神龙已来，杏园宴后，皆于慈恩塔下题名，及第后知闻。或遇未及第时题名处，则为添前字，故诗云。见《摭言》）"
        ],
        "volume": "卷七百九十六",
        "no#": 74
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "华阳观里钟声集，建福门前鼓动期。"
        ],
        "notes": [
            "（新进士放榜后翌日，排光范门候过宰相。虽云排建福门，实集于西方馆，故诗云云。见《南部新书》）"
        ],
        "volume": "卷七百九十六",
        "no#": 75
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "陇右诸侯供语鸟，日南太守送名花。"
        ],
        "notes": [
            "（李德裕营平泉，远方之人，多以异物奉之，时有题诗云云。见《剧谈录》）"
        ],
        "volume": "卷七百九十六",
        "no#": 76
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "八百孤寒齐下泪，一时回首望崖州。"
        ],
        "notes": [
            "（德裕为相，颇为寒进开路。及谪官南去，人咏此。见《摭言》）"
        ],
        "volume": "卷七百九十六",
        "no#": 77
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "让劫已令多士伏，沽名还得世人闻。"
        ],
        "notes": [
            "（陈咏有诗名，善弈棋。昭宗幸陕之岁，策名归蜀，韦庄以诗贺之。乡中有嫉善者，属和韦诗云云，比之涤器当垆也。见《纪事》）"
        ],
        "volume": "卷七百九十六",
        "no#": 78
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "人为子推初禁火，花愁青女再飞霜。"
        ],
        "notes": [
            "（《寒食》，见《事文类聚》）"
        ],
        "volume": "卷七百九十六",
        "no#": 79
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "楝花开后风光好，梅子黄时雨意浓。"
        ],
        "notes": [
            " （江南自初春至初夏，有二十四番花信风，最后为楝花风，故唐人有此句。见《东皋杂录》）"
        ],
        "volume": "卷七百九十六",
        "no#": 80
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "兰汤备浴传荆俗，水马浮江吊屈魂。"
        ],
        "notes": [
            "（唐人《午日》诗。南方竞渡，治其舟使轻利，名飞凫，名水车，又名水马。见《荆楚岁时记》）"
        ],
        "volume": "卷七百九十六",
        "no#": 81
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "城头椎鼓传花枝，席上抟拳握松子。"
        ],
        "notes": [
            "（见《东皋杂录》）"
        ],
        "volume": "卷七百九十六",
        "no#": 82
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "鸟向不香花里宿，人从无影月中归。"
        ],
        "notes": [
            "（《雪》）"
        ],
        "volume": "卷七百九十六",
        "no#": 83
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "轻著冻痕销水面，密随和气入花根。"
        ],
        "notes": [
            "（《春雪》）"
        ],
        "volume": "卷七百九十六",
        "no#": 84
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "万木尽如花落候，四簷鸣□雨来时。"
        ],
        "notes": [
            "（《晴雪》，王安石尝举此三联，云唐人诗）"
        ],
        "volume": "卷七百九十六",
        "no#": 85
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "浓绿万枝红一点，动人春色不须多。"
        ],
        "notes": [
            "（王安石在翰苑，见榴花止开一朵，有诗。陈正敏谓此乃唐人诗，非安石所作。见《泊宅编》）"
        ],
        "volume": "卷七百九十六",
        "no#": 86
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "晚菘细切肥牛肚，新笋初尝嫩马蹄。"
        ],
        "notes": [
            "（见《锦绣万花谷》）"
        ],
        "volume": "卷七百九十六",
        "no#": 87
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "蝉离楚树鸣犹少，叶到嵩山落更多。"
        ],
        "notes": [
            "（《风骚旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 88
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "风和日暖方开眼，雨润烟浓不举头。"
        ],
        "notes": [
            "（《风骚旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 89
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "窗前闲咏鸳鸯句，壁上时观獬廌图。"
        ],
        "notes": [
            "（《风骚旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 90
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "可能有事关心后，得似无人识面时。"
        ],
        "notes": [
            "（《风骚旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 91
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "一点孤灯人梦觉，万重寒叶雨声多。"
        ],
        "notes": [
            "（炙毂子《诗格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 92
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "此心只待相逢说，时复登楼看远山。"
        ],
        "notes": [
            "（炙毂子《诗格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 93
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "九江有浪船难济，三峡无猿客有愁。"
        ],
        "notes": [
            "（炙毂子《诗格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 94
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "四海鱼龙精魄冷，五山鸾凤骨毛寒。"
        ],
        "notes": [
            "（炙毂子《诗格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 95
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "谁家绿酒欢留客，何处红楼睡失明。"
        ],
        "notes": [
            "（炙毂子《诗格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 96
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "雨露施恩无厚薄，蓬蒿随分有枯荣。"
        ],
        "notes": [
            "（梅尧臣《续金针诗格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 97
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "去年花下留连饮，暖日夭桃莺乱啼。"
        ],
        "notes": [
            "（梅尧臣《续金针诗格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 98
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "今日江边容易别，淡烟衰草马频嘶。"
        ],
        "notes": [
            "（梅尧臣《续金针诗格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 99
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "蛇蝎性灵生便毒，蕙兰根异死犹香。"
        ],
        "notes": [
            "（梅尧臣《续金针诗格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 100
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "三间茅屋无人到，十里松门独自游。"
        ],
        "notes": [
            "（梅尧臣《续金针诗格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 101
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "翻忆旧山青碧里，绕庵闲伴野僧禅。"
        ],
        "notes": [
            "（《云》）"
        ],
        "volume": "卷七百九十六",
        "no#": 102
    },
    {
        "title": "句",
        "author": "无名氏",
        "biography": "",
        "paragraphs": [
            "不甘五等诸侯荐，直肯九重天子知。"
        ],
        "notes": [
            "（刘素者，字仲华。好学，不事科举，颇通迁、固、寿、晔之书，尝有人贻诗云云，然卒不及仕。见《江南野志》）"
        ],
        "volume": "卷七百九十六",
        "no#": 103
    }
]