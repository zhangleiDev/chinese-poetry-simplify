[
    {
        "title": "吴：黄金车",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "分擘山河即渐开，许昌基业已倾颓。",
            "黄金车与斑斓耳，早个须知入谶来。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 1
    },
    {
        "title": "吴：赤壁",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "会猎书来举国惊，只应周鲁不教迎。",
            "曹公一战奔波后，赤壁功传万古名。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 2
    },
    {
        "title": "吴：鲁肃指囷",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "破产移家事亦难，佐吴从此霸江山。",
            "争教不立功勋得，指出千囷如等闲。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 3
    },
    {
        "title": "吴：甘宁斫营",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "夜深偷入魏军营，满寨惊忙火似星。",
            "百口宝刀千匹绢，也应消得与甘宁。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 4
    },
    {
        "title": "吴：徐盛",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "欲把江山鼎足分，邢真衔册到江南。",
            "当时将相谁堪重，徐盛将军最不甘。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 5
    },
    {
        "title": "吴：鲁肃",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "斫案兴言断众疑，鼎分从此定雄雌。",
            "若无子敬心相似，争得乌林破魏师。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 6
    },
    {
        "title": "吴：武昌",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "西塞山高截九垓，谶谣终日自相催。",
            "武昌鱼美应难恋，历数须归建业来。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 7
    },
    {
        "title": "吴：顾雍",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "赞国经纶更有谁，蔡公相叹亦相师。",
            "贵为丞相封侯了，归后家人总不知。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 8
    },
    {
        "title": "吴：吕蒙",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "幼小家贫实可哀，愿征行去志难回。",
            "不探虎穴求身达，争得人间富贵来。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 9
    },
    {
        "title": "吴：介象",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "好道君王遇亦难，变通灵异几多般。",
            "介先生有神仙术，钓得鲈鱼在玉盘。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 10
    },
    {
        "title": "吴：濡须坞",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "风揭洪涛响若雷，枕波为垒险相隈。",
            "莫言有个濡须坞，几度曹公失志回。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 11
    },
    {
        "title": "吴：周泰",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "名与诸公又不同，金疮痕在满身中。",
            "不将御盖宣恩泽，谁信将军别有功。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 12
    },
    {
        "title": "吴：张纮",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "东部张公与众殊，共施经略赞全吴。",
            "陈琳漫自称雄伯，神气应须怯大巫。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 13
    },
    {
        "title": "吴：太史慈",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "圣德招贤远近知，曹公心计却成欺。",
            "陈韩昔日尝投楚，岂是当归召得伊。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 14
    },
    {
        "title": "吴：孙坚后",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "委存张公翊圣材，几将贤德赞文台。",
            "争教不霸江山得，日月征曾入梦来。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 15
    },
    {
        "title": "吴：陆统",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "将军身殁有儿孤，虎子为名教读书。",
            "更向宫中教骑马，感君恩重合何如。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 16
    },
    {
        "title": "吴：青盖",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "历数将终势已摧，不修君德更堪哀。",
            "被他青盖言相误，元是须教入晋来。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 17
    },
    {
        "title": "晋：七宝鞭",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "天命须知岂偶然，乱臣徒欲用兵权。",
            "圣谟庙略还应别，浑不消他七宝鞭。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 18
    },
    {
        "title": "晋：庾悦鹅炙",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "春暖江南景气新，子鹅炙美就中珍。",
            "庾家厨盛刘公困，浑弗相贻也恼人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 19
    },
    {
        "title": "晋：谢玄",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "百万兵来逼合肥，谢玄为将统雄师。",
            "旌旗首尾千余里，浑不消他一局棋。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 20
    },
    {
        "title": "晋：谢混",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "尚主当初偶未成，此时谁合更关情。",
            "可怜谢混风华在，千古翻传禁脔名。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 21
    },
    {
        "title": "晋：陆玩",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "陆公高论亦由衷，谦让还惭未有功。",
            "天下忠良人欲尽，始应交我作三公。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 22
    },
    {
        "title": "晋：王坦之",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "晋祚安危只此行，坦之何必苦忧惊。",
            "谢公合定寰区在，争遣当时事得成。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 23
    },
    {
        "title": "晋：蒲葵扇",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "抛舍东山岁月遥，几施经略挫雄豪。",
            "若非名德喧寰宇，争得蒲葵价数高。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 24
    },
    {
        "title": "晋：王郎",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "太尉门庭亦甚高，王郎名重礼相饶。",
            "自家妻父犹如此，谁更逢君得折腰。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 25
    },
    {
        "title": "晋：刘毅",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "绕床堪壮喝卢声，似铁容仪众尽惊。",
            "二十七人同举义，几人全得旧功名。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 26
    },
    {
        "title": "晋：王恭",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "春风濯濯柳容仪，鹤氅神情举世推。",
            "可惜教君仗旄钺，枉将心地托牢之。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 27
    },
    {
        "title": "晋：谢公赌墅",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "发遣将军欲去时，略无情挠只贪棋。",
            "自从乞与羊昙后，赌墅功成更有谁。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 28
    },
    {
        "title": "晋：苻坚投棰",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "投棰填江语未终，谢安乘此立殊功。",
            "三台星烂乾坤在，且与张华死不同。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 29
    },
    {
        "title": "晋：卫玠",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "叔宝羊车海内稀，山家女婿好风姿。",
            "江东士女无端甚，看杀玉人浑不知。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 30
    },
    {
        "title": "晋：郭璞脱襦",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "吟坐因思郭景纯，每言穷达似通神。",
            "到头分命难移改，解脱青襦与别人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 31
    },
    {
        "title": "晋：庾楼",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "江州楼上月明中，从事同登眺远空。",
            "玉树忽薶千载后，有谁重此继清风。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 32
    },
    {
        "title": "晋：新亭",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "容易乘虚逼帝畿，满江艛橹与旌旗。",
            "卢循若解新亭上，胜负还应未可知。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 33
    },
    {
        "title": "宋：大岘",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "大岘才过喜可知，指空言已副心期。",
            "公孙计策嗟无用，天与南朝作霸基。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 34
    },
    {
        "title": "宋：放宫人",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "纳谏廷臣免犯颜，自然恩可霸江山。",
            "姚兴侍女方承宠，放出宫闱若等闲。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 35
    },
    {
        "title": "宋：借南苑",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "人主词应不偶然，几人曾说笑掀天。",
            "不知南苑今何在，借与张公三百年。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 36
    },
    {
        "title": "宋：谢澹云霞友",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "仗气凌人岂可亲，只将范泰是知闻。",
            "缘何唤作云霞友，却恐云霞未似君。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 37
    },
    {
        "title": "宋：乌衣巷",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "古迹荒基好叹嗟，满川吟景只烟霞。",
            "乌衣巷在何人住，回首令人忆谢家。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 38
    },
    {
        "title": "宋：袁粲",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "负才尚气满朝知，高卧闲吟见客稀。",
            "独步何人识袁尹，白杨郊外醉方归。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 39
    },
    {
        "title": "宋：刘伯龙",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "位重何如不厌贫，伯龙孤子只修身。",
            "固知生计还须有，穷鬼临时也笑人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 40
    },
    {
        "title": "宋：王方平",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "拂衣耕钓已多时，江上山前乐可知。",
            "著却貂裘将采药，任他人唤作渔师。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 41
    },
    {
        "title": "宋：黄罗襦",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "戚属群臣尽见猜，预忧身后又堪哀。",
            "到头委付何曾是，虚把罗襦与彦回。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 42
    },
    {
        "title": "宋：谢朏",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "谢家诸子尽兰香，各震芳名满帝乡。",
            "惟有千金更堪重，只将高卧向齐王。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 43
    },
    {
        "title": "宋：羊玄保",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "运命将来各有期，好官才阙即思之。",
            "就中堪爱羊玄保，偏受君王分外知。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 44
    },
    {
        "title": "齐：谢朏",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "解玺传呼诏侍中，却来高卧岂疏慵。",
            "此时忠节还稀有，堪羡君王特地容。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 45
    },
    {
        "title": "齐：小儿执烛",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "谢公情量已难量，忠宋心诚岂暂忘。",
            "执烛小儿浑放去，略无言语与君王。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 46
    },
    {
        "title": "齐：王僧佑",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "肯与公卿作等伦，澹然名德只推君。",
            "任他车骑来相访，箫鼓盈庭似不闻。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 47
    },
    {
        "title": "齐：王僧虔",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "位高名重不堪疑，恳让仪同帝亦知。",
            "不学常流争进取，却忧门有二台司。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 48
    },
    {
        "title": "齐：明帝裹蒸",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "至尊尊贵异人间，御膳天厨岂等闲。",
            "惜得裹蒸无用处，不如安霸取江山。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 49
    },
    {
        "title": "齐：郁林王",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "强哀强惨亦从伊，归到私庭喜可知。",
            "喜字漫书三十六，到头能得几多时。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 50
    },
    {
        "title": "齐：何氏小山",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "显达何曾肯系心，筑居郊外好园林。",
            "赚他谢朏出山去，赢得高名直至今。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 51
    },
    {
        "title": "齐：王伦之",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "豫章太守重词林，图画陈蕃与华歆。",
            "更奠子将并孺子，为君千载作知音。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 52
    },
    {
        "title": "齐：潘妃",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "曾步金莲宠绝伦，岂甘今日委埃尘。",
            "玉儿还有怀恩处，不肯将身嫁小臣。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 53
    },
    {
        "title": "齐：王亮",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "后见梁王未免哀，奈何无计拯倾颓。",
            "若教彼相颠扶得，争遣明公到此来。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 54
    },
    {
        "title": "梁：分宫女",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "涤荡齐宫法令新，分张宫女二千人。",
            "可怜无限如花貌，重见世间桃李春。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 55
    },
    {
        "title": "梁：马仙埤",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "齐朝太守不甘降，忠节当时动四方。",
            "义士要教天下见，且留君住待袁昂。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 56
    },
    {
        "title": "梁：勍敌",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "传闻天子重儒才，特为皇华绮宴开。",
            "今日方惊遇勍敌，此人元自北朝来。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 57
    },
    {
        "title": "梁：蔡撙",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "紫茄白苋以为珍，守任清真转更贫。",
            "不饮吴兴郡中水，古今能有几多人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 58
    },
    {
        "title": "梁：楚祠",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "曾与萧侯醉玉杯，此时神影尽倾颓。",
            "莫云千古无灵圣，也向西川助敌来。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 59
    },
    {
        "title": "梁：谢朏小舆",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "小舆升殿掌钧台，不免无憀却忆回。",
            "应恨被他何胤误，悔先容易出山来。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 60
    },
    {
        "title": "梁：八关斋",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "依凭金地甚虔诚，忍溺空王为圣明。",
            "内殿设斋申祷祝，岂无功德及台城。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 61
    },
    {
        "title": "梁：庾信",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "苦心词赋向谁谈，沦落周朝志岂甘。",
            "可惜多才庾开府，一生惆怅忆江南。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 62
    },
    {
        "title": "陈：王僧辨",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "彼此英雄各有名，石头高卧拟争衡。",
            "当时堪笑王僧辨，待欲将心托圣明。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 63
    },
    {
        "title": "陈：武帝蚌盘",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "金翠丝黄略不舒，蚌盘清宴意何如。",
            "岂知三阁繁华日，解为君王妙破除。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 64
    },
    {
        "title": "陈：虞居士",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "苦谏将军总不知，几随烟焰作尘飞。",
            "东山居士何人识，惟有君王却许归。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 65
    },
    {
        "title": "陈：姚察",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "曾佐徐陵向北游，剖陈疑事动名流。",
            "却归掌选清何甚，一匹花𦈌不肯收。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 66
    },
    {
        "title": "陈：宣帝伤将卒",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "前后兵师战胜回，百余城垒尽归来。",
            "当时将卒应知感，况得君王为举哀。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 67
    },
    {
        "title": "陈：临春阁",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "临春高阁上侵云，风起香飘数里闻。",
            "自是君王正沈醉，岂知消息报隋军。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 68
    },
    {
        "title": "陈：结绮阁",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "结绮高宜眺海涯，上凌丹汉拂云霞。",
            "一千朱翠同居此，争奈恩多属丽华。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 69
    },
    {
        "title": "陈：望仙阁",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "多少沈檀结筑成，望仙为号倚青冥。",
            "不知孔氏何形状，醉得君王不解醒。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 70
    },
    {
        "title": "陈：三阁",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "三阁相通绮宴开，数千朱翠绕周回。",
            "只知断送君王醉，不道韩擒已到来。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 71
    },
    {
        "title": "陈：狎客",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "八宫妃尽赋篇章，风揭歌声锦绣香。",
            "选得十人为狎客，有谁能解谏君王。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 72
    },
    {
        "title": "陈：淮水",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "文物衣冠尽入秦，六朝繁盛忽埃尘。",
            "自从淮水干枯后，不见王家更有人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 73
    },
    {
        "title": "陈：江令宅",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "不向南朝立谏名，旧居基在事分明。",
            "令人惆怅江中令，只作篇章过一生。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 74
    },
    {
        "title": "陈：后庭舞",
        "author": "孙元晏",
        "biography": "",
        "paragraphs": [
            "嬿婉回风态若飞，丽华翘袖玉为姿。",
            "后庭一曲从教舞，舞破江山君未知。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百六十七",
        "no#": 75
    }
]