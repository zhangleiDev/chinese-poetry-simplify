[
    {
        "title": "登长安慈恩寺塔",
        "author": "卢宗回",
        "biography": "",
        "paragraphs": [
            "东方晓日上翔鸾，西转苍龙拂露盘。渭水寒光摇藻井，",
            "暂辍去蓬悲不定，一凭金界望长安。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百九十",
        "no#": 1
    },
    {
        "title": "古镜歌",
        "author": "周匡物",
        "biography": "",
        "paragraphs": [
            "轩辕铸镜谁将去，曾被良工泻金取。明月中心桂不生，",
            "欲向高台对晓开，不知谁是孤光主。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百九十",
        "no#": 2
    },
    {
        "title": "及第谣",
        "author": "周匡物",
        "biography": "",
        "paragraphs": [
            "水国寒消春日长，燕莺催促花枝忙。风吹金榜落凡世，",
            "骅骝一百三十蹄，踏破蓬莱五云地。物经千载出尘埃，"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百九十",
        "no#": 3
    },
    {
        "title": "及第后谢座主",
        "author": "周匡物",
        "biography": "",
        "paragraphs": [
            "一从东越入西秦，十度闻莺不见春。试向昆山投瓦砾，",
            "中夜自将形影语，古来吞炭是何人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百九十",
        "no#": 4
    },
    {
        "title": "自题读书堂",
        "author": "周匡物",
        "biography": "",
        "paragraphs": [
            "窗外卷帘侵碧落，槛前敲竹响青冥。",
            "黄昏不欲留人宿，云起风生龙虎醒。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百九十",
        "no#": 5
    },
    {
        "title": "应举题钱塘公馆",
        "author": "周匡物",
        "biography": "",
        "paragraphs": [
            "万里茫茫天堑遥，秦皇底事不安桥。",
            "钱塘江口无钱过，又阻西陵两信潮。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百九十",
        "no#": 6
    },
    {
        "title": "题旅榇",
        "author": "廖有方",
        "biography": "",
        "paragraphs": [
            "嗟君没世委空囊，几度劳心翰墨场。",
            "半面为君申一恸，不知何处是家乡。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百九十",
        "no#": 7
    },
    {
        "title": "立春日呈宫傅侍郎",
        "author": "皇甫曙",
        "biography": "",
        "paragraphs": [
            "朝旦微风吹晓霞，散为和气满家家。不知容貌潜消落，",
            "摩娑酒瓮重封闭，待入新年共赏花。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百九十",
        "no#": 8
    },
    {
        "title": "赋得玉声如乐",
        "author": "潘存实",
        "biography": "",
        "paragraphs": [
            "表质自坚贞，因人一扣鸣。静将金并响，妙与乐同声。",
            "杳杳疑风送，泠泠似曲成。韵含湘瑟切，音带舜弦清。",
            "不独藏虹气，犹能畅物情。后夔如为听，从此振琮琤。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百九十",
        "no#": 9
    },
    {
        "title": "送林刺史简言之漳州",
        "author": "陈去疾",
        "biography": "",
        "paragraphs": [
            "江树欲含曛，清歌一送君。征骖辞荔浦，别袂暗松云。",
            "路狭横柯度，山深坠叶闻。明朝宿何处，未忍醉中分。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百九十",
        "no#": 10
    },
    {
        "title": "忆山中",
        "author": "陈去疾",
        "biography": "",
        "paragraphs": [
            "长吟重悒然，为忆山中年。清瑟泛遥夜，乱花随暮烟。",
            "珠林余露气，乳窦滴香泉。迹远尘埃外，花开绮藻前。",
            "岩罗云貌逸，竹抱水容妍。蕙磴飞英绕，萍潭片影悬。",
            "林藏诸曲胜，台擅一峰偏。会可标真寄，焚香对石筵。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百九十",
        "no#": 11
    },
    {
        "title": "元夕京城和欧阳衮",
        "author": "陈去疾",
        "biography": "",
        "paragraphs": [
            "兰焰芳芬彻晓开，珠光新霭映人来。歌迎甲夜催银管，",
            "此时月色同沾醉，何处游轮陌上回。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百九十",
        "no#": 12
    },
    {
        "title": "送韩将军之雁门",
        "author": "陈去疾",
        "biography": "",
        "paragraphs": [
            "荒塞峰烟百道驰，雁门风色暗旌旗。破围铁骑长驱疾，",
            "归来长揖功成后，黄石当年故有期。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百九十",
        "no#": 13
    },
    {
        "title": "赋得骐骥长鸣",
        "author": "陈去疾",
        "biography": "",
        "paragraphs": [
            "骐骥忻知己，嘶鸣忽异常。积悲摅怨抑，一举彻穹苍。",
            "迹类三年鸟，心驰五达庄。何言从蹇踬，今日逐腾骧。",
            "牛皂休维絷，天衢恣陆梁。向非逢伯乐，谁足见其长。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百九十",
        "no#": 14
    },
    {
        "title": "偶题",
        "author": "陈去疾",
        "biography": "",
        "paragraphs": [
            "魂梦天南垂，宿昔万里道。池台花气深，到处生春草。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百九十",
        "no#": 15
    },
    {
        "title": "春宫曲",
        "author": "陈去疾",
        "biography": "",
        "paragraphs": [
            "流莺春晓唤樱桃，花外传呼殿影高。",
            "抱里琵琶最承宠，君王敕赐玉檀槽。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百九十",
        "no#": 16
    },
    {
        "title": "采莲曲",
        "author": "陈去疾",
        "biography": "",
        "paragraphs": [
            "粉光花色叶中开，荷气衣香水上来。",
            "棹响清潭见斜领，双鸳何事亦相猜。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百九十",
        "no#": 17
    },
    {
        "title": "踏歌行",
        "author": "陈去疾",
        "biography": "",
        "paragraphs": [
            "鸳鸯楼下万花新，翡翠宫前百戏陈。",
            "夭矫翔龙衔火树，飞来瑞凤散芳春。",
            "仙跸初传紫禁香，瑞云开处夜花芳。",
            "繁弦促管升平调，绮缀丹莲借月光。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百九十",
        "no#": 18
    },
    {
        "title": "塞下曲",
        "author": "陈去疾",
        "biography": "",
        "paragraphs": [
            "春至金河雪似花，萧条玉塞但胡沙。",
            "晓来重上关城望，惟见惊尘不见家。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百九十",
        "no#": 19
    },
    {
        "title": "送人谪幽州",
        "author": "陈去疾",
        "biography": "",
        "paragraphs": [
            "临路深怀放废惭，梦中犹自忆江南。",
            "莫言塞北春风少，还胜炎荒入瘴岚。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百九十",
        "no#": 20
    },
    {
        "title": "西上辞母坟",
        "author": "陈去疾",
        "biography": "",
        "paragraphs": [
            "高盖山头日影微，黄昏独立宿禽稀。",
            "林间滴酒空垂泪，不见丁宁嘱早归。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百九十",
        "no#": 21
    }
]