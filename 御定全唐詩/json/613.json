[
    {
        "title": "过云居院玄福上人旧居",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "重到云居独悄然，隔窗窥影尚疑禅。不逢野老来听法，",
            "南宗弟子时时到，泣把山花奠几筵。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 1
    },
    {
        "title": "陪江西裴公游襄州延庆寺",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "丹霄路上歇征轮，胜地偷闲一日身。不署前驱惊野鸟，",
            "更向碧山深处问，不妨犹有草茅臣。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 2
    },
    {
        "title": "西塞山泊渔家",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "白纶巾下发如丝，静倚枫根坐钓矶。中妇桑村挑叶去，",
            "西塞山前终日客，隔波相羡尽依依。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 3
    },
    {
        "title": "襄州春游",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "信马腾腾触处行，春风相引与诗情。等闲遇事成歌咏，",
            "岑牟单绞何曾著，莫道倡狂似祢衡。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 4
    },
    {
        "title": "送从弟皮崇归复州",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "羡尔优游正少年，竟陵烟月似吴天。车螯近岸无妨取，",
            "殷勤莫笑襄阳住，为爱南溪缩项鳊。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 5
    },
    {
        "title": "题潼关兰若",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "潼津罢警有招提，近百年无战马嘶。壮士不言三尺剑，",
            "关吏不劳重借问，弃𦈡生拟入耶溪。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 6
    },
    {
        "title": "襄阳闲居与友生夜会",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "习隐悠悠世不知，林园幽事递相期。旧丝再上琴调晚，",
            "草玄寂淡无人爱，不遇刘歆更语谁。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 7
    },
    {
        "title": "习池晨起",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "清曙萧森载酒来，凉风相引绕亭台。数声翡翠背人去，",
            "竹屏风下登山屐，十宿高阳忘却回。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 8
    },
    {
        "title": "秋晚自洞庭湖别业寄穆秀才",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "破村寥落过重阳，独自撄宁葺草房。风撦红蕉仍换叶，",
            "他日若修耆旧传，为予添取此书堂。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 9
    },
    {
        "title": "华山李炼师所居",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "麻姑古貌上仙才，谪向莲峰管玉台。瑞气染衣金液启，",
            "深夜寂寥存想歇，月天时下草堂来。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 10
    },
    {
        "title": "宏词下第感恩献兵部侍郎",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "分明仙籍列清虚，自是还丹九转疏。画虎已成翻类狗，",
            "犹有报恩方寸在，不知通塞竟何如。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 11
    },
    {
        "title": "襄州汉阳王故宅",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "碑字依稀庙已荒，犹闻耆旧忆贤王。园林一半为他主，",
            "柱天功业缘何事，不得终身似霍光。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 12
    },
    {
        "title": "《伤卢献秀才",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "湣征新价欲凌空，一首堪欺左太冲。只为白衣声过重，",
            "手弄桂枝嫌不折，直教身殁负春风。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 13
    },
    {
        "title": "南阳",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "昆阳王气已萧疏，依旧山河捧帝居。废路塌平残瓦砾，",
            "二百年来霸王业，可知今日是丘墟。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 14
    },
    {
        "title": "秋晚访李处士所居",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "门前襄水碧潺潺，静钓归来不掩关。书阁鼠穿厨簏破，",
            "莫为爱诗偏念我，访君多得醉中还。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 15
    },
    {
        "title": "李处士郊居",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "石衣如发小溪清，溪上柴门架树成。园里水流浇竹响，",
            "满引红螺诗一首，刘桢失却病心情。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 16
    },
    {
        "title": "送令狐补阙归朝",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "文如日月气如虹，举国重生正始风。且愿仲山居左掖，",
            "为说明年今日事，晋廷新拜黑头公。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 17
    },
    {
        "title": "洛中寒食二首",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "千门万户掩斜晖，绣幰金衔晚未归。击鞠王孙如锦地，",
            "唯有路傍无意者，献书未纳问淮肥。",
            "远近垂杨映钿车，天津桥影压神霞。弄春公子正回首，",
            "欲知豪贵堪愁处，请看邙山晚照斜。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 18
    },
    {
        "title": "登第后寒食杏园有宴，因寄录事宋垂文同年",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "雨洗清明万象鲜，满城车马簇红筵。恩荣虽得陪高会，",
            "纵来恐被青娥笑，未纳春风一宴钱。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 19
    },
    {
        "title": "陈先辈故居",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "杉桂交阴一里余，逢人浑似洞天居。千株橘树唯沽酒，",
            "襄阳无限烟霞地，难觅幽奇似此殊。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 20
    },
    {
        "title": "奉和鲁望寒夜访寂上人次韵",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "院寒青霭正沉沉，霜栈干鸣入古林。数叶贝书松火暗，",
            "何事欲攀尘外契，除君皆有利名心。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 21
    },
    {
        "title": "江南道中怀茅山广文南阳博士三首",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "寒岚依约认华阳，遥想高人卧草堂。半日始斋青z3饭，",
            "谁道夫君无伴侣，不离窗下见羲皇。",
            "住在华阳第八天，望君唯欲结良缘。堂扃洞里千秋燕，",
            "不知何事迎新岁，乌纳裘中一觉眠。",
            "五色香烟惹内文，石饴初熟酒初醺。将开丹灶那防鹤，",
            "如何世外无交者，一卧金坛只有君。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 22
    },
    {
        "title": "奉和鲁望早春雪中作吴体见寄",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "威仰噤死不敢语，琼花云魄清珊珊。溪光冷射触鸀鳿，",
            "全吴缥瓦十万户，惟君与我如袁安。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 23
    },
    {
        "title": "吴中言情寄鲁望",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "古来伧父爱吴乡，一上胥台不可忘。爱酒有情如手足，",
            "为说松江堪老处，满船烟月湿莎裳。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 24
    },
    {
        "title": "行次野梅",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "茑拂萝捎一树梅，玉妃无侣独裴回。好临王母瑶池发，",
            "不堪便向多情道，万片霜华雨损来。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 25
    },
    {
        "title": "扬州看辛夷花",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "腊前千朵亚芳丛，细腻偏胜素柰功。螓首不言披晓雪，",
            "应为当时天女服，至今犹未放全红。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 26
    },
    {
        "title": "暇日独处寄鲁望",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "幽慵不觉耗年光，犀柄金徽乱一床。野客共为赊酒计，",
            "无限高情好风月，不妨犹得事吾王。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 27
    },
    {
        "title": "屣步访鲁望不遇",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "雪晴墟里竹欹斜，蜡屐徐吟到陆家。荒径扫稀堆柏子，",
            "拟受太玄今不遇，可怜遗恨似侯芭。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 28
    },
    {
        "title": "开元寺客省早景即事",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "客省萧条柿叶红，楼台如画倚霜空。铜池数滴桂上雨，",
            "如何尘外虚为契，不得支公此会同。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 29
    },
    {
        "title": "奉和鲁望独夜有怀吴体见寄",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "病鹤带雾傍独屋，破巢含雪倾孤梧。濯足将加汉光腹，",
            "此时枉欠高散物，楠瘤作樽石作垆。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 30
    },
    {
        "title": "病中有人惠海蟹转寄鲁望",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "绀甲青筐染菭衣，岛夷初寄北人时。离居定有石帆觉，",
            "病中无用霜螯处，寄与夫君左手持。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 31
    },
    {
        "title": "病中美景颇阻追游因寄鲁望",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "瘿床闲卧昼迢迢，唯把真如慰寂寥。南国不须收薏苡，",
            "看取病来多少日，早梅零落玉华焦。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 32
    },
    {
        "title": "鲁望以花翁之什见招因次韵酬之",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "九十携锄伛偻翁，小园幽事尽能通。㔉烟栽药为身计，",
            "不知家道能多少，只在句芒一夜风。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 33
    },
    {
        "title": "病中庭际海石榴花盛发，感而有寄",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "一夜春光绽绛囊，碧油枝上昼煌煌。风匀只似调红露，",
            "不知桂树知情否，无限同游阻陆郎。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 34
    },
    {
        "title": "早春以橘子寄鲁望",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "个个和枝叶捧鲜，彩凝犹带洞庭烟。不为韩嫣金丸重，",
            "知君多病仍中圣，尽送寒苞向枕边。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 35
    },
    {
        "title": "病中书情寄上崔谏议",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "十日来来旷奉公，闭门无事忌春风。虫丝度日萦琴荐，",
            "殷勤莫怪求医切，只为山樱欲放红。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 36
    },
    {
        "title": "病孔雀",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "烟花虽媚思沈冥，犹自抬头护翠翎。强听紫箫如欲舞，",
            "尽日春风吹不起，钿毫金缕一星星。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 37
    },
    {
        "title": "奉和鲁望上元日道室焚修",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "明真台上下仙官，玄藻初吟万籁寒。飙御有声时杳杳，",
            "端简不知清景暮，灵芜香烬落金坛。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 38
    },
    {
        "title": "奉酬鲁望惜春见寄",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "十五日中春日好，可怜沉痼冷如灰。以前虽被愁将去，",
            "病中无限花番次，为约东风且住开。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 39
    },
    {
        "title": "闻鲁望游颜家林园病中有寄",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "一夜韶姿著水光，谢家春草满池塘。细挑泉眼寻新脉，",
            "分明不得同君赏，尽日倾心羡索郎。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 40
    },
    {
        "title": "奉和鲁望春雨即事次韵",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "织恨凝愁映鸟飞，半旬飘洒掩韶晖。山容洗得如烟瘦，",
            "何年细湿华阳道，两乘巾车相并归。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 41
    },
    {
        "title": "鲁望春日多寻野景日休抱疾杜门因有是寄",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "野侣相逢不待期，半缘幽事半缘诗。乌纱任岸穿筋竹，",
            "病中不用君相忆，折取山樱寄一枝。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 42
    },
    {
        "title": "鲁望以躬掇野蔬兼示雅什，用以酬谢",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "杖擿春烟暖向阳，烦君为我致盈筐。深挑乍见牛唇液，",
            "雕胡饭熟bu糊软，不是高人不合尝。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 43
    },
    {
        "title": "卧病感春寄鲁望",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "乌皮几上困腾腾，玉柄清羸愧不能。昨夜眠时稀似鹤，",
            "任是雨多游未得，也须收在探花朋。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 44
    },
    {
        "title": "奉和鲁望徐方平后闻赦次韵",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "金鸡烟外上临轩，紫诰新垂作解恩。涿鹿未销初败血，",
            "未遣蒲车问幽隐，共君应老抱桐孙。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 45
    },
    {
        "title": "奉酬鲁望见答鱼笺之什",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "轻如隐起腻如饴，除却鲛工解制稀。欲写恐成河伯诏，",
            "若用莫将闲处去，好题春思赠江妃。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 46
    },
    {
        "title": "病后春思",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "连钱锦暗麝氛氲，荆思多才咏鄂君。孔雀钿寒窥沼见，",
            "应笑病来惭满愿，花笺好作断肠文。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 47
    },
    {
        "title": "偶成小酌招鲁望不至，以诗为解，因次韵酬之",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "醉侣相邀爱早阳，小筵催办不胜忙。冲深柳驻吴娃幰，",
            "如何共是忘形者，不见渔阳掺一场。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 48
    },
    {
        "title": "以纱巾寄鲁望因而有作",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "周家新样替三梁。裹发偏宜白面郎。掩敛乍疑裁黑雾，",
            "更有一般君未识，虎文巾在绛霄房。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 49
    },
    {
        "title": "临顿宅将有归于之日鲁望以诗见贶，因抒怀酬之",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "共老林泉忍暂分，此生应不识回文。几枚竹笥送德曜，",
            "居然自是幽人事，辄莫教他孙寿闻。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 50
    },
    {
        "title": "奉和鲁望谢惠巨鱼之半",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "钓公来信自松江，三尺春鱼拨剌霜。腹内旧钩苔染涩，",
            "何事贶君偏得所，只缘同是越航郎。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 51
    },
    {
        "title": "馆娃宫怀古",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "艳骨已成兰麝土，宫墙依旧压层崖。弩台雨坏逢金镞，",
            "姑苏麋鹿真闲事，须为当时一怆怀。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 52
    },
    {
        "title": "以紫石砚寄鲁望兼酬见赠",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "样如金蹙小能轻，微润将融紫玉英。石墨一研为凤尾，",
            "两地有期皆好用，不须空把洗溪声。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 53
    },
    {
        "title": "奉和鲁望同游北禅院",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "戚历杉阴入草堂，老僧相见似相忘。吟多几转莲花漏，",
            "云林满眼空羁滞，欲对弥天却自伤。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 54
    },
    {
        "title": "孙发百篇将游天台请诗赠行，因以送之",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "孙子荆家思有余，元戎曾荐入公车。百篇宫体喧金屋，",
            "因逢二老如相问，正滞江南为eE鱼。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 55
    },
    {
        "title": "奉和鲁望蔷薇次韵",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "谁绣连延满户陈，暂应遮得陆郎贫。红芳掩敛将迷蝶，",
            "只应是董双成戏，剪得神霞寸寸新。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 56
    },
    {
        "title": "闻开元寺开笋园寄章上人",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "园锁开声骇鹿群，满林鲜箨水犀文。森森竞泫林梢雨，",
            "折烟束露如相遗，何胤明朝不茹荤。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 57
    },
    {
        "title": "开元寺佛钵诗",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "帝青石作绿冰姿，曾得金人手自持。拘律树边斋散后，",
            "从此共君新顶戴，斜风应不等闲吹。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 58
    },
    {
        "title": "夏首病愈因招鲁望",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "晓入清和尚袷衣，夏阴初合掩双扉。一声拨谷桑柘晚，",
            "明朝早起非无事，买得莼丝待陆机。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 59
    },
    {
        "title": "奉和鲁望新夏东郊闲泛",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "水物轻明淡似秋，多情才子倚兰舟。碧莎裳下携诗草，",
            "共君莫问当时事，一点沙禽胜五侯。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 60
    },
    {
        "title": "奉和鲁望四月十五日道室书事",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "望朝斋戒是寻常，静启金根第几章。竹叶饮为甘露色，",
            "剩欲与君终此志，顽仙唯恐鬓成霜。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 61
    },
    {
        "title": "奉和鲁望看压新醅",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "一箦松花细有声，旋将渠碗撇寒清。秦吴只恐𥬠来近，",
            "五湖烟水郎山月，合向樽前问底名。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 62
    },
    {
        "title": "登初阳楼寄怀北平郎中",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "危楼新制号初阳，白粉青葌射沼光。避酒几浮轻舴艋，",
            "莫怪重登频有恨，二年曾侍旧吴王。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 63
    },
    {
        "title": "夏初访鲁望偶题小斋",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "半里芳阴到陆家，藜床相劝饭胡麻。林间度宿抛棋局，",
            "踟蹰未放闲人去，半岸纱帩待月华。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 64
    },
    {
        "title": "所居首夏，水木尤清，适然有作",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "病来无事草堂空，昼水休闻十二筒。桂静似逢青眼客，",
            "尽日枕书慵起得，被君犹自笑从公。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 65
    },
    {
        "title": "重玄寺元达年逾八十，好种名药，凡所植者多至自天台四明包山句曲，丛翠纷糅，各可指名，余奇而访之，因题二章",
        "author": "皮日休",
        "biography": "",
        "paragraphs": [
            "雨涤烟锄伛偻赍，绀牙红甲两三畦。药名却笑桐君少，",
            "怪来昨日休持钵，一尺雕胡似掌齐。",
            "香蔓蒙茏覆昔邪，桂烟杉露湿袈裟。石盆换水捞松叶，",
            "支公谩道怜神骏，不及今朝种一麻。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百一十三",
        "no#": 66
    }
]