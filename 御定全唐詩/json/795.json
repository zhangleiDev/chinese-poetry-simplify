[
    {
        "title": "句",
        "author": "李日知",
        "biography": "荥阳人，历相，中宗睿宗",
        "paragraphs": [
            "所愿暂知居者乐，无使时称主者劳。"
        ],
        "notes": [
            "（中宗幸安乐公主第，从官赋诗，日知卒章，独存规诫，识者多之）"
        ],
        "volume": "卷七百九十五",
        "no#": 1
    },
    {
        "title": "句",
        "author": "赵仁奖",
        "biography": "河南人，景龙中为御史睿宗朝左授上蔡丞",
        "paragraphs": [
            "令乘骢马去，丞脱绣衣来。"
        ],
        "notes": [
            "（《赠上蔡令潘好礼拜御史》，见《朝野佥载》）"
        ],
        "volume": "卷七百九十五",
        "no#": 2
    },
    {
        "title": "句",
        "author": "郭廷谓",
        "biography": "",
        "paragraphs": [
            "魂逐东流水，坟依独坐山。"
        ],
        "notes": [
            "（《哭陈子昂》）"
        ],
        "volume": "卷七百九十五",
        "no#": 3
    },
    {
        "title": "句",
        "author": "韦青",
        "biography": "官将军",
        "paragraphs": [
            "三代掌纶诰，一身能唱歌。"
        ],
        "notes": [
            "（见《乐府杂录》）"
        ],
        "volume": "卷七百九十五",
        "no#": 4
    },
    {
        "title": "句",
        "author": "尉迟匡",
        "biography": "幽并人，开元进士",
        "paragraphs": [
            "明月飞出海，黄河流上天。"
        ],
        "notes": [
            "（《暮行潼关》）"
        ],
        "volume": "卷七百九十五",
        "no#": 5
    },
    {
        "title": "句",
        "author": "尉迟匡",
        "biography": "",
        "paragraphs": [
            "芙蓉初出水，桃李忽无言。"
        ],
        "notes": [
            "（《观内人楼上踏歌》）"
        ],
        "volume": "卷七百九十五",
        "no#": 6
    },
    {
        "title": "句",
        "author": "尉迟匡",
        "biography": "",
        "paragraphs": [
            "夜夜月为青冢镜，年年雪作黑山花。"
        ],
        "notes": [
            "（《塞上曲》）"
        ],
        "volume": "卷七百九十五",
        "no#": 7
    },
    {
        "title": "句",
        "author": "何涓",
        "biography": "",
        "paragraphs": [
            "雁影数行秋半逢，渔歌一声夜深发。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百九十五",
        "no#": 8
    },
    {
        "title": "句",
        "author": "杜伟",
        "biography": "",
        "paragraphs": [
            "忽睹邢武辞，泠其金石备。"
        ],
        "notes": [
            "（《宣城总集》云：唐开元甲子，武平一同河间邢巨同游泾川琴溪，题绝句，古刻尚存。后一纪，杜伟自柱史谪掾宣城，陪连帅班景倩来观，题句云云。余逸）"
        ],
        "volume": "卷七百九十五",
        "no#": 9
    },
    {
        "title": "句",
        "author": "申堂构",
        "biography": "丹徒人，尝为武进尉",
        "paragraphs": [
            "霜添柏树冷，气拂桂林寒。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百九十五",
        "no#": 10
    },
    {
        "title": "句",
        "author": "周愿",
        "biography": "竟陵刺史",
        "paragraphs": [
            "八十年前棠树阴，竟陵太守公先人。"
        ],
        "notes": [
            "（愿与竟陵陆羽尝佐岭南连帅李复幕府。后愿刺竟陵，则复已捐馆，而羽亦前谢。复父斋物先亦为竟陵守，愿因为七言诗陈事。见《方舆胜览》）"
        ],
        "volume": "卷七百九十五",
        "no#": 11
    },
    {
        "title": "句",
        "author": "苐五琦",
        "biography": "字禹珪，长安人，肃宗朝为盐铁铸钱使，拜相坐事长流终太子賔客",
        "paragraphs": [
            "阴天闻断雁，夜浦送归人。"
        ],
        "notes": [
            "（《送丘郎中》）"
        ],
        "volume": "卷七百九十五",
        "no#": 12
    },
    {
        "title": "句",
        "author": "颜允南",
        "biography": "歴官，司封郎中，国子司业",
        "paragraphs": [
            "谁言百人会，兄弟也沾陪。"
        ],
        "notes": [
            "（《侍宴》见《颜真卿集》）"
        ],
        "volume": "卷七百九十五",
        "no#": 13
    },
    {
        "title": "句",
        "author": "季广琛",
        "biography": "𤓰州刺史，歴肃代朝终㪚骑常侍",
        "paragraphs": [
            "刻舟寻已化，弹铗未酬恩。"
        ],
        "notes": [
            "（河西骑将宋青春有剑，是青龙精。刃所及，若叩铜铁。青春死，为广琛所得。或风雨后迸光出室，环烛方丈。哥舒翰求易以他宝，广琛不与，因赠诗。见《酉阳杂俎》）"
        ],
        "volume": "卷七百九十五",
        "no#": 14
    },
    {
        "title": "句",
        "author": "陈蜕",
        "biography": "肃代间人",
        "paragraphs": [
            "梦里换春秋。"
        ],
        "notes": [
            "（《咏华清宫》）"
        ],
        "volume": "卷七百九十五",
        "no#": 15
    },
    {
        "title": "句",
        "author": "卫准",
        "biography": "准一作单大，歴五年进士",
        "paragraphs": [
            "莫言闲话是闲话，往往事从闲话来。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百九十五",
        "no#": 16
    },
    {
        "title": "句",
        "author": "卫准",
        "biography": "",
        "paragraphs": [
            "何必剃头为弟子，无家便是出家人。"
        ],
        "notes": [
            "（并见张为《主客图》）"
        ],
        "volume": "卷七百九十五",
        "no#": 17
    },
    {
        "title": "句",
        "author": "杜鸿渐",
        "biography": "字之㢲，濮阳人，累官朔方度支副使，以迎立肃宗，封衞国公代宗朝拜相",
        "paragraphs": [
            "常愿追禅理，安能挹化源。"
        ],
        "notes": [
            "（鸿渐酷好浮屠道，晚年乐退静，常悠然赋句，朝士多属和之。见《本传》）"
        ],
        "volume": "卷七百九十五",
        "no#": 18
    },
    {
        "title": "句",
        "author": "李挚",
        "biography": "贞元十二年宏词科登第",
        "paragraphs": [
            "因缘三纪异，契分四般同。"
        ],
        "notes": [
            "（挚与李行敏同姓，同甲子、同年登第，俱二十五岁，又同门，故云。见《纪事》）"
        ],
        "volume": "卷七百九十五",
        "no#": 19
    },
    {
        "title": "句",
        "author": "韦绶",
        "biography": "德宗朝翰林学士",
        "paragraphs": [
            "在室愧屋漏。"
        ],
        "notes": [
            "（绶子温疾革，诵绶此句曰：「今不负斯诫矣。」见《唐书》）"
        ],
        "volume": "卷七百九十五",
        "no#": 20
    },
    {
        "title": "句",
        "author": "冯戡",
        "biography": "东川人",
        "paragraphs": [
            "桃花浪里成龙去，竹叶山头退鹢飞。"
        ],
        "notes": [
            "（《赠柳棠及第》，见《纪事》）"
        ],
        "volume": "卷七百九十五",
        "no#": 21
    },
    {
        "title": "句",
        "author": "韩泰",
        "biography": "字安平，昌黎人，户部郎中，以附王叔文贬䖍州司马后终郴州刺史",
        "paragraphs": [
            "庾岭东边吏隐州，溪山竹树亦清幽。"
        ],
        "notes": [
            "（见《方舆胜览》）"
        ],
        "volume": "卷七百九十五",
        "no#": 22
    },
    {
        "title": "句",
        "author": "卢并",
        "biography": "文宗朝资州守",
        "paragraphs": [
            "地灵无俗草，岩静有仙禽。"
        ],
        "notes": [
            "（《等慈寺北岩》，见《方舆胜览》）"
        ],
        "volume": "卷七百九十五",
        "no#": 23
    },
    {
        "title": "句",
        "author": "缪岛云",
        "biography": "岛云少从浮图武宗朝，准勅，反初服",
        "paragraphs": [
            "白鸟远行树，玉虹孤饮潭。"
        ],
        "notes": [
            "（《庐山瀑布》）"
        ],
        "volume": "卷七百九十五",
        "no#": 24
    },
    {
        "title": "句",
        "author": "缪岛云",
        "biography": "",
        "paragraphs": [
            "四五片霞生绝壁，两三行雁过疏松。"
        ],
        "notes": [
            "（以上见《纪事》）"
        ],
        "volume": "卷七百九十五",
        "no#": 25
    },
    {
        "title": "句",
        "author": "缪岛云",
        "biography": "",
        "paragraphs": [
            "抛芥子降颠狒狒，折杨枝洒醉猩猩。"
        ],
        "notes": [
            "（见《摭言》）",
            "见《唐摭言》"
        ],
        "volume": "卷七百九十五",
        "no#": 26
    },
    {
        "title": "句",
        "author": "刘敬之",
        "biography": "云安人",
        "paragraphs": [
            "山近衡阳虽少雁，水连巴蜀岂无鱼。"
        ],
        "notes": [
            "（敬之甥雍陶，蜀川上第后，薄于亲友，不寄书。敬之让焉，陶得诗愧赧）"
        ],
        "volume": "卷七百九十五",
        "no#": 27
    },
    {
        "title": "句",
        "author": "李涛",
        "biography": "长沙人，温飞卿任太学博士，主秋试。涛与衞丹、张郃等诗赋，皆牓于都",
        "paragraphs": [
            "水声长在耳，山色不离门。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百九十五",
        "no#": 28
    },
    {
        "title": "句",
        "author": "李涛",
        "biography": "",
        "paragraphs": [
            "扫地树留影，拂床琴有声。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百九十五",
        "no#": 29
    },
    {
        "title": "句",
        "author": "李涛",
        "biography": "",
        "paragraphs": [
            "落日长安道，秋槐满地花。"
        ],
        "notes": [
            "（以下并见《纪事》）"
        ],
        "volume": "卷七百九十五",
        "no#": 30
    },
    {
        "title": "句",
        "author": "高元裕",
        "biography": "字景圭，渤海人，开成中翰林学士，终吏部尚书",
        "paragraphs": [
            "中丞为国拔贤才，寒俊欣逢藻鉴开。"
        ],
        "notes": [
            "（《赠知贡举陈商》，见《池阳志》）"
        ],
        "volume": "卷七百九十五",
        "no#": 31
    },
    {
        "title": "句",
        "author": "韦澳",
        "biography": "字子斐，大中初以考功郎，知制诰，充翰林学士承旨，终户部侍郎",
        "paragraphs": [
            "莫将韦监同殷监，错认容身是保身。"
        ],
        "notes": [
            "（懿宗朝，澳为吏部侍郎，不受请托，为执政所恶，出为邠宁节度。时宰复发其部事，罢镇，以秘书监分司东都。尝戏吟此句，京师权幸闻而尤怒之。见《唐书》）"
        ],
        "volume": "卷七百九十五",
        "no#": 32
    },
    {
        "title": "句",
        "author": "王龟",
        "biography": "字大年，官㑹稽太守",
        "paragraphs": [
            "珠箔卷繁星，金樽泻月明。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百九十五",
        "no#": 33
    },
    {
        "title": "句",
        "author": "李郁",
        "biography": "泉州人",
        "paragraphs": [
            "身死为修刘稹表，名高因让陆洿书。"
        ],
        "notes": [
            "（《吊欧阳秬》秬，詹从子，开成中擢第。司勋郎中陆洿弃官隐吴中，后赴召复出，秬移书让之，因不出。秬名益闻，竟赴泽潞刘稹幕辟。稹表指斥时政，朝廷疑秬所草，流崖州，赐死）"
        ],
        "volume": "卷七百九十五",
        "no#": 34
    },
    {
        "title": "句",
        "author": "詹雄",
        "biography": "字伯镇，福州人，不第终",
        "paragraphs": [
            "尘飞遗恨尽，花落古宫平。"
        ],
        "notes": [
            "（《洛阳古城》）"
        ],
        "volume": "卷七百九十五",
        "no#": 35
    },
    {
        "title": "句",
        "author": "詹雄",
        "biography": "",
        "paragraphs": [
            "红粉笙歌人代远，月明陵树水东流。"
        ],
        "notes": [
            "（《铜雀台》）"
        ],
        "volume": "卷七百九十五",
        "no#": 36
    },
    {
        "title": "句",
        "author": "任涛",
        "biography": "豫章筠州人，咸通中十哲之一也，不第终",
        "paragraphs": [
            "露团沙鹤起，人卧钓船流。"
        ],
        "notes": [
            "（见《摭言》）"
        ],
        "volume": "卷七百九十五",
        "no#": 37
    },
    {
        "title": "句",
        "author": "剧燕",
        "biography": "蒲坂人，为诗雅正，亦十哲之一也。后客王重荣，被杀",
        "paragraphs": [
            "只向国门安四海，不离乡井拜三公。"
        ],
        "notes": [
            "（《赠王重荣》。《纪事》：重荣镇河中，燕投此诗，甚加礼敬。竟以凌轹诸从事，受正平之祸）"
        ],
        "volume": "卷七百九十五",
        "no#": 38
    },
    {
        "title": "句",
        "author": "王璘",
        "biography": "璘词学富赡，崔詹事廉问表荐于朝，先试之使廨。璘请十书吏皆给笔札，璘口授十吏笔不停缀。日未亭午，已七千余言。时路嵓方当钧轴，遣一介召之。璘曰：请俟见帝。嵓大怒，亟命奏废万言科。璘杖䇿而归",
        "paragraphs": [
            "芍药花开菩萨面，棕闾叶散野人头。"
        ],
        "notes": [
            "（璘与李群玉相遇岳麓寺，群玉待之甚浅。曰：「请与公联句。」破题而授之，璘略不伫思，继之云云。群玉遂屈。见《纪事》）",
            "生平见《唐诗纪事》卷六六"
        ],
        "volume": "卷七百九十五",
        "no#": 39
    },
    {
        "title": "句",
        "author": "李蔚",
        "biography": "咸通中宰相，后出为节镇",
        "paragraphs": [
            "虚心纤质雁衔余，凤吹龙吟定不如。"
        ],
        "notes": [
            "（薛阳陶善吹芦管。蔚镇淮海，阳陶为浙右小校，监押度支。运米至，蔚召，令出芦管，于赏心亭奏之。蔚大嘉赏，赠诗此其终篇也）"
        ],
        "volume": "卷七百九十五",
        "no#": 40
    },
    {
        "title": "句",
        "author": "王枳",
        "biography": "常州刺史",
        "paragraphs": [
            "今朝拜贡盈襟泪，不进新芽是进心。"
        ],
        "notes": [
            "（常州旧贡阳羡茶。僖宗幸蜀，枳间关驰贡，故有此句。见《常州志》）"
        ],
        "volume": "卷七百九十五",
        "no#": 41
    },
    {
        "title": "句",
        "author": "郑賨",
        "biography": "字贡华，干符进士",
        "paragraphs": [
            "万蕴千牌次碧牙，缥笺金字间明霞。"
        ],
        "notes": [
            "（賨善楷书。天复中，挈家自华至陈，未尝废词翰。其题经藏有此句，见《宣和书谱》）"
        ],
        "volume": "卷七百九十五",
        "no#": 42
    },
    {
        "title": "句",
        "author": "张茔",
        "biography": "速江人，大顺初登第，官至礼部尚书",
        "paragraphs": [
            "一箭不中鹄，五湖归钓鱼。"
        ],
        "notes": [
            "（见《地志》）"
        ],
        "volume": "卷七百九十五",
        "no#": 43
    },
    {
        "title": "句",
        "author": "吴霭",
        "biography": "字廷俊，连州人，光化三年进士，为朱忠幕僚",
        "paragraphs": [
            "烟随红焰断，化作白云飞。"
        ],
        "notes": [
            "（七岁时咏野烧，识者知其为青云器）"
        ],
        "volume": "卷七百九十五",
        "no#": 44
    },
    {
        "title": "句",
        "author": "陈咏",
        "biography": "眉州青神人，善奕碁（围棋），昭宗时登第",
        "paragraphs": [
            "隔岸水牛浮鼻渡，傍溪沙鸟点头行。"
        ],
        "notes": [
            "（见《纪事》）"
        ],
        "volume": "卷七百九十五",
        "no#": 45
    },
    {
        "title": "句",
        "author": "蒋密",
        "biography": "零陵儒士",
        "paragraphs": [
            "绮罗因片叶，桃李谩同时。"
        ],
        "notes": [
            "（咏桑 见《诗注总话》）"
        ],
        "volume": "卷七百九十五",
        "no#": 46
    },
    {
        "title": "句",
        "author": "符蒙",
        "biography": "字适之，后唐同光三年进士，官侍郎",
        "paragraphs": [
            "都缘心似水，故以钵为舟。"
        ],
        "notes": [
            "（《题壁画杯渡道人》）"
        ],
        "volume": "卷七百九十五",
        "no#": 47
    },
    {
        "title": "句",
        "author": "李景遂",
        "biography": "字退身，元宗母弟，初封燕王，改封晋王",
        "paragraphs": [
            "路指丹阳分虎节，心存双阙恋龙颜。"
        ],
        "notes": [
            "（《赴润州镇赐饯》，见《江表志》）"
        ],
        "volume": "卷七百九十五",
        "no#": 48
    },
    {
        "title": "句",
        "author": "李弘茂",
        "biography": "",
        "paragraphs": [
            "甜于泉水茶须信，狂似杨花蝶未知。"
        ],
        "notes": [
            "（《咏雪》）"
        ],
        "volume": "卷七百九十五",
        "no#": 49
    },
    {
        "title": "句",
        "author": "李弘茂",
        "biography": "",
        "paragraphs": [
            "半窗月在犹煎药，几夜灯闲不照书。"
        ],
        "notes": [
            "（《病中》，并见《江表志》）"
        ],
        "volume": "卷七百九十五",
        "no#": 50
    },
    {
        "title": "句",
        "author": "李平",
        "biography": "闗右人，本姓名曰杨讷，归南唐，官尚书郎卫尉卿",
        "paragraphs": [
            "至人无梦梦不到，天道恶盈盈有余。"
        ],
        "notes": [
            "（《闲书》）"
        ],
        "volume": "卷七百九十五",
        "no#": 51
    },
    {
        "title": "句",
        "author": "李平",
        "biography": "",
        "paragraphs": [
            "龙髯已断嫔嫱老，豹尾不来岐路长。"
        ],
        "notes": [
            "（《读武帝内传》）"
        ],
        "volume": "卷七百九十五",
        "no#": 52
    },
    {
        "title": "句",
        "author": "胡元龟",
        "biography": "庐陵人，官临川令",
        "paragraphs": [
            "翻身腾白浪，探爪擢明珠。"
        ],
        "notes": [
            "（永新令请题屏画戏珠龙。令好受馈遗，元龟写意讥之。令悟，追捕，遂亡入金陵）"
        ],
        "volume": "卷七百九十五",
        "no#": 53
    },
    {
        "title": "句",
        "author": "蒋钩",
        "biography": "",
        "paragraphs": [
            "因借梦书过竹寺，学耕秋粟绕茆原。"
        ],
        "notes": [
            "（《寄柳宣》）"
        ],
        "volume": "卷七百九十五",
        "no#": 54
    },
    {
        "title": "句",
        "author": "蒋钩",
        "biography": "",
        "paragraphs": [
            "芭蕉叶上无愁雨，自是多情听断肠。"
        ],
        "notes": [
            "（戎昱诗有「一夜不眠孤客耳，主人门外有芭蕉。」代为之答）"
        ],
        "volume": "卷七百九十五",
        "no#": 55
    },
    {
        "title": "句",
        "author": "史虚白",
        "biography": "",
        "paragraphs": [
            "风雨揭却屋，全家醉不知。"
        ],
        "notes": [
            "（《咏渔父》　见《江南野录》）"
        ],
        "volume": "卷七百九十五",
        "no#": 56
    },
    {
        "title": "句",
        "author": "夏宝松",
        "biography": "",
        "paragraphs": [
            "孤猿叫落中岩月，野客吟残半夜灯。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百九十五",
        "no#": 57
    },
    {
        "title": "句",
        "author": "夏宝松",
        "biography": "",
        "paragraphs": [
            "雁飞南浦砧初断，月满西楼酒半醒。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百九十五",
        "no#": 58
    },
    {
        "title": "句",
        "author": "夏宝松",
        "biography": "",
        "paragraphs": [
            "晓来羸驷依前去，目断遥山数点清。"
        ],
        "notes": [
            "（《宿江城》，并见《锦绣万花谷》）"
        ],
        "volume": "卷七百九十五",
        "no#": 59
    },
    {
        "title": "句",
        "author": "赵庆",
        "biography": "水曹郎",
        "paragraphs": [
            "迈古文章金鸑鷟，出群行止玉麒麟。"
        ],
        "notes": [
            "（《赠邵拙》，见《马令书》）"
        ],
        "volume": "卷七百九十五",
        "no#": 60
    },
    {
        "title": "句",
        "author": "潘天锡",
        "biography": "南唐时为省郎",
        "paragraphs": [
            "风便磬声远，日斜楼影长。"
        ],
        "notes": [
            "（《游古观》，见《郡阁雅谈》）"
        ],
        "volume": "卷七百九十五",
        "no#": 61
    },
    {
        "title": "句",
        "author": "朱勰",
        "biography": "南唐时，尝为县令",
        "paragraphs": [
            "好是晚来香雨里，担簦亲送绮罗人。"
        ],
        "notes": [
            "（见《诗史》）"
        ],
        "volume": "卷七百九十五",
        "no#": 62
    },
    {
        "title": "句",
        "author": "齐镐",
        "biography": "",
        "paragraphs": [
            "莲中花更好，云里月常新。"
        ],
        "notes": [
            "（南唐宫嫔窅娘善舞，后主作金莲，令窅娘缠足作新月状，舞莲中。镐诗因窅娘作也。见《道山清话》）"
        ],
        "volume": "卷七百九十五",
        "no#": 63
    },
    {
        "title": "句",
        "author": "黄可",
        "biography": "字不可，南唐时人",
        "paragraphs": [
            "天下传将舞马赋，门前迎得跨驴宾。"
        ],
        "notes": [
            "（《献高侍郎》）"
        ],
        "volume": "卷七百九十五",
        "no#": 64
    },
    {
        "title": "句",
        "author": "高元矩",
        "biography": "宣城人",
        "paragraphs": [
            "砚贮寒泉碧，庭堆败叶红。"
        ],
        "notes": [
            "（《赠宣城宰》）"
        ],
        "volume": "卷七百九十五",
        "no#": 65
    },
    {
        "title": "句",
        "author": "高元矩",
        "biography": "",
        "paragraphs": [
            "燕掠琴弦穿静院，吏收诗草下闲庭。"
        ],
        "notes": [
            "（《赠徐学士》，见《雅言系述》）"
        ],
        "volume": "卷七百九十五",
        "no#": 66
    },
    {
        "title": "句",
        "author": "陈贶",
        "biography": "南唐处士",
        "paragraphs": [
            "年年闻尔者，未有不伤情。"
        ],
        "notes": [
            "（《蝉》）"
        ],
        "volume": "卷七百九十五",
        "no#": 67
    },
    {
        "title": "句",
        "author": "陈贶",
        "biography": "",
        "paragraphs": [
            "出得风尘者，合知岐路人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百九十五",
        "no#": 68
    },
    {
        "title": "句",
        "author": "陈贶",
        "biography": "",
        "paragraphs": [
            "拂榻灯未来，开门月先入。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百九十五",
        "no#": 69
    },
    {
        "title": "句",
        "author": "陈贶",
        "biography": "",
        "paragraphs": [
            "忽生云是匣，高以月为台。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百九十五",
        "no#": 70
    },
    {
        "title": "句",
        "author": "陈贶",
        "biography": "",
        "paragraphs": [
            "入夜虽无伤物意，向明还有动人心。"
        ],
        "notes": [
            "（《画虎》，并《见吟窗杂录》）"
        ],
        "volume": "卷七百九十五",
        "no#": 71
    },
    {
        "title": "句",
        "author": "赵休",
        "biography": "幸夤逊同时人",
        "paragraphs": [
            "金茎来白露，玉宇起清风。"
        ],
        "notes": [
            "（《侍宴》，见《吟窗杂录》）"
        ],
        "volume": "卷七百九十五",
        "no#": 72
    },
    {
        "title": "句",
        "author": "庸仁杰",
        "biography": "泉州人，初为僧。陈德诚劝之，反初服，官终汾阳令",
        "paragraphs": [
            "红旆渡江霞蘸水，青蛇出匣雪侵衣。"
        ],
        "notes": [
            "（《赠池阳守陈德诚》）",
            "反初服：恢复做官时的服饰。出至《文选．曹植．七启》：「愿反初服，从子而归。」"
        ],
        "volume": "卷七百九十五",
        "no#": 73
    },
    {
        "title": "句",
        "author": "庸仁杰",
        "biography": "",
        "paragraphs": [
            "云散便凝千里望，日斜长占半城阴。"
        ],
        "notes": [
            "（《升元阁》）"
        ],
        "volume": "卷七百九十五",
        "no#": 74
    },
    {
        "title": "句",
        "author": "庸仁杰",
        "biography": "",
        "paragraphs": [
            "只住此山宁有意，向来求佛本无心。"
        ],
        "notes": [
            "（《赠嘉禾峰僧》）",
            "宁:寜"
        ],
        "volume": "卷七百九十五",
        "no#": 75
    },
    {
        "title": "句",
        "author": "邵拙",
        "biography": "字拙之，宣城人",
        "paragraphs": [
            "万国不得雨，孤云犹在山。"
        ],
        "notes": [
            "（见《江南野录》）"
        ],
        "volume": "卷七百九十五",
        "no#": 76
    },
    {
        "title": "句",
        "author": "毛炳",
        "biography": "南唐时丰城人",
        "paragraphs": [
            "先生不在此，千载只空山。"
        ],
        "notes": [
            "（《题斋壁》，见《江南野录》）"
        ],
        "volume": "卷七百九十五",
        "no#": 77
    },
    {
        "title": "句",
        "author": "陈德诚",
        "biography": "建州人，百胜军节度使",
        "paragraphs": [
            "建水旧传刘夜坐，螺川新有夏江城。"
        ],
        "notes": [
            "（刘洞有《夜坐》诗，夏宝松有《宿江城》诗皆见称一时，号刘夜坐、夏江城云）"
        ],
        "volume": "卷七百九十五",
        "no#": 78
    },
    {
        "title": "句",
        "author": "陈甫",
        "biography": "字惟岳，吉州人",
        "paragraphs": [
            "一雨洗残暑，万家生早凉。"
        ],
        "notes": [
            "（《漳江感怀》）"
        ],
        "volume": "卷七百九十五",
        "no#": 79
    },
    {
        "title": "句",
        "author": "陈甫",
        "biography": "",
        "paragraphs": [
            "暮鸟归巢急，寒牛下陇迟。"
        ],
        "notes": [
            "（《村居》）"
        ],
        "volume": "卷七百九十五",
        "no#": 80
    },
    {
        "title": "句",
        "author": "陈甫",
        "biography": "",
        "paragraphs": [
            "算吟千百首，方得两三联。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百九十五",
        "no#": 81
    },
    {
        "title": "句",
        "author": "陈甫",
        "biography": "",
        "paragraphs": [
            "清时不作登龙客，绿鬓闲梳傍草堂。"
        ],
        "notes": [
            "（《赠黄岩》，并《雅言系述》）"
        ],
        "volume": "卷七百九十五",
        "no#": 82
    },
    {
        "title": "句",
        "author": "徐融",
        "biography": "南唐人",
        "paragraphs": [
            "淮船分蚁队，江市聚蝇声。"
        ],
        "notes": [
            "（《夜宿金山》，见《古今诗话》）"
        ],
        "volume": "卷七百九十五",
        "no#": 83
    },
    {
        "title": "句",
        "author": "高若拙",
        "biography": "荆南髙从诲幕客",
        "paragraphs": [
            "人间不见月，天外自分明。"
        ],
        "notes": [
            "（《中秋不见月》，见《大定录》）",
            "曾在荆南文献王高从诲担任幕客"
            
        ],
        "volume": "卷七百九十五",
        "no#": 84
    },
    {
        "title": "句",
        "author": "皮光业",
        "biography": "字文通，日休之子，呉越署为推官，后拜相",
        "paragraphs": [
            "烧平樵路出，潮落海山高。"
        ],
        "notes": [
            "（见《葆光录》）"
        ],
        "volume": "卷七百九十五",
        "no#": 85
    },
    {
        "title": "句",
        "author": "皮光业",
        "biography": "",
        "paragraphs": [
            "行人折柳和轻絮，飞燕衔泥带落花。"
        ],
        "notes": [
            "（见《西清诗话》）"
        ],
        "volume": "卷七百九十五",
        "no#": 86
    },
    {
        "title": "句",
        "author": "屠瓌智",
        "biography": "海盐人，越州指挥赠节使上柱国",
        "paragraphs": [
            "轻身都是义，殉主始为忠。"
        ],
        "notes": [
            "（《咏志》）"
        ],
        "volume": "卷七百九十五",
        "no#": 87
    },
    {
        "title": "句",
        "author": "元德昭",
        "biography": "字名逺，杭州人，吴越相",
        "paragraphs": [
            "满堂罗绮兼朱紫，四代儿孙奉老翁。"
        ],
        "notes": [
            "（德昭理家以孝爱闻，每时序置酒，环列几席者凡四从，故其诗云。见《备史》）"
        ],
        "volume": "卷七百九十五",
        "no#": 88
    },
    {
        "title": "句",
        "author": "林无隐",
        "biography": "闽人，寓明州吴越相鼎即其子也",
        "paragraphs": [
            "雪消二月江湖阔，花发千山道路香。"
        ],
        "notes": [
            "（见《备史》）",
            "吴越宰相林鼎之父"
        ],
        "volume": "卷七百九十五",
        "no#": 89
    },
    {
        "title": "句",
        "author": "杨义方",
        "biography": "眉山人，王建时举进士",
        "paragraphs": [
            "海边红日半离水，天外暖风轻到花。"
        ],
        "notes": [
            "（《春日》）"
        ],
        "volume": "卷七百九十五",
        "no#": 90
    },
    {
        "title": "句",
        "author": "杨义方",
        "biography": "",
        "paragraphs": [
            "雨声鞭自禁门出，一簇人从天上来。"
        ],
        "notes": [
            "（《赠王枢密处回》，见《异闻录》）"
        ],
        "volume": "卷七百九十五",
        "no#": 91
    },
    {
        "title": "句",
        "author": "王廷珪",
        "biography": "",
        "paragraphs": [
            "十字水中分岛屿，数重花外见楼台。"
        ],
        "notes": [
            "（浣花溪江皆创亭榭，孟昶游之，曰：「曲江金殿锁千门，未及此。」廷珪赋句，昶称善。见《蜀梼杌》）"
        ],
        "volume": "卷七百九十五",
        "no#": 92
    },
    {
        "title": "句",
        "author": "欧阳彬",
        "biography": "字齐羙，衡山人。孟㫤时，为嘉州刺史，后至䕫州节度",
        "paragraphs": [
            "无钱将乞樊知客，名纸生毛不为通。"
        ],
        "notes": [
            "（彬初投谒马殷，知客樊姓者索贿不得，不与通。后为《九州歌》干之，又不问，因入蜀。见《南部新书》）"
        ],
        "volume": "卷七百九十五",
        "no#": 93
    },
    {
        "title": "句",
        "author": "欧阳彬",
        "biography": "",
        "paragraphs": [
            "桑柘残阳里，儿孙落叶中。"
        ],
        "notes": [
            "（彬有子稚齿，作《田父诗》云云。廖凝见之，知其不寿，寻果卒。见《郡阁雅谈》）"
        ],
        "volume": "卷七百九十五",
        "no#": 94
    },
    {
        "title": "句",
        "author": "李尧夫",
        "biography": "蜀梓潼山人",
        "paragraphs": [
            "向外疑无地，其中别有天。"
        ],
        "notes": [
            "（《大内盆池》）"
        ],
        "volume": "卷七百九十五",
        "no#": 95
    },
    {
        "title": "句",
        "author": "李尧夫",
        "biography": "",
        "paragraphs": [
            "方外共推为道友，关中独自占诗家。"
        ],
        "notes": [
            "（《赠滕郎中》）"
        ],
        "volume": "卷七百九十五",
        "no#": 96
    },
    {
        "title": "句",
        "author": "李尧夫",
        "biography": "",
        "paragraphs": [
            "炎暑郁蒸无处避，凉风消息几时来。"
        ],
        "notes": [
            "（见《古今诗话》）"
        ],
        "volume": "卷七百九十五",
        "no#": 97
    },
    {
        "title": "句",
        "author": "石文德",
        "biography": "连州人，仕长沙，歴水部员外郎、融州刺史",
        "paragraphs": [
            "月沈湘浦冷，花谢汉宫秋。"
        ],
        "notes": [
            "（《挽马希范夫人》，见《五代史补》）"
        ],
        "volume": "卷七百九十五",
        "no#": 98
    },
    {
        "title": "句",
        "author": "戴偃",
        "biography": "金陵人，避地湘中，值希范大兴土木。自称𤣥黄子。献《渔父诗》百篇以讽，希范怒。迁之湖上",
        "paragraphs": [
            "须把咽喉吞世界，盖因奢侈致危亡。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百九十五",
        "no#": 99
    },
    {
        "title": "句",
        "author": "戴偃",
        "biography": "",
        "paragraphs": [
            "若须抛却便抛却，莫待风高更亦深。"
        ],
        "notes": [
            "（并《渔父》篇。见《五代史补》）"
        ],
        "volume": "卷七百九十五",
        "no#": 100
    },
    {
        "title": "句",
        "author": "张迥",
        "biography": "湖南诗人",
        "paragraphs": [
            "夜长灯影灭，天绕鹤声孤。"
        ],
        "notes": [
            "（见《吟窗杂录》）"
        ],
        "volume": "卷七百九十五",
        "no#": 101
    },
    {
        "title": "句",
        "author": "钟元章",
        "biography": "南汉员外郎",
        "paragraphs": [
            "金筈离弦三尺电，星髇破的一声雷。"
        ],
        "notes": [
            "（《吟窗杂录》）"
        ],
        "volume": "卷七百九十五",
        "no#": 102
    },
    {
        "title": "句",
        "author": "杨凫",
        "biography": "字舄之，闽人",
        "paragraphs": [
            "背日流泉生冻早，逆风归鸟入巢迟。"
        ],
        "notes": [
            "（《山中即事》，见《雅言系述》）"
        ],
        "volume": "卷七百九十五",
        "no#": 103
    },
    {
        "title": "句",
        "author": "王俊",
        "biography": "考功郎中",
        "paragraphs": [
            "胜日登楼望，山川一半春。"
        ],
        "notes": [
            "（《桂林逍遥楼》）"
        ],
        "volume": "卷七百九十五",
        "no#": 104
    },
    {
        "title": "句",
        "author": "薛沆",
        "biography": "庐州刺史",
        "paragraphs": [
            "也知别有风光主，花蕾枝枝似去年。"
        ],
        "notes": [
            "（《题藏舟浦花》，见《南部新书》）"
        ],
        "volume": "卷七百九十五",
        "no#": 105
    },
    {
        "title": "句",
        "author": "张𫖮",
        "biography": "官左司郎中",
        "paragraphs": [
            "金殿圣人看纵笔，玉堂词客尽裁诗。"
        ],
        "notes": [
            "（《赠𧦬光》）"
        ],
        "volume": "卷七百九十五",
        "no#": 106
    },
    {
        "title": "句",
        "author": "张林",
        "biography": "擢进士第官至御史",
        "paragraphs": [
            "菱叶乍翻人采后，芰荷初没舸行时。"
        ],
        "notes": [
            "（见《纪事》）"
        ],
        "volume": "卷七百九十五",
        "no#": 107
    },
    {
        "title": "句",
        "author": "卢休",
        "biography": "",
        "paragraphs": [
            "春寒酒力迟，冉冉生微红。"
        ],
        "notes": [
            "（《寒月》）"
        ],
        "volume": "卷七百九十五",
        "no#": 108
    },
    {
        "title": "句",
        "author": "卢休",
        "biography": "",
        "paragraphs": [
            "自然草木性，谁祝元化功。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百九十五",
        "no#": 109
    },
    {
        "title": "句",
        "author": "卢休",
        "biography": "",
        "paragraphs": [
            "湓浦风生破胆愁。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百九十五",
        "no#": 110
    },
    {
        "title": "句",
        "author": "卢休",
        "biography": "",
        "paragraphs": [
            "血染剑花明帐幕，三千车马出渔阳。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百九十五",
        "no#": 111
    },
    {
        "title": "句",
        "author": "卢休",
        "biography": "",
        "paragraphs": [
            "入门堪笑复堪怜，三径苔荒一钓船。"
        ],
        "notes": [
            "（并见张为《主客图》）"
        ],
        "volume": "卷七百九十五",
        "no#": 112
    },
    {
        "title": "句",
        "author": "陈峤",
        "biography": "字景山，闽人，暮年登第，还乡不仕",
        "paragraphs": [
            "小桥风月年年事，争奈潘郎去□何。"
        ],
        "notes": [
            "见《南部新书》，第二句缺一字"
        ],
        "volume": "卷七百九十五",
        "no#": 113
    },
    {
        "title": "句",
        "author": "李范",
        "biography": "闗中人",
        "paragraphs": [
            "清猿啼远木，白鸟下前滩。"
        ],
        "notes": [
            "《秋日江干逺望》"
        ],
        "volume": "卷七百九十五",
        "no#": 114
    },
    {
        "title": "句",
        "author": "李范",
        "biography": "",
        "paragraphs": [
            "鹤归秋汉远，人去草堂空。"
        ],
        "notes": [
            "《经王山人旧居》"
        ],
        "volume": "卷七百九十五",
        "no#": 115
    },
    {
        "title": "句",
        "author": "李范",
        "biography": "",
        "paragraphs": [
            "虽当南北路，不碍往来人。"
        ],
        "notes": [
            "《道傍树》"
        ],
        "volume": "卷七百九十五",
        "no#": 116
    },
    {
        "title": "句",
        "author": "李范",
        "biography": "",
        "paragraphs": [
            "钓叟无机沙鸟睡，禅师入定白牛闲。"
        ],
        "notes": [
            "《江寺闲书》"
        ],
        "volume": "卷七百九十五",
        "no#": 117
    },
    {
        "title": "句",
        "author": "李范",
        "biography": "",
        "paragraphs": [
            "天涯故友无来信，窗外拒霜空落花。"
        ],
        "notes": [
            "（《暮秋怀故人》，并见《雅言杂载》）"
        ],
        "volume": "卷七百九十五",
        "no#": 118
    },
    {
        "title": "句",
        "author": "卞震",
        "biography": "蜀人",
        "paragraphs": [
            "雨壁长秋菌，风枝落病蝉。"
        ],
        "notes": [
            "《即事》"
        ],
        "volume": "卷七百九十五",
        "no#": 119
    },
    {
        "title": "句",
        "author": "卞震",
        "biography": "",
        "paragraphs": [
            "老笻搘瘦影，寒木凭吟身。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百九十五",
        "no#": 120
    },
    {
        "title": "句",
        "author": "卞震",
        "biography": "",
        "paragraphs": [
            "诗债到春无处避，离愁因醉暂时无。"
        ],
        "notes": [
            "《春日偶题》"
        ],
        "volume": "卷七百九十五",
        "no#": 121
    },
    {
        "title": "句",
        "author": "卞震",
        "biography": "",
        "paragraphs": [
            "茶香解睡磨铛煮，山色牵怀著屐登。"
        ],
        "notes": [
            "《即事》"
        ],
        "volume": "卷七百九十五",
        "no#": 122
    },
    {
        "title": "句",
        "author": "林楚才",
        "biography": "贺州富川人",
        "paragraphs": [
            "身闲不恨辞官早，诗好常甘得句迟。"
        ],
        "notes": [
            "（《赠致仕黄损》，见《雅言系述》"
        ],
        "volume": "卷七百九十五",
        "no#": 123
    },
    {
        "title": "句",
        "author": "孟不疑",
        "biography": "有诗名，溺于游览，不复应举",
        "paragraphs": [
            "白日故乡远，青山佳句中。"
        ],
        "notes": [
            "（见《酉阳杂俎》）"
        ],
        "volume": "卷七百九十五",
        "no#": 124
    },
    {
        "title": "句",
        "author": "厐季子",
        "biography": "",
        "paragraphs": [
            "冥云生易满，秋草长难高。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百九十五",
        "no#": 125
    },
    {
        "title": "句",
        "author": "郭思",
        "biography": "",
        "paragraphs": [
            "落星一石几千年，门外何人扣汉川。"
        ],
        "notes": [
            "（《白石镇古城》）"
        ],
        "volume": "卷七百九十五",
        "no#": 126
    },
    {
        "title": "句",
        "author": "卢载",
        "biography": "",
        "paragraphs": [
            "五千里地望皆见，七十二峰中最高。"
        ],
        "notes": [
            "（《祝融峰》）"
        ],
        "volume": "卷七百九十五",
        "no#": 127
    },
    {
        "title": "句",
        "author": "郑翱",
        "biography": "",
        "paragraphs": [
            "花烧落第眼，雨破到家程。"
        ],
        "notes": [
            "（下第东归，见《海录碎事》）"
        ],
        "volume": "卷七百九十五",
        "no#": 128
    },
    {
        "title": "句",
        "author": "郑说",
        "biography": "",
        "paragraphs": [
            "架上紫衣闲不著，案头金字坐长看。"
        ],
        "notes": [
            "（《赠温州大云寺僧鸿楚》，见《高僧传》）"
        ],
        "volume": "卷七百九十五",
        "no#": 129
    },
    {
        "title": "句",
        "author": "史瑜",
        "biography": "",
        "paragraphs": [
            "溪从沮水流嶓冢，岭接青泥入剑天。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百九十五",
        "no#": 130
    },
    {
        "title": "句",
        "author": "贺公",
        "biography": "石晋兵部",
        "paragraphs": [
            "但存方寸地，留与子孙耕。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百九十五",
        "no#": 131
    },
    {
        "title": "句",
        "author": "米都知",
        "biography": "伶人，也善骚雅，梁补阙尝赠以诗",
        "paragraphs": [
            "小旗村店酒，微雨野塘花。"
        ],
        "notes": [
            "骚雅：《离骚》与《诗经》中《大雅》﹑《小雅》的并称",
            "补阙：唐代言官",
            "（见《南部新书》）"
        ],
        "volume": "卷七百九十五",
        "no#": 132
    },
    {
        "title": "句",
        "author": "陈秀才",
        "biography": "",
        "paragraphs": [
            "地偏云自去，日暮山更深。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百九十五",
        "no#": 133
    },
    {
        "title": "句",
        "author": "崔李二生",
        "biography": "",
        "paragraphs": [
            "恰似传花人饮散，空抛床下最繁枝。--崔",
            "艳魄香魂如有在，还应羞见堕楼人。--李"
        ],
        "notes": [
            "（二生并与武椽游。歩非烟死，同赋其事）",
            "崔李二生为唐代小说《非烟传》中人物名，二人同与武公业游，武公业妾步非烟死，二人作诗吟其事"

        ],
        "volume": "卷七百九十五",
        "no#": 134
    },
    {
        "title": "句",
        "author": "范氏子",
        "biography": "《纪事》云，吴人摅子，七岁能诗。方干，见其《赠隠者》句，曰：「此子他日必成名。」又吟《夏日》句，干云：「必不享寿！」。果十岁卒。摅，即撰《云溪友议》者",
        "paragraphs": [
            "扫叶随风便，浇花趁日阴。"
        ],
        "notes": [
            "（《赠隠者》）",
            "范氏子：范摅之子",
            "摅：范摅",
            "见《尧山堂外纪：卷三十六唐》"
        ],
        "volume": "卷七百九十五",
        "no#": 135
    },
    {
        "title": "句",
        "author": "范氏子",
        "biography": "",
        "paragraphs": [
            "闲云生不雨，病叶落非秋。"
        ],
        "notes": [
            "（《夏日》）"
        ],
        "volume": "卷七百九十五",
        "no#": 136
    },
    {
        "title": "句",
        "author": "临川小吏",
        "biography": "李建勲镇临川，赏牡丹。有小吏捧砚，举止有士人风。公曰：「学诗乎？」对曰：「麤亲笔砚。」因令口占一篇。公竒之，勉令就学。因成诗名",
        "paragraphs": [
            "三月能辞千日醉，一生能得几回看。"
        ],
        "notes": [
            "（见《诗话总龟》）",
            "见《诗话总龟前集》卷三"
        ],
        "volume": "卷七百九十五",
        "no#": 137
    },
    {
        "title": "句",
        "author": "韩熙载客",
        "biography": "",
        "paragraphs": [
            "最是五更留不住，向人枕畔著衣裳。"
        ],
        "notes": [
            "（熙载婢妾甚多，不为防闲，往往私出侍客，客赋诗云云。见《南唐近事》）"
        ],
        "volume": "卷七百九十五",
        "no#": 138
    },
    {
        "title": "句",
        "author": "段义宗",
        "biography": "外夷",
        "paragraphs": [
            "悬心秋夜月，万里照乡关。"
        ],
        "notes": [
            "（《思乡》）"
        ],
        "volume": "卷七百九十五",
        "no#": 139
    },
    {
        "title": "句",
        "author": "段义宗",
        "biography": "",
        "paragraphs": [
            "玉排复道珊瑚殿，金错危椆翡翠楼。"
        ],
        "notes": [
            "（《题三学院经楼》）"
        ],
        "volume": "卷七百九十五",
        "no#": 140
    },
    {
        "title": "句",
        "author": "段义宗",
        "biography": "",
        "paragraphs": [
            "此花不与众花同，为感高僧护法功。"
        ],
        "notes": [
            "（《大慈寺芍药》，并《吟窗杂录》）"
        ],
        "volume": "卷七百九十五",
        "no#": 141
    }
]