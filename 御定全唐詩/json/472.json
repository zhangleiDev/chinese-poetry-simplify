[
    {
        "title": "奉陪吕使君楼上夜看花",
        "author": "段弘古",
        "biography": "",
        "paragraphs": [
            "城上芳园花满枝，城头太守夜看时。",
            "为报林中高举烛，感人情思欲题诗。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百七十二",
        "no#": 1
    },
    {
        "title": "所居寺院凉夜书情，呈上吕和叔温郎中",
        "author": "何元上",
        "biography": "",
        "paragraphs": [
            "庾公念病宜清暑，遣向僧家占上方。月光似水衣裳湿，",
            "何由一示云霄路，肠断星星两鬓霜。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百七十二",
        "no#": 2
    },
    {
        "title": "塞上闻笛",
        "author": "宋济",
        "biography": "",
        "paragraphs": [
            "胡儿吹笛戍楼间，楼上萧条海月闲。",
            "借问梅花何处落，风吹一夜满关山。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百七十二",
        "no#": 3
    },
    {
        "title": "东邻美人歌",
        "author": "宋济",
        "biography": "",
        "paragraphs": [
            "花暖江城斜日阴，莺啼绣户晓云深。",
            "春风不道珠帘隔，传得歌声与客心。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百七十二",
        "no#": 4
    },
    {
        "title": "题李八百洞",
        "author": "符载",
        "biography": "",
        "paragraphs": [
            "太极之年混沌坼，此山亦是神仙宅。",
            "后世何人来飞升，紫阳真人李八百。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百七十二",
        "no#": 5
    },
    {
        "title": "甘州歌",
        "author": "符载",
        "biography": "",
        "paragraphs": [
            "月里嫦娥不画眉，只将云雾作罗衣。",
            "不知梦逐青鸾去，犹把花枝盖面归。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百七十二",
        "no#": 6
    },
    {
        "title": "句",
        "author": "符载",
        "biography": "",
        "paragraphs": [
            "绿迸穿篱笋，红飘隔户花。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百七十二",
        "no#": 7
    },
    {
        "title": "贞元八年十二月谒先主庙绝句三首",
        "author": "张俨",
        "biography": "",
        "paragraphs": [
            "仗顺继皇业，并吞势由己。天命屈雄图，谁歌大风起。",
            "得股肱贤明，能以奇用兵。何事伤客情，何人归帝京。",
            "雄名垂竹帛，荒陵压阡陌。终古更何闻，悲风入松柏。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百七十二",
        "no#": 8
    },
    {
        "title": "题安乐山",
        "author": "先汪",
        "biography": "",
        "paragraphs": [
            "碧峰横倚白云端，隋氏真人化迹残。",
            "翠柏不凋龙骨瘦，石泉犹在镜光寒。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百七十二",
        "no#": 9
    },
    {
        "title": "姑熟杂咏：姑熟溪",
        "author": "李赤",
        "biography": "",
        "paragraphs": [
            "爱此溪水闲，乘流兴无极。击楫怕鸥惊，垂竿待鱼食。",
            "波翻晓霞影，岸叠春山色。何处浣纱人，红颜未相识。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百七十二",
        "no#": 10
    },
    {
        "title": "姑熟杂咏：丹阳湖",
        "author": "李赤",
        "biography": "",
        "paragraphs": [
            "湖与元气通，风波浩难止。天外贾客归，云间片帆起。",
            "龟游莲叶上，鸟宿芦花里。少女棹舟归，歌声逐流水。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百七十二",
        "no#": 11
    },
    {
        "title": "姑熟杂咏：谢公宅",
        "author": "李赤",
        "biography": "",
        "paragraphs": [
            "青山日将暝，寂寞谢公宅。竹里无人声，池中虚月白。",
            "荒庭衰草遍，废井苍苔积。唯有清风闻，时时起泉石。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百七十二",
        "no#": 12
    },
    {
        "title": "姑熟杂咏：凌歊台",
        "author": "李赤",
        "biography": "",
        "paragraphs": [
            "旷望登古台，台高极人目。叠嶂列远空，闲花杂平陆。",
            "白云入窗牖，野翠生松竹。欲览碑上文，苔侵岂堪读。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百七十二",
        "no#": 13
    },
    {
        "title": "姑熟杂咏：桓公井",
        "author": "李赤",
        "biography": "",
        "paragraphs": [
            "桓公名已古，废井曾未竭。石甃冷苍苔，寒泉湛孤月。",
            "秋来桐暂落，春至桃还发。路远人罕窥，谁能见清澈。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百七十二",
        "no#": 14
    },
    {
        "title": "姑熟杂咏：慈姥竹",
        "author": "李赤",
        "biography": "",
        "paragraphs": [
            "野竹攒石生，含烟映江岛。翠色落波深，虚声带寒早。",
            "龙吟曾未听，凤曲吹应好。不学蒲柳凋，贞心常自保。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百七十二",
        "no#": 15
    },
    {
        "title": "姑熟杂咏：望夫山",
        "author": "李赤",
        "biography": "",
        "paragraphs": [
            "颙望临碧空，怨情感离别。芳草不知愁，岩花但争发。",
            "云山万重隔，音信千里绝。春去秋复来，相思几时歇。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百七十二",
        "no#": 16
    },
    {
        "title": "姑熟杂咏：牛渚矶",
        "author": "李赤",
        "biography": "",
        "paragraphs": [
            "绝壁临巨川，连峰势相向。乱石流洑间，回波自成浪。",
            "但惊群木秀，莫测精灵状。更听猿夜啼，忧心醉江上。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百七十二",
        "no#": 17
    },
    {
        "title": "姑熟杂咏：灵墟山",
        "author": "李赤",
        "biography": "",
        "paragraphs": [
            "丁令辞世人，拂衣向仙路。伏炼九丹成，方随五云去。",
            "松萝蔽幽洞，桃杏深隐处。不知曾化鹤，辽海归几度。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百七十二",
        "no#": 18
    },
    {
        "title": "姑熟杂咏：天门山",
        "author": "李赤",
        "biography": "",
        "paragraphs": [
            "迥出江水上，双峰自相对。岸映松色寒，石分浪花碎。",
            "参差远天际，缥缈晴霞外。落日舟去遥，回首沈青霭。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百七十二",
        "no#": 19
    },
    {
        "title": "敕赠康尚书美人",
        "author": "薛zm",
        "biography": "",
        "paragraphs": [
            "天门喜气晓氛氲，圣主临轩召冠军。",
            "欲令从此行霖雨，先赐巫山一片云。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百七十二",
        "no#": 20
    },
    {
        "title": "将赴东都上李相国",
        "author": "孙叔向",
        "biography": "",
        "paragraphs": [
            "四海兵初偃，平津阁正开。谁知大炉火，还有不然灰。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百七十二",
        "no#": 21
    },
    {
        "title": "题昭应温泉",
        "author": "孙叔向",
        "biography": "",
        "paragraphs": [
            "一道温泉绕御楼，先皇曾向此中游。",
            "虽然水是无情物，也到宫前咽不流。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百七十二",
        "no#": 22
    },
    {
        "title": "送咸安公主",
        "author": "孙叔向",
        "biography": "",
        "paragraphs": [
            "卤簿迟迟出国门，汉家公主嫁乌孙。",
            "玉颜便向穹庐去，卫霍空承明主恩。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百七十二",
        "no#": 23
    },
    {
        "title": "边城柳",
        "author": "刘皂",
        "biography": "",
        "paragraphs": [
            "一株新柳色，十里断孤城。为近东西路，长悬离别情。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百七十二",
        "no#": 24
    },
    {
        "title": "长门怨三首",
        "author": "刘皂",
        "biography": "",
        "paragraphs": [
            "雨滴长门秋夜长，愁心和雨到昭阳。",
            "泪痕不学君恩断，拭却千行更万行。",
            "宫殿沉沉月欲分，昭阳更漏不堪闻。",
            "珊瑚枕上千行泪，不是思君是恨君。",
            "蝉鬓慵梳倚帐门，蛾眉不扫惯承恩。",
            "旁人未必知心事，一面残妆空泪痕。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百七十二",
        "no#": 25
    },
    {
        "title": "旅次朔方",
        "author": "刘皂",
        "biography": "",
        "paragraphs": [
            "客舍并州数十霜，归心日夜忆咸阳。",
            "无端又渡桑干水，却望并州似故乡。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百七十二",
        "no#": 26
    },
    {
        "title": "早起",
        "author": "杨厚",
        "biography": "",
        "paragraphs": [
            "星汉转寒更，伊余索寞情。钟催归梦断，雁引远愁生。",
            "危壁兰光暗，疏帘露气清。闲庭聊一望，海日未分明。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百七十二",
        "no#": 27
    },
    {
        "title": "长门怨",
        "author": "裴交泰",
        "biography": "",
        "paragraphs": [
            "自闭长门经几秋，罗衣湿尽泪还流。",
            "一种蛾眉明月夜，南宫歌管北宫愁。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百七十二",
        "no#": 28
    },
    {
        "title": "禁中送任山人",
        "author": "李秘",
        "biography": "",
        "paragraphs": [
            "子去非长往，君恩取大还。补天留彩石，缩地入青山。",
            "献寿千春外，来朝数月间。莫抛残药物，窃取驻童颜。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百七十二",
        "no#": 29
    },
    {
        "title": "府试中元观道流步虚",
        "author": "殷尧恭",
        "biography": "",
        "paragraphs": [
            "玄都开秘箓，白石礼先生。上界秋光静，中元夜景清。",
            "星辰朝帝处，鸾鹤步虚声。玉洞花长发，珠宫月最明。",
            "扫坛天地肃，投简鬼神惊。倘赐刀圭药，还成不死名。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百七十二",
        "no#": 30
    },
    {
        "title": "王仙坛",
        "author": "林杰",
        "biography": "",
        "paragraphs": [
            "羽客已登仙路去，丹炉草木尽凋残。",
            "不知千载归何日，空使时人扫旧坛。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百七十二",
        "no#": 31
    },
    {
        "title": "乞巧",
        "author": "林杰",
        "biography": "",
        "paragraphs": [
            "七夕今宵看碧霄，牵牛织女渡河桥。",
            "家家乞巧望秋月，穿尽红丝几万条。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百七十二",
        "no#": 32
    },
    {
        "title": "句",
        "author": "林杰",
        "biography": "",
        "paragraphs": [
            "金盘摘下挂朱颗，红壳开时饮玉浆。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百七十二",
        "no#": 33
    },
    {
        "title": "哭林杰",
        "author": "郑立之",
        "biography": "",
        "paragraphs": [
            "才高未及贾生年，何事孤魂逐逝川。",
            "萤聚帐中人已去，鹤离台上月空圆。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百七十二",
        "no#": 34
    },
    {
        "title": "咏和亲",
        "author": "苏郁",
        "biography": "",
        "paragraphs": [
            "关月夜悬青冢镜，寒云秋薄汉宫罗。",
            "君王莫信和亲策，生得胡雏虏更多。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百七十二",
        "no#": 35
    },
    {
        "title": "鹦鹉词",
        "author": "苏郁",
        "biography": "",
        "paragraphs": [
            "莫把金笼闭鹦鹉，个个分明解人语。",
            "忽然更向君前言，三十六宫愁几许。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百七十二",
        "no#": 36
    },
    {
        "title": "步虚词",
        "author": "苏郁",
        "biography": "",
        "paragraphs": [
            "十二楼藏玉堞中，凤凰双宿碧芙蓉。",
            "流霞浅酌谁同醉，今夜笙歌第几重。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百七十二",
        "no#": 37
    },
    {
        "title": "句",
        "author": "苏郁",
        "biography": "",
        "paragraphs": [
            "吟倚雨残树，月收山下村。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百七十二",
        "no#": 38
    },
    {
        "title": "赋得琢玉成器",
        "author": "浩虚舟",
        "biography": "",
        "paragraphs": [
            "已沐识坚贞，应怜器未成。辉山方可重，散璞乍堪惊。",
            "玷灭随心正，瑕消夺眼明。琢磨虹气在，拂拭水容生。",
            "赏玩冰光冷，提携月魄轻。伫当亲捧握，瑚琏幸齐名。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百七十二",
        "no#": 39
    },
    {
        "title": "责商山四皓",
        "author": "蔡京",
        "biography": "",
        "paragraphs": [
            "秦末家家思逐鹿，商山四皓独忘机。",
            "如何鬓发霜相似，更出深山定是非。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百七十二",
        "no#": 40
    },
    {
        "title": "假节邕交道由吴溪",
        "author": "蔡京",
        "biography": "",
        "paragraphs": [
            "停桡横水中，举目孤烟外。借问吴溪人，谁家有山卖。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百七十二",
        "no#": 41
    },
    {
        "title": "咏子规",
        "author": "蔡京",
        "biography": "",
        "paragraphs": [
            "千年冤魄化为禽，永逐悲风叫远林。愁血滴花春艳死，",
            "肠断楚词归不得，剑门迢递蜀江深。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百七十二",
        "no#": 42
    },
    {
        "title": "献蔡京",
        "author": "张顶",
        "biography": "",
        "paragraphs": [
            "抛却长竿卷却丝，手持蓑笠献新诗。",
            "临川太守清如镜，不是渔人下钓时。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百七十二",
        "no#": 43
    }
]