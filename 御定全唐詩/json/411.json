[
    {
        "title": "杏园",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "浩浩长安车马尘，狂风吹送每年春。",
            "门前本是虚空界，何事栽花误世人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十一",
        "no#": 1
    },
    {
        "title": "菊花",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "秋丛绕舍似陶家，遍绕篱边日渐斜。",
            "不是花中偏爱菊，此花开尽更无花。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十一",
        "no#": 2
    },
    {
        "title": "酬哥舒大少府寄同年科第",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "前年科第偏年少，未解知羞最爱狂。九陌争驰好鞍马，",
            "赖得官闲且疏散，到君花下忆诸郎。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十一",
        "no#": 3
    },
    {
        "title": "幽栖",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "野人自爱幽栖所，近对长松远是山。尽日望云心不系，",
            "辽海若思千岁鹤，且留城市会飞还。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十一",
        "no#": 4
    },
    {
        "title": "清都春霁，寄胡三、吴十一",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "蕊珠宫殿经微雨，草树无尘耀眼光。白日当空天气暖，",
            "时节催年春不住，武陵花谢忆诸郎。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十一",
        "no#": 5
    },
    {
        "title": "华岳寺",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "山前古寺临长道，来往淹留为爱山。双燕营巢始西别，",
            "羞见窦师无外役，竹窗依旧老身闲。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十一",
        "no#": 6
    },
    {
        "title": "天坛上境",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "野人性僻穷深僻，芸署官闲不似官。万里洞中朝玉帝，",
            "因为南昌检仙籍，马君家世奉还丹。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十一",
        "no#": 7
    },
    {
        "title": "寻西明寺僧不在",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "春来日日到西林，飞锡经行不可寻。",
            "莲池旧是无波水，莫逐狂风起浪心。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十一",
        "no#": 8
    },
    {
        "title": "与吴侍御春游",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "苍龙阙下陪骢马，紫阁峰头见白云。",
            "满眼流光随日度，今朝花落更纷纷。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十一",
        "no#": 9
    },
    {
        "title": "晚春",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "昼静帘疏燕语频，双双斗雀动阶尘。",
            "柴扉日暮随风掩，落尽闲花不见人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十一",
        "no#": 10
    },
    {
        "title": "先醉",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "今日樽前败饮名，三杯未尽不能倾。",
            "怪来花下长先醉，半是春风荡酒情。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十一",
        "no#": 11
    },
    {
        "title": "独醉",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "一树芳菲也当春，漫随车马拥行尘。",
            "桃花解笑莺能语，自醉自眠那藉人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十一",
        "no#": 12
    },
    {
        "title": "宿醉",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "风引春心不自由，等闲冲席饮多筹。",
            "朝来始向花前觉，度却醒时一夜愁。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十一",
        "no#": 13
    },
    {
        "title": "惧醉",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "闻道秋来怯夜寒，不辞泥水为杯盘。",
            "殷勤惧醉有深意，愁到醒时灯火阑。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十一",
        "no#": 14
    },
    {
        "title": "羡醉",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "绮陌高楼竞醉眠，共期憔悴不相怜。",
            "也应自有寻春日，虚度而今正少年。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十一",
        "no#": 15
    },
    {
        "title": "忆醉",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "自叹旅人行意速，每嫌杯酒缓归期。",
            "今朝偏遇醒时别，泪落风前忆醉时。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十一",
        "no#": 16
    },
    {
        "title": "病醉",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "醉伴见侬因病酒，道侬无酒不相窥。",
            "那知下药还沽底，人去人来剩一卮。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十一",
        "no#": 17
    },
    {
        "title": "《拟醉",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "九月闲宵初向火，一尊清酒始行杯。",
            "怜君城外遥相忆，冒雨冲泥黑地来。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十一",
        "no#": 18
    },
    {
        "title": "劝醉",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "窦家能酿销愁酒，但是愁人便与销。",
            "愿我共君俱寂寞，只应连夜复连朝。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十一",
        "no#": 19
    },
    {
        "title": "任醉",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "本怕酒醒浑不饮，因君相劝觉情来。",
            "殷勤满酌从听醉，乍可欲醒还一杯。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十一",
        "no#": 20
    },
    {
        "title": "同醉",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "柏树台中推事人，杏花坛上炼形真。",
            "心源一种闲如水，同醉樱桃林下春。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十一",
        "no#": 21
    },
    {
        "title": "狂醉",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "一自柏台为御史，二年辜负两京春。",
            "岘亭今日颠狂醉，舞引红娘乱打人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十一",
        "no#": 22
    },
    {
        "title": "伴僧行",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "春来求事百无成，因向愁中识道情。",
            "花满杏园千万树，几人能伴老僧行。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十一",
        "no#": 23
    },
    {
        "title": "古寺",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "古寺春余日半斜，竹风萧爽胜人家。",
            "花时不到有花院，意在寻僧不在花。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十一",
        "no#": 24
    },
    {
        "title": "定僧",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "落魄闲行不著家，遍寻春寺赏年华。",
            "野僧偶向花前定，满树狂风满树花。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十一",
        "no#": 25
    },
    {
        "title": "观心处",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "满坐喧喧笑语频，独怜方丈了无尘。",
            "灯前便是观心处，要似观心有几人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十一",
        "no#": 26
    },
    {
        "title": "智度师二首",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "四十年前马上飞，功名藏尽拥禅衣。",
            "石榴园下擒生处，独自闲行独自归。",
            "三陷思明三突围，铁衣抛尽衲禅衣。",
            "天津桥上无人识，闲凭栏干望落晖。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十一",
        "no#": 27
    },
    {
        "title": "西明寺牡丹",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "花向琉璃地上生，光风炫转紫云英。",
            "自从天女盘中见，直至今朝眼更明。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十一",
        "no#": 28
    },
    {
        "title": "忆杨十二",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "去时芍药才堪赠，看却残花已度春。",
            "只为情深偏怆别，等闲相见莫相亲。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十一",
        "no#": 29
    },
    {
        "title": "送复梦赴韦令幕",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "世上于今重检身，吾徒耽酒作狂人。",
            "西曹旧事多持法，慎莫吐他丞相茵。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十一",
        "no#": 30
    },
    {
        "title": "送刘太白",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "洛阳大底居人少，从善坊西最寂寥。",
            "想得刘君独骑马，古堤愁树隔中桥。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十一",
        "no#": 31
    },
    {
        "title": "奉诚园",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "萧相深诚奉至尊，旧居求作奉诚园。",
            "秋来古巷无人扫，树满空墙闭戟门。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十一",
        "no#": 32
    },
    {
        "title": "与太白同之东洛至栎阳太白染疾驻行予九月二十五日至华岳寺雪后望山",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "共作洛阳千里伴，老刘因疾驻行轩。",
            "今朝独自山前立，雪满三峰倚寺门。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十一",
        "no#": 33
    },
    {
        "title": "野狐泉柳林",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "去日野狐泉上柳，紫牙初绽拂眉低。",
            "秋来寥落惊风雨，叶满空林踏作泥。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十一",
        "no#": 34
    },
    {
        "title": "酬胡三凭人问牡丹",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "窃见胡三问牡丹，为言依旧满西栏。",
            "花时何处偏相忆，寥落衰红雨后看。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十一",
        "no#": 35
    },
    {
        "title": "酬乐天秋兴见赠本句云莫怪独吟秋兴苦比君校近二毛年",
        "author": "元稹",
        "biography": "",
        "paragraphs": [
            "劝君休作悲秋赋，白发如星也任垂。",
            "毕竟百年同是梦，长年何异少何为。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百一十一",
        "no#": 36
    }
]