[
    {
        "title": "和太原张相公山亭怀古",
        "author": "崔公信",
        "biography": "",
        "paragraphs": [
            "叠石状崖𪩘，翠含城上楼。前移庐霍峰，远带沅湘流。",
            "潇洒主人静，夤缘芳径幽。清辉在昏旦，岂异东山游。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百八十四",
        "no#": 1
    },
    {
        "title": "过小妓英英墓",
        "author": "杨虞卿",
        "biography": "",
        "paragraphs": [
            "萧晨骑马出皇都，闻说埋冤在路隅。别我已为泉下土，",
            "兰质蕙心何所在，焉知过者是狂夫。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百八十四",
        "no#": 2
    },
    {
        "title": "句",
        "author": "杨虞卿",
        "biography": "",
        "paragraphs": [
            "河势昆仑远，山形菡萏秋。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百八十四",
        "no#": 3
    },
    {
        "title": "和段相公登武担寺西台",
        "author": "杨汝士",
        "biography": "",
        "paragraphs": [
            "清净此道宫，层台复倚空。偶时三伏外，列席九霄中。",
            "平视云端路，高临树杪风。自怜荣末座，前日别池笼。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百八十四",
        "no#": 4
    },
    {
        "title": "和段相公夏登张仪楼",
        "author": "杨汝士",
        "biography": "",
        "paragraphs": [
            "从公城上来，秋近绝纤埃。楼古秦规在，江分蜀望开。",
            "远山标宿雪，末席本寒灰。陪赏今为忝，临欢敢诉杯。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百八十四",
        "no#": 5
    },
    {
        "title": "和宗人尚书嗣复祠祭武侯毕，题临淮公旧碑",
        "author": "杨汝士",
        "biography": "",
        "paragraphs": [
            "古柏森然地，修严蜀相祠。一过荣异代，三顾盛当时。",
            "功德流何远，馨香荐未衰。敬名探国志，饰像慰甿思。",
            "昔谒从征盖，今闻拥信旗。固宜光宠下，有泪刻前碑。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百八十四",
        "no#": 6
    },
    {
        "title": "宴杨仆射新昌里第",
        "author": "杨汝士",
        "biography": "",
        "paragraphs": [
            "隔坐应须赐御屏，尽将仙翰入高冥。文章旧价留鸾掖，",
            "当时疏广虽云盛，讵有兹筵醉绿醽。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百八十四",
        "no#": 7
    },
    {
        "title": "建节后偶作",
        "author": "杨汝士",
        "biography": "",
        "paragraphs": [
            "抛却弓刀上砌台，上方台榭与云开。",
            "山僧见我衣裳窄，知道新从战地来。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百八十四",
        "no#": 8
    },
    {
        "title": "题画山水",
        "author": "杨汝士",
        "biography": "",
        "paragraphs": [
            "太华峰前是故乡，路人遥指读书堂。",
            "如今老大骑官马，羞向关西道姓杨。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百八十四",
        "no#": 9
    },
    {
        "title": "贺筵占赠营妓",
        "author": "杨汝士",
        "biography": "",
        "paragraphs": [
            "郎君得意及青春，蜀国将军又不贫。",
            "一曲高歌红一匹，两头娘子谢夫人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百八十四",
        "no#": 10
    },
    {
        "title": "句",
        "author": "杨汝士",
        "biography": "",
        "paragraphs": [
            "昔日兰亭无艳质，此时金谷有高人。（裴令公居守东洛，",
            "士云云。白知不能加，遽裂之，曰：「笙歌鼎沸，勿作",
            "冷淡生活。」元顾白曰：「乐天所谓能全其名者也。」）"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百八十四",
        "no#": 11
    },
    {
        "title": "赋得芙蓉出水",
        "author": "陈至",
        "biography": "",
        "paragraphs": [
            "菡萏迎秋吐，夭摇映水滨。剑芒开宝匣，峰影写蒲津。",
            "下覆参差荇，高辞苒弱苹。自当巢翠甲，非止戏赪鳞。",
            "莫以时先后，而言色故新。芳香正堪玩，谁报涉江人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百八十四",
        "no#": 12
    },
    {
        "title": "荐冰",
        "author": "陈至",
        "biography": "",
        "paragraphs": [
            "凌寒开涸冱，寝庙致精诚。色静澄三酒，光寒肃两楹。",
            "形盐非近进，玉豆为潜英。礼自春分展，坚从北陆成。",
            "藉茅心共结，出鉴水渐明。幸得来观荐，灵台一小生。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百八十四",
        "no#": 13
    },
    {
        "title": "句",
        "author": "陈至",
        "biography": "",
        "paragraphs": [
            "藻井尚寒龙迹在，红楼初施日光通。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百八十四",
        "no#": 14
    },
    {
        "title": "荐冰",
        "author": "赵蕃",
        "biography": "",
        "paragraphs": [
            "仲月开凌室，斋心感圣情。寒姿分玉坐，皓彩发丹楹。",
            "积素因风壮，虚空向日明。遥涵窗户冷，近映冕旒清。",
            "在掌光逾澈，当轩质自轻。良辰方可致，由此表精诚。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百八十四",
        "no#": 15
    },
    {
        "title": "老人星",
        "author": "赵蕃",
        "biography": "",
        "paragraphs": [
            "大史占南极，秋分见寿星。增辉延宝历，发曜起祥经。",
            "灼烁依狼地，昭彰近帝庭。高悬方杳杳，孤白乍荧荧。",
            "应见光新吐，休征德自形。既能符圣祚，从此表遐龄。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷四百八十四",
        "no#": 16
    }
]