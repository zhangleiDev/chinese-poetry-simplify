[
    {
        "title": "登毗陵青山楼",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "高楼闲上对晴空，豁目开襟半日中。千里吴山清不断，",
            "吟罢倚栏何限意，回头城郭暮烟笼。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 1
    },
    {
        "title": "晋陵罢任寓居，依韵和陈锐秀才见寄",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "掩门三径莓苔绿，车马谁来陋巷间。卧弃琴书公干病，",
            "济物未能伸一术，敢于明代爱青山。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 2
    },
    {
        "title": "春苔",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "春霖催得锁烟浓，竹院莎斋径小通。",
            "谁爱落花风味处，莫愁门巷衬残红。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 3
    },
    {
        "title": "夏云",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "如峰形状在西郊，未见从龙上泬寥。",
            "多谢好风吹起后，化为甘雨济田苗。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 4
    },
    {
        "title": "书夏秀才幽居壁",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "永巷苔深户半开，床头书剑积尘埃。",
            "最怜小槛疏篁晚，幽鸟双双何处来。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 5
    },
    {
        "title": "红花",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "红花颜色掩千花，任是猩猩血未加。",
            "染出轻罗莫相贵，古人崇俭诫奢华。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 6
    },
    {
        "title": "安福县秋吟寄陈锐秘书",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "县庭事简得余功，诗兴秋来不可穷。卧听寒蛩莎砌月，",
            "苦恨交亲多契阔，未知良会几时同。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 7
    },
    {
        "title": "新喻县酬王仲华少府见贻",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "事简开樽有逸情，共忻官舍月华清。每惭花欠河阳景，",
            "与君尽力行公道，敢向昌朝俟陟明。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 8
    },
    {
        "title": "暮春有感寄宋维员外",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "杜宇声中老病心，此心无计驻光阴。西园雨过好花尽，",
            "明年才候东风至，结驷期君预去寻。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 9
    },
    {
        "title": "题吉水县厅前新栽小松",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "㔉开幽涧藓苔斑，移得孤根植砌前。影小未遮官舍月，",
            "输我婆娑栏槛内，晚风萧飒学幽泉。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 10
    },
    {
        "title": "《赠念法华经》绶上人",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "五更初起扫松堂，瞑目先焚一炷香。",
            "念彻莲经谁得见，千峰岩外晓苍苍。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 11
    },
    {
        "title": "秋日途中",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "信步腾腾野岩边，离家都为利名牵。",
            "疏林一路斜阳里，飒飒西风满耳蝉。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 12
    },
    {
        "title": "宿韦校书幽居",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "溪上高眠与鹤闲，开樽留我待柴关。",
            "园林月白秋霖歇，一夜泉声似故山。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 13
    },
    {
        "title": "依韵和友人秋夕见寄",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "夕风庭叶落，谁见此时情。不作关河梦，空闻砧杵声。",
            "会须求至理，何必叹无成。好约高僧宿，同看海月生。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 14
    },
    {
        "title": "吉水作尉酬高援秀才见赠",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "佐邑惭无术，敢言贫与清。风骚谁是主，烟月自关情。",
            "卷箔当山色，开窗就竹声。怜君惠嘉句，资我欲垂名。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 15
    },
    {
        "title": "送智雄上人",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "忽起游方念，飘然不可留。未知携一锡，乘兴向何州。",
            "古岸春云散，遥天晚雨收。想应重会面，风月又清秋。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 16
    },
    {
        "title": "览友人卷",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "新诗开卷处，造化竭精英。雪霁楚山碧，月高湘水清。",
            "初吟尘虑息，再味古风生。自此寰区内，喧腾二雅名。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 17
    },
    {
        "title": "送人南游",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "浪迹天涯去，南荒必动情。草青虞帝庙，云暗夜郎城。",
            "越鸟惊乡梦，蛮风解宿酲。早思归故里，华发等闲生。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 18
    },
    {
        "title": "晋陵县夏日作",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "事简公庭静，开帘暑气中。依经煎绿茗，入竹就清风。",
            "至论招禅客，忘机忆钓翁。晚凉安枕簟，海月出墙东。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 19
    },
    {
        "title": "邮亭早起",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "邮舍残灯在，村林鸡唱频。星河吟里晓，川陆望中春。",
            "旧友青云贵，殊乡素发新。悠悠念行计，难更驻征轮。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 20
    },
    {
        "title": "客中寒食",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "旅次经寒食，思乡泪湿巾。音书天外断，桃李雨中春。",
            "欲饮都无绪，唯吟似有因。输他郊郭外，多少踏青人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 21
    },
    {
        "title": "旅馆秋夕",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "寥寥山馆里，独坐酒初醒。旧业多年别，秋霖一夜听。",
            "砌蛩声渐息，窗烛影犹停。早晚无他事，休如泛水萍。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 22
    },
    {
        "title": "宿青溪米处士幽居",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "寄宿溪光里，夜凉高士家。养风窗外竹，叫月水中蛙。",
            "静虑同搜句，清神旋煮茶。唯忧晓鸡唱，尘里事如麻。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 23
    },
    {
        "title": "维舟秋浦，逢故人张矩同泊",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "卸帆清夜碧江滨，冉冉凉风动白苹。波上正吟新霁月，",
            "且饮一壶销百恨，会须遭遇识通津。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 24
    },
    {
        "title": "代别",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "明日鸣鞭天一涯，悠悠此夕怯分离。红楼有恨金波转，",
            "花时定是慵开鉴，独向春风忍扫眉。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 25
    },
    {
        "title": "悼怀王丧妃",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "花绽花开事可惊，暂来浮世返蓬瀛。楚宫梦断云空在，",
            "音容寂寞春牢落，谁会楼中独立情。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 26
    },
    {
        "title": "酒醒",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "睡觉花阴芳草软，不知明月出墙东。",
            "杯盘狼藉人何处，聚散空惊似梦中。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 27
    },
    {
        "title": "送虞道士",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "烟霞聚散通三岛，星斗分明在一壶。",
            "笑说余杭沽酒去，蔡家重要会麻姑。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 28
    },
    {
        "title": "怀旧夜吟寄赵杞",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "长笛声中海月飞，桃花零落满庭墀。魂销事去无寻处，",
            "悠悠方寸何因解，明日江楼望渺弥。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 29
    },
    {
        "title": "宿临江驿",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "候馆寥寥辍棹过，酒醒无奈旅愁何。雨昏郊郭行人少，",
            "篇章早晚逢知己，苦志忘形自有魔。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 30
    },
    {
        "title": "途中柳",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "翠色晴来近，长亭路去遥。无人折烟缕，落日拂溪桥。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 31
    },
    {
        "title": "隔墙花",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "颜色尤难近，馨香不易通。朱门金锁隔，空使怨春风。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 32
    },
    {
        "title": "广陵寒食夜",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "广陵寒食夜，豪贵足佳期。紫陌人归后，红楼月上时。",
            "绮罗香未歇，丝竹韵犹迟。明日踏青兴，输他轻薄儿。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 33
    },
    {
        "title": "寄庐山庄隐士",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "烟萝拥竹关，物外自求安。逼枕溪声近，当簷岳色寒。",
            "药苗应自采，琴调对谁弹。待了浮名后，依君共挂冠。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 34
    },
    {
        "title": "吉水寄阎侍御",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "何处怀君切，令人欲白头。偶寻花外寺，独立水边楼。",
            "不得论休戚，何因校献酬。吟余兴难尽，风笛起渔舟。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 35
    },
    {
        "title": "送张惟贞少府之江阴",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "相送烟汀畔，酒阑登小舟。离京梅雨歇，到邑早蝉秋。",
            "俗必期康济，诗谁互唱酬。晚凉诸吏散，海月入虚楼。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 36
    },
    {
        "title": "钟陵禁烟寄从弟",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "落絮飞花日又西，踏青无侣草萋萋。",
            "交亲书断竟不到，忍听黄昏杜宇啼。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 37
    },
    {
        "title": "夜泊江渚",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "水乡明月上晴空，汀岛香生杜若风。",
            "不是当年独醒客，且沽村酒待渔翁。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 38
    },
    {
        "title": "吉水县依韵酬华松秀才见寄",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "官况萧条在水村，吏归无事好论文。枕欹独听残春雨，",
            "诗情冷淡知音少，独喜江皋得见君。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 39
    },
    {
        "title": "贻庐山清溪观王尊师",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "霞帔星冠复杖藜，积年修炼住灵溪。松轩睡觉冷云起，",
            "鼎中龙虎功成后，海上三山去不迷。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 40
    },
    {
        "title": "王昭君",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "蛾眉翻自累，万里陷穷边。滴泪胡风起，宽心汉月圆。",
            "飞尘长翳日，白草自连天。谁贡和亲策，千秋污简编。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 41
    },
    {
        "title": "送姚端先辈归宁",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "知君归觐省，称意涉通津。解缆汀洲晓，张帆烟水春。",
            "牵吟芳草远，贳酒乱花新。拜庆庭闱处，蟾枝香满身。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 42
    },
    {
        "title": "江村晚秋作",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "高秋水村路，隔岸见人家。好是经霜叶，红于带露花。",
            "临罾鱼易得，就店酒难赊。吟兴胡能尽，风清日又斜。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 43
    },
    {
        "title": "蛩",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "月冷莎庭夜已深，百虫声外有清音。",
            "诗情正苦无眠处，愧尔阶前相伴吟。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 44
    },
    {
        "title": "感秋书事",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "宦途憔悴雪生头，家计相牵未得休。",
            "红蓼白苹消息断，旧溪烟月负渔舟。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 45
    },
    {
        "title": "访庐山归章禅伯",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "沉沉石室疏钟后，寂寂莎池片月明。",
            "多少学徒求妙法，要于言下悟无生。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 46
    },
    {
        "title": "庐山栖隐洞谭先生院留题",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "坛畔归云冷湿襟，拂苔移石坐花阴。",
            "偶然醒得庄周梦，始觉玄门兴味深。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 47
    },
    {
        "title": "江行值暴风雨",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "风狂雨暗舟人惧，自委神明志不邪。",
            "投得苇湾波浪息，岸头烟火近人家。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 48
    },
    {
        "title": "杪秋夕吟怀寄宋维先辈",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "江岛穷秋木叶稀，月高何处捣寒衣。",
            "苦嗟不见登龙客，此夜悠悠一梦飞。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 49
    },
    {
        "title": "七夕",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "星河耿耿正新秋，丝竹千家列彩楼。",
            "可惜穿针方有兴，纤纤初月苦难留。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 50
    },
    {
        "title": "吉水作尉时酬阎侍御见寄",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "谬佐驱鸡任，常思赋𫛳人。未谐林下约，空感病来身。",
            "锁径青苔老，铺阶红叶新。相思不可见，犹喜得书频。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 51
    },
    {
        "title": "清溪逢张惟贞秀才",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "洞隐红霞外，房开碧嶂根。昔年同炼句，几夜共听猿。",
            "考古书千卷，忘忧酒一樽。如今归建业，雅道喜重论。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 52
    },
    {
        "title": "送阎侍御归阙",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "羡君乘紫诏，归路指通津。鼓棹烟波暖，还京雨露新。",
            "趋朝丹禁晓，耸辔九衢春。自愧湮沈者，随轩未有因。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 53
    },
    {
        "title": "甲子岁罢吉水县过钟陵时暮春维舟江渚谒柴太尉席上作",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "公侯延驻暂踟蹰，况值风光三月初。乱落杯盘花片小，",
            "却笑田家门下客，当时容易叹车鱼。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 54
    },
    {
        "title": "海上春夕旅怀寄左偃",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "柳过清明絮乱飞，感时怀旧思凄凄。月生楼阁云初散，",
            "北山高卧风骚客，安得同吟复杖藜。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 55
    },
    {
        "title": "江次维舟登古寺",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "辍棹因过古梵宫，荒凉门径锁苔茸。绿阴满地前朝树，",
            "吟余却返来时路，回首盘桓尚驻筇。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 56
    },
    {
        "title": "春日招宋维先辈",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "瓮中竹叶今朝熟，鉴里桃花昨日开。",
            "为报广寒攀桂客，莫辞相访共衔杯。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 57
    },
    {
        "title": "吹笛儿",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "陇头休听月明中，妙竹嘉音际会逢。",
            "见尔樽前吹一曲，令人重忆许云封。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 58
    },
    {
        "title": "所思",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "解佩当时在洛滨，悠悠疑是梦中身。",
            "自从物外无消息，花谢莺啼近十春。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 59
    },
    {
        "title": "吉水县酬夏侯秀才见寄",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "启鉴悠悠两鬓苍，病来心绪易凄凉。",
            "知音不到吟还懒，锁印开帘又夕阳。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 60
    },
    {
        "title": "和友人喜雪",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "腊雪频频降，成堆不可除。伴吟花莫并，销瘴药何如。",
            "谢女诗成处，袁安睡起初。深迷樵子径，冷逼旅人居。",
            "惹砌催樽俎，飘窗入簿书。最宜楼上望，散乱满空虚。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 61
    },
    {
        "title": "感事呈所知",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "竞爱松筠翠，皆怜桃李芳。如求济世广，桑柘愿商量。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 62
    },
    {
        "title": "送汪涛",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "知君别家后，不免泪沾襟。芳草千里路，夕阳孤客心。",
            "花飞当野渡，猿叫在烟岑。霄汉知音在，何须恨陆沈。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 63
    },
    {
        "title": "言志寄刘钧秀才",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "童稚亲儒墨，时平喜道存。酬身指书剑，赋命委乾坤。",
            "秋爽鼓琴兴，月清搜句魂。与君同此志，终待至公论。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 64
    },
    {
        "title": "访澄上人",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "寻师来静境，神骨觉清凉。一饷逢秋雨，相留坐竹堂。",
            "石渠堆败叶，莎砌咽寒螀。话到南能旨，怡然万虑忘。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 65
    },
    {
        "title": "经古寺",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "殿宇半隳摧，门临野水开。云凝何代树，草蔽此时台。",
            "绕塔堆黄叶，沿阶积绿苔。踟蹰日将暮，栖鸟入巢来。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 66
    },
    {
        "title": "送王道士游东海",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "巨浸常牵梦，云游岂觉劳。遥空收晚雨，虚阁看秋涛。",
            "必若思三岛，应须钓六鼇。如通十洲去，谁信碧天高。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 67
    },
    {
        "title": "贻青阳宰",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "征赋常登限，名山管最多。吏闲民讼少，时得访烟萝。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 68
    },
    {
        "title": "哭故主人陈太师",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "十年孤迹寄侯门，入室升堂忝厚恩。游遍春郊随茜旆，",
            "长恸裴回逝川上，白杨萧飒又黄昏。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 69
    },
    {
        "title": "秋江夜泊寄刘钧",
        "author": "李中",
        "biography": "",
        "paragraphs": [
            "万里江山敛暮烟，旅情当此独悠然。沙汀月冷帆初卸，",
            "与君共俟酬身了，结侣波中寄钓船。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百四十九",
        "no#": 70
    }
]