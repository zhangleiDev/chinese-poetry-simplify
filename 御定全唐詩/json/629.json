[
    {
        "title": "和袭美友人许惠酒以诗征之",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "冻醪初漉嫩如春，轻蚁漂漂杂蕊尘。",
            "得伴方平同一醉，明朝应作蔡经身。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 1
    },
    {
        "title": "闻圆载上人挟儒书洎释典归日本国，更作一绝以送",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "九流三藏一时倾，万轴光凌渤澥声。",
            "从此遗编东去后，却应荒外有诸生。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 2
    },
    {
        "title": "闲吟",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "闲吟料得三更尽，始把孤灯背竹窗。",
            "一夜西风高浪起，不教归梦过寒江。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 3
    },
    {
        "title": "秘色越器",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "九秋风露越窑开，夺得千峰翠色来。",
            "好向中宵盛沆瀣，共嵇中散斗遗杯。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 4
    },
    {
        "title": "景阳宫井",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "古堞烟埋宫井树，陈主吴姬堕泉处。",
            "舜没苍梧万里云，却不闻将二妃去。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 5
    },
    {
        "title": "江南二首",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "便风船尾香粳熟，细雨层头赤鲤跳。",
            "待得江餐闲望足，日斜方动木兰桡。",
            "村边紫豆花垂次，岸上红梨叶战初。",
            "莫怪烟中重回首，酒家青纻一行书。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 6
    },
    {
        "title": "溪思雨中",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "雨映前山万絇丝，橹声冲破似鸣机。",
            "无端织得愁成段，堪作骚人酒病衣。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 7
    },
    {
        "title": "江城夜泊",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "漏移寒箭丁丁急，月挂虚弓霭霭明。",
            "此夜离魂堪射断，更须江笛两三声。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 8
    },
    {
        "title": "宫人斜",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "草著愁烟似不春，晚莺哀怨问行人。",
            "须知一种埋香骨，犹胜昭君作虏尘。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 9
    },
    {
        "title": "送棋客",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "满目山川似势棋，况当秋雁正斜飞。",
            "金门若召羊玄保，赌取江东太守归。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 10
    },
    {
        "title": "汉宫词",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "招灵阁上霓旌绝，柏梁台中珠翠稠。",
            "一身三十六宫夜，露滴玉盘青桂秋。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 11
    },
    {
        "title": "上清",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "玉林风露寂寥清，仙妃对月闲吹笙。",
            "新篁冷涩曲未尽，细拂云枝栖凤惊。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 12
    },
    {
        "title": "秋荷",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "蒲茸承露有佳色，茭叶束烟如效颦。",
            "盈盈一水不得渡，冷翠遗香愁向人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 13
    },
    {
        "title": "有别二首",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "且将丝wf系兰舟，醉下烟汀减去愁。",
            "江上有楼君莫上，落花随浪正东流。",
            "池上已看莺舌默，云间应即雁翰开。",
            "唯愁别后当风立，万树将秋入恨来。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 14
    },
    {
        "title": "病中晓思",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "月堕霜西竹井寒，辘轳丝冻下瓶难。",
            "幽人病久浑成渴，愁见龙书一鼎干。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 15
    },
    {
        "title": "送友人之湖上",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "故人溪上有渔舟，竿倚风苹夜不收。",
            "欲寄一函聊问讯，洪乔宁作置书邮。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 16
    },
    {
        "title": "寒日逢僧",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "瘦胫高褰梵屟轻，野塘风劲锡环鸣。",
            "如何不向深山里，坐拥闲云过一生。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 17
    },
    {
        "title": "冬柳",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "柳汀斜对野人窗，零落衰条傍晓江。",
            "正是霜风飘断处，寒鸥惊起一双双。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 18
    },
    {
        "title": "寄友人",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "敬亭寒夜溪声里，同听先生讲太玄。",
            "上得云梯不回首，钓竿犹在五湖边。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 19
    },
    {
        "title": "岛树",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "波涛漱苦盘根浅，风雨飘多著叶迟。",
            "迥出孤烟残照里，鹭鹚相对立高枝。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 20
    },
    {
        "title": "头陀僧",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "万峰围绕一峰深，向此长修苦行心。",
            "自扫雪中归鹿迹，天明恐被猎人寻。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 21
    },
    {
        "title": "晚渡",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "半波风雨半波晴，渔曲飘秋野调清。",
            "各样莲船逗村去，笠簷蓑袂有残声。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 22
    },
    {
        "title": "赠老僧二首",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "枯貌自同霜里木，余生唯指佛前灯。",
            "少时写得坐禅影，今见问人何处僧。",
            "自有家山供衲线，不离溪曲取庵茅。",
            "旧曾闻说林中鸟，定后长来顶上巢。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 23
    },
    {
        "title": "忆山泉",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "一夜寒声来梦里，平明著屐到声边。",
            "心期盛夏同过此，脱却荷衣石上眠。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 24
    },
    {
        "title": "白芙蓉",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "澹然相对却成劳，月染风裁个个高。",
            "似说玉皇亲谪堕，至今犹著水霜袍。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 25
    },
    {
        "title": "严光钓台",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "片帆竿外揖清风，石立云孤万古中。",
            "不是狂奴为故态，仲华争得黑头公。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 26
    },
    {
        "title": "读陈拾遗集",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "蓬颗何时与恨平，蜀江衣带蜀山轻。",
            "寻闻骑士枭黄祖，自是无人祭祢衡。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 27
    },
    {
        "title": "吴宫怀古",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "香径长洲尽棘丛，奢云艳雨只悲风。",
            "吴王事事须亡国，未必西施胜六宫。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 28
    },
    {
        "title": "送琴客之建康",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "蕙风杉露共泠泠，三峡寒泉漱玉清。",
            "君到南朝访遗事，柳家双锁旧知名。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 29
    },
    {
        "title": "闺怨",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "白袷行人又远游，日斜空上映花楼。",
            "愁丝堕絮相逢著，绊惹春风卒未休。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 30
    },
    {
        "title": "丁香",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "江上悠悠人不问，十年云外醉中身。",
            "殷勤解却丁香结，纵放繁枝散诞春。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 31
    },
    {
        "title": "种蒲",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "杜若溪边手自移，旋抽烟剑碧参差。",
            "何时织得孤帆去，悬向秋风访所思。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 32
    },
    {
        "title": "范蠡",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "平吴专越祸胎深，岂是功成有去心。",
            "勾践不知嫌鸟喙，归来犹自铸良金。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 33
    },
    {
        "title": "山僧二首",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "山藓几重生草履，涧泉长自满铜瓶。",
            "时将如意敲眠虎，遣向林间坐听经。",
            "一夏不离苍岛上，秋来频话石城南。",
            "思归瀑布声前坐，却把松枝拂旧庵。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 34
    },
    {
        "title": "上云乐",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "青丝作筰桂为船，白兔捣药虾蟆丸。",
            "便浮天汉泊星渚，回首笑君承露盘。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 35
    },
    {
        "title": "新秋杂题六首：眠",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "一簟临窗薤叶秋，小帘风荡半离钩。",
            "魂清雨急梦难到，身在五湖波上头。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 36
    },
    {
        "title": "新秋杂题六首：行",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "寻人直到月坞北，觅鹤便过云峰西。",
            "只今犹有疏野调，但绕莓苔风雨畦。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 37
    },
    {
        "title": "新秋杂题六首：倚",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "橘下凝情香染巾，竹边留思露摇身。",
            "背烟垂首尽日立，忆得山中无事人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 38
    },
    {
        "title": "新秋杂题六首：吟",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "忆山摇膝石上晚，怀古掉头溪畔凉。",
            "有时得句一声发，惊起鹭鹚和夕阳。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 39
    },
    {
        "title": "新秋杂题六首：食",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "日午空斋带睡痕，水蔬山药荐盘飧。",
            "林乌信我无机事，长到而今下石盆。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 40
    },
    {
        "title": "新秋杂题六首：坐",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "偶避蝉声来隙地，忽随鸿影入辽天。",
            "闲僧不会寂寥意，道学西方人坐禅。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 41
    },
    {
        "title": "新沙",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "渤澥声中涨小堤，官家知后海鸥知。",
            "蓬莱有路教人到，应亦年年税紫芝。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 42
    },
    {
        "title": "邺宫词二首",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "魏武平生不好香，枫胶蕙炷洁宫房。",
            "可知遗令非前事，却有余熏在绣囊。",
            "花飞蝶骇不愁人，水殿云廊别置春。",
            "晓日靓妆千骑女，白樱桃下紫纶巾。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 43
    },
    {
        "title": "古别离",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "仙人左手把长箭，欲射日乌乌不栖。",
            "何事离情畏明发，一心唯恨汝南鸡。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 44
    },
    {
        "title": "高道士",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "峨眉道士风骨峻，手把玉皇书一通。",
            "东游借得琴高鲤，骑入蓬莱清浅中。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 45
    },
    {
        "title": "蔬食",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "孔融不要留残脍，庾悦无端吝子鹅。",
            "香稻熟来秋菜嫩，伴僧餐了听云和。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 46
    },
    {
        "title": "寄远",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "缥梨花谢莺口吃，黄犊少年人未归。",
            "画扇红弦相掩映，独看斜月下帘衣。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 47
    },
    {
        "title": "山中僧",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "手关一室翠微里，日暮白云栖半间。",
            "白云朝出天际去，若比老僧犹未闲。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 48
    },
    {
        "title": "洞宫秋夕",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "浓霜打叶落地声，南溪石泉细泠泠。",
            "洞宫寂寞人不去，坐见月生云母屏。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 49
    },
    {
        "title": "连昌宫词二首：门",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "金铺零落兽镮空，斜掩双扉细草中。",
            "日暮鸟归宫树绿，不闻鸦轧闭春风。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 50
    },
    {
        "title": "连昌宫词二首：阶",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "草没苔封叠翠斜，坠红千叶拥残霞。",
            "年年直为秋霖苦，滴陷青瑉隐起花。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 51
    },
    {
        "title": "怀宛陵旧游",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "陵阳佳地昔年游，谢脁青山李白楼。",
            "唯有日斜溪上思，酒旗风影落春流。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 52
    },
    {
        "title": "松石晓景图",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "霜骨云根惨澹愁，宿烟封著未全收。",
            "将归与说文通后，写得松江岸上秋。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 53
    },
    {
        "title": "钓车",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "小轮轻线妙无双，曾伴幽人酒一缸。",
            "洛客见诗如有问，辗烟冲雨过桐江。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 54
    },
    {
        "title": "漉酒巾",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "靖节高风不可攀，此巾犹坠冻醪间。",
            "偏宜雪夜山中戴，认取时情与醉颜。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 55
    },
    {
        "title": "华阳巾",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "莲花峰下得佳名，云褐相兼上鹤翎。",
            "须是古坛秋霁后，静焚香炷礼寒星。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 56
    },
    {
        "title": "方响",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "击霜寒玉乱丁丁，花底秋风拂坐生。",
            "王母闲看汉天子，满猗兰殿佩环声。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 57
    },
    {
        "title": "白鹭",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "雪然飞下立苍苔，应伴江鸥拒我来。",
            "见欲扁舟摇荡去，倩君先作水云媒。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 58
    },
    {
        "title": "溪行",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "晚天寒雨上滩时，他已扬舲我尚迟。",
            "自是樯低帆幅少，溪风终不两般吹。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 59
    },
    {
        "title": "太湖叟",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "细桨轻船卖石归，酒痕狼藉遍苔衣。",
            "攻车战舰繁如织，不肯回头问是非。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 60
    },
    {
        "title": "答友人",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "荆卿雄骨化为尘，燕市应无共饮人。",
            "能脱鹔鹴来换酒，五湖赊与一年春。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 61
    },
    {
        "title": "偶作",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "酒信巧为缲病绪，花音长作嫁愁媒。",
            "也知愁病堪回避，争奈流莺唤起来。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 62
    },
    {
        "title": "春思二首",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "竹外麦烟愁漠漠，短翅啼禽飞魄魄。",
            "此时忆著千里人，独坐支颐看花落。",
            "江南酒熟清明天，高高绿旆当风悬。",
            "谁家无事少年子，满面落花犹醉眠。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 63
    },
    {
        "title": "访僧不遇",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "棹倚东林欲问禅，远公飞锡未应还。",
            "蒙庄弟子相看笑，何事空门亦有关。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 64
    },
    {
        "title": "谢山泉",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "决决春泉出洞霞，石坛封寄野人家。",
            "草堂尽日留僧坐，自向前溪摘茗芽。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 65
    },
    {
        "title": "洞宫夕",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "月午山空桂花落，华阳道士云衣薄。",
            "石坛香散步虚声，杉云清泠滴栖鹤。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 66
    },
    {
        "title": "峡客行",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "万仞峰排千剑束，孤舟夜系峰头宿。",
            "蛮溪雪坏蜀江倾，滟滪朝来大如屋。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 67
    },
    {
        "title": "江边",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "江边日晚潮烟上，树里鸦鸦桔槔响。",
            "无因得似灌园翁，十亩春蔬一藜杖。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 68
    },
    {
        "title": "帘",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "枕映疏容晚向欹，秋烟脉脉雨微微。",
            "逆风障燕寻常事，不学人前当妓衣。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 69
    },
    {
        "title": "开元杂题七首：玉龙子",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "何代奇工碾玉英，细髯纤角尽雕成。",
            "烟干雾悄君心苦，风雨长随一掷声。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 70
    },
    {
        "title": "开元杂题七首：照夜白",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "雪虬轻骏步如飞，一练腾光透月旗。",
            "应笑穆王抛万乘，踏风鞭露向瑶池。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 71
    },
    {
        "title": "开元杂题七首：舞马",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "月窟龙孙四百蹄，骄骧轻步应金鞞。曲终似要君王宠，回望红楼不敢嘶。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 72
    },
    {
        "title": "开元杂题七首：杂伎",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "拜象驯犀角抵豪，星丸霜剑出花高。",
            "六宫争近乘舆望，珠翠三千拥赭袍。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 73
    },
    {
        "title": "开元杂题七首：雪衣女",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "嫩红钩曲雪花攒，月殿栖时片影残。",
            "自说夜来春梦恶，学持金偈玉栏干。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 74
    },
    {
        "title": "开元杂题七首：绣岭宫",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "绣岭花残翠倚空，碧窗瑶砌旧行宫。",
            "闲乘小驷浓阴下，时举金鞭半袖风。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 75
    },
    {
        "title": "开元杂题七首：汤泉",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "暖殿流汤数十间，玉渠香细浪回环。",
            "上皇初解云衣浴，珠棹时敲瑟瑟山。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 76
    },
    {
        "title": "题杜秀才水亭",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "晓和风露立晴烟，只恐腥魂涴洞天。",
            "云肆有龙君若买，便敲初日铸金钱。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 77
    },
    {
        "title": "翠碧",
        "author": "陆龟蒙",
        "biography": "",
        "paragraphs": [
            "红襟翠翰两参差，径拂烟华上细枝。",
            "春水渐生鱼易得，莫辞风雨坐多时。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百二十九",
        "no#": 78
    }
]