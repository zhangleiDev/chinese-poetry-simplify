[
    {
        "title": "山居即事二首",
        "author": "陆希声",
        "biography": "",
        "paragraphs": [
            "君山苍翠接青冥，东走洮湖上洞庭。茅屋向阳梳白发，",
            "为问前时金马客，此焉还作少微星。",
            "不是幽栖矫性灵，从来无意在膻腥。满川风物供高枕，",
            "君阳遁叟何为乐，一炷清香两卷经。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百八十九",
        "no#": 1
    },
    {
        "title": "阳羡杂咏十九首：苦行径",
        "author": "陆希声",
        "biography": "",
        "paragraphs": [
            "山前无数碧琅玕，一径清森五月寒。",
            "世上何人怜苦节，应须细问子猷看。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百八十九",
        "no#": 2
    },
    {
        "title": "阳羡杂咏十九首：梅花坞",
        "author": "陆希声",
        "biography": "",
        "paragraphs": [
            "冻蕊凝香色艳新，小山深坞伴幽人。",
            "知君有意凌寒色，羞共千花一样春。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百八十九",
        "no#": 3
    },
    {
        "title": "阳羡杂咏十九首：石兕台",
        "author": "陆希声",
        "biography": "",
        "paragraphs": [
            "大河波浪激潼关，青兕胡为伏此山。",
            "遥想楚王云梦泽，蜺旌羽盖定空还。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百八十九",
        "no#": 4
    },
    {
        "title": "阳羡杂咏十九首：讲易台",
        "author": "陆希声",
        "biography": "",
        "paragraphs": [
            "年逾知命志尤坚，独向青山更绝编。",
            "天下有山山有水，养蒙肥遁正翛然。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百八十九",
        "no#": 5
    },
    {
        "title": "阳羡杂咏十九首：观鱼亭",
        "author": "陆希声",
        "biography": "",
        "paragraphs": [
            "惠施徒自学多方，谩说观鱼理未长。",
            "不得庄生濠上旨，江湖何以见相忘。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百八十九",
        "no#": 6
    },
    {
        "title": "阳羡杂咏十九首：绿云亭",
        "author": "陆希声",
        "biography": "",
        "paragraphs": [
            "六月清凉绿树阴，小亭高卧涤烦襟。",
            "羲皇向上何人到，永日时时弄素琴。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百八十九",
        "no#": 7
    },
    {
        "title": "阳羡杂咏十九首：清辉堂",
        "author": "陆希声",
        "biography": "",
        "paragraphs": [
            "野人心地本无机，为爱茅簷倚翠微。",
            "尽日尊前谁是客，秋山含水有清辉。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百八十九",
        "no#": 8
    },
    {
        "title": "阳羡杂咏十九首：观妙庵",
        "author": "陆希声",
        "biography": "",
        "paragraphs": [
            "妙理难观旨甚深，欲知无欲是无心。",
            "茅庵不异人间世，河上真人自可寻。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百八十九",
        "no#": 9
    },
    {
        "title": "阳羡杂咏十九首：西阳亭",
        "author": "陆希声",
        "biography": "",
        "paragraphs": [
            "隔林残日照孤亭，玄晏先生酒未醒。",
            "入夜莫愁迷下路，昔人犹在逐流萤。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百八十九",
        "no#": 10
    },
    {
        "title": "阳羡杂咏十九首：弄云亭",
        "author": "陆希声",
        "biography": "",
        "paragraphs": [
            "自知无业致吾君，只向春山弄白云。",
            "已共此山私断当，不须转辙重移文。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百八十九",
        "no#": 11
    },
    {
        "title": "阳羡杂咏十九首：伏龟堂",
        "author": "陆希声",
        "biography": "",
        "paragraphs": [
            "盘崖蹙缩似灵龟，鬼谷先生隐遁时。",
            "不独卷怀经世志，白云流水是心期。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百八十九",
        "no#": 12
    },
    {
        "title": "阳羡杂咏十九首：桃花谷",
        "author": "陆希声",
        "biography": "",
        "paragraphs": [
            "君阳山下足春风，满谷仙桃照水红。",
            "何必武陵源上去，涧边好过落花中。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百八十九",
        "no#": 13
    },
    {
        "title": "阳羡杂咏十九首：含桃圃",
        "author": "陆希声",
        "biography": "",
        "paragraphs": [
            "小圃初晴风露光，含桃花发满山香。",
            "看花对酒心无事，倍觉春来白日长。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百八十九",
        "no#": 14
    },
    {
        "title": "阳羡杂咏十九首：茗坡",
        "author": "陆希声",
        "biography": "",
        "paragraphs": [
            "二月山家谷雨天，半坡芳茗露华鲜。",
            "春醒酒病兼消渴，惜取新芽旋摘煎。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百八十九",
        "no#": 15
    },
    {
        "title": "阳羡杂咏十九首：松岭",
        "author": "陆希声",
        "biography": "",
        "paragraphs": [
            "岭上青松手自栽，已能苍翠映莓苔。",
            "岁寒本是君家事，好送清风月下来。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百八十九",
        "no#": 16
    },
    {
        "title": "阳羡杂咏十九首：桃溪",
        "author": "陆希声",
        "biography": "",
        "paragraphs": [
            "芳草霏霏遍地齐，桃花脉脉自成溪。",
            "也知百舌多言语，任向春风尽意啼。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百八十九",
        "no#": 17
    },
    {
        "title": "阳羡杂咏十九首：李径",
        "author": "陆希声",
        "biography": "",
        "paragraphs": [
            "一径秾芳万蕊攒，风吹雨打未摧残。",
            "怜君尽向高枝发，应为行人要整冠。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百八十九",
        "no#": 18
    },
    {
        "title": "阳羡杂咏十九首：鸿盘",
        "author": "陆希声",
        "biography": "",
        "paragraphs": [
            "落落飞鸿渐始盘，青云起处剩须看。",
            "如今天路多矰缴，纵使衔芦去也难。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百八十九",
        "no#": 19
    },
    {
        "title": "阳羡杂咏十九首：偃月岭",
        "author": "陆希声",
        "biography": "",
        "paragraphs": [
            "山岭依稀偃月形，数层倚石叠空青。",
            "几回雪夜寒光积，直似金光照户庭。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百八十九",
        "no#": 20
    },
    {
        "title": "寄z6光上人",
        "author": "陆希声",
        "biography": "",
        "paragraphs": [
            "笔下龙蛇似有神，天池雷雨变逡巡。",
            "寄言昔日不龟手，应念江头洴澼人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百八十九",
        "no#": 21
    },
    {
        "title": "喜杜荀鹤及第",
        "author": "李昭象",
        "biography": "",
        "paragraphs": [
            "深岩贫复病，榜到见君名。贫病浑如失，山川顿觉清。",
            "一春新酒兴，四海旧诗声。日使能吟者，西来步步轻。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百八十九",
        "no#": 22
    },
    {
        "title": "赴举出山留寄山居郑参军",
        "author": "李昭象",
        "biography": "",
        "paragraphs": [
            "还如费冠卿，向此振高名。肯羡鱼须美，长夸鹤氅轻。",
            "理琴寒指倦，试药黑髭生。时泰难云卧，随看急诏行。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百八十九",
        "no#": 23
    },
    {
        "title": "题顾正字溪居",
        "author": "李昭象",
        "biography": "",
        "paragraphs": [
            "高敞吟轩近钓湾，尘中来似出人间。若教明月休生桂，",
            "莫夸恬淡胜荣禄，雁引行高未许闲。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百八十九",
        "no#": 24
    },
    {
        "title": "寄献山中顾公员外",
        "author": "李昭象",
        "biography": "",
        "paragraphs": [
            "抽却朝簪著钓蓑，近来声迹转巍峨。祥麟避网虽山野，",
            "深惭未副吹嘘力，竟困风埃争奈何。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百八十九",
        "no#": 25
    },
    {
        "title": "山中寄崔谏议",
        "author": "李昭象",
        "biography": "",
        "paragraphs": [
            "半生猿鸟共山居，吟月吟风两鬓疏。新句未尝忘教化，",
            "从此升腾休说命，只希公道数封书。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百八十九",
        "no#": 26
    },
    {
        "title": "学仙词寄顾云",
        "author": "李昭象",
        "biography": "",
        "paragraphs": [
            "记得初传九转方，碧云峰下祝虚皇。丹砂未熟心徒切，",
            "仙人恩重何由报，焚尽星坛午夜香。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百八十九",
        "no#": 27
    },
    {
        "title": "寄尉迟侍御",
        "author": "李昭象",
        "biography": "",
        "paragraphs": [
            "我眠青嶂弄澄潭，君戴貂蝉白玉𥮾。应向谢公楼上望，九华山色在西南。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百八十九",
        "no#": 28
    },
    {
        "title": "招西洞道者",
        "author": "李昭象",
        "biography": "",
        "paragraphs": [
            "危峰抹黛夹晴川，树簇红英草碧烟。",
            "樵客云僧两无事，此中堪去觅灵仙。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百八十九",
        "no#": 29
    },
    {
        "title": "句",
        "author": "李昭象",
        "biography": "",
        "paragraphs": [
            "投文得仕而今少，佩印还家古所荣。（送周繇之建德。",
            "《唐诗纪事》）"
        ],
        "notes": [
            ""
        ],
        "volume": "卷六百八十九",
        "no#": 30
    }
]