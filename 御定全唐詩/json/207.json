[
    {
        "title": "和袁郎中破贼后经剡县山水上太尉",
        "author": "李嘉佑",
        "biography": "",
        "paragraphs": [
            "受律仙郎贵，长驱下会稽。鸣笳山月晓，摇旆野云低。",
            "翦寇人皆贺，回军马自嘶。地闲春草绿，城静夜乌啼。",
            "破竹清闽岭，看花入剡溪。元戎催献捷，莫道事攀跻。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百零七",
        "no#": 1
    },
    {
        "title": "送评事十九叔入秦",
        "author": "李嘉佑",
        "biography": "",
        "paragraphs": [
            "白露沾蕙草，王孙转忆归。蔡州新战罢，郢路去人稀。",
            "谒帝不辞远，怀亲空有违。孤舟看落叶，平楚逐斜晖。",
            "北阙见端冕，南台当绣衣。唯余播迁客，只伴鹧鸪飞。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百零七",
        "no#": 2
    },
    {
        "title": "赠王八衢",
        "author": "李嘉佑",
        "biography": "",
        "paragraphs": [
            "丹地偏相逐，清江若有期。腰金才子贵，剖竹老人迟。",
            "桂楫闲迎客，茶瓯对说诗。渚田分邑里，山桂树罘罳。",
            "心静无华发，人和似古时。别君远山去，幽独更应悲。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百零七",
        "no#": 3
    },
    {
        "title": "入睦州分水路忆刘长卿",
        "author": "李嘉佑",
        "biography": "",
        "paragraphs": [
            "北阙忤明主，南方随白云。沿洄滩草色，应接海鸥群。",
            "建德潮已尽，新安江又分。回看严子濑，朗咏谢安文。",
            "雨过暮山碧，猿吟秋日曛。吴洲不可到，刷鬓为思君。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百零七",
        "no#": 4
    },
    {
        "title": "奉和杜相公长兴新宅即事呈元相公",
        "author": "李嘉佑",
        "biography": "",
        "paragraphs": [
            "意有空门乐，居无甲第奢。经过容法侣，雕饰让侯家。",
            "隐树重簷肃，开园一径斜。据梧听好鸟，行药寄名花。",
            "梦蝶留清簟，垂貂坐绛纱。当山不掩户，映日自倾茶。",
            "雅望归安石，深知在叔牙。还成吉甫颂，赠答比瑶华。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百零七",
        "no#": 5
    },
    {
        "title": "江湖秋思",
        "author": "李嘉佑",
        "biography": "",
        "paragraphs": [
            "趋陪禁掖雁行随，迁向江潭鹤发垂。素浪遥疑八溪水，",
            "共望汉朝多霈泽，苍蝇早晚得先知。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百零七",
        "no#": 6
    },
    {
        "title": "送朱中舍游江东",
        "author": "李嘉佑",
        "biography": "",
        "paragraphs": [
            "孤城郭外送王孙，越水吴洲共尔论。野寺山边斜有径，",
            "若到西陵征战处，不堪秋草自伤魂。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百零七",
        "no#": 7
    },
    {
        "title": "送窦拾遗赴朝因寄中书十七弟",
        "author": "李嘉佑",
        "biography": "",
        "paragraphs": [
            "自叹未沾黄纸诏，那堪远送赤墀人。老为侨客偏相恋，",
            "凭尔将书通令弟，唯论华发愧头巾。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百零七",
        "no#": 8
    },
    {
        "title": "自苏台至望亭驿人家尽空春物增思怅然有作因寄从弟纾",
        "author": "李嘉佑",
        "biography": "",
        "paragraphs": [
            "南浦菰蒋覆白苹，东吴黎庶逐黄巾。野棠自发空临水，",
            "那堪回首长洲苑，烽火年年报虏尘。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百零七",
        "no#": 9
    },
    {
        "title": "承恩量移宰江邑，临鄱江怅然之作",
        "author": "李嘉佑",
        "biography": "",
        "paragraphs": [
            "四年谪宦滞江城，未厌门前鄱水清。谁言宰邑化黎庶，",
            "惆怅闲眠临极浦，夕阳秋草不胜情。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百零七",
        "no#": 10
    },
    {
        "title": "题灵台县东山村主人",
        "author": "李嘉佑",
        "biography": "",
        "paragraphs": [
            "处处征胡人渐稀，山村寥落暮烟微。门临莽苍经年闭，",
            "天子如今能用武，只应岁晚息兵机。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百零七",
        "no#": 11
    },
    {
        "title": "同皇甫冉登重玄阁",
        "author": "李嘉佑",
        "biography": "",
        "paragraphs": [
            "高阁朱栏不厌游，蒹葭白水绕长洲。孤云独鸟川光暮，",
            "谁怜远作秦吴别，离恨归心双泪流。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百零七",
        "no#": 12
    },
    {
        "title": "宋州东登望题武陵驿",
        "author": "李嘉佑",
        "biography": "",
        "paragraphs": [
            "梁宋人稀鸟自啼，登舻一望倍含凄。白骨半随河水去，",
            "明主频移虎符守，几时行县向黔黎。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百零七",
        "no#": 13
    },
    {
        "title": "晚登江楼有怀",
        "author": "李嘉佑",
        "biography": "",
        "paragraphs": [
            "独坐南楼佳兴新，青山绿水共为邻。爽气遥分隔浦岫，",
            "只忆帝京不可到，秋琴一弄欲沾巾。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百零七",
        "no#": 14
    },
    {
        "title": "游徐城河忽见清淮，因寄赵八",
        "author": "李嘉佑",
        "biography": "",
        "paragraphs": [
            "自缘迟暮忆沧洲，翻爱南河浊水流。初过重阳惜残菊，",
            "长恨相逢即分首，含情掩泪独回头。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百零七",
        "no#": 15
    },
    {
        "title": "题游仙阁白公庙",
        "author": "李嘉佑",
        "biography": "",
        "paragraphs": [
            "仙冠轻举竟何之，薜荔缘阶竹映祠。甲子不知风驭日，",
            "逐客自怜双鬓改，焚香多负白云期。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百零七",
        "no#": 16
    },
    {
        "title": "送郑正则汉阳迎妇",
        "author": "李嘉佑",
        "biography": "",
        "paragraphs": [
            "锦字相催鸟急飞，郎君暂脱老莱衣。遥想双眉待人画，",
            "令秩和鸣真可羡，此行谁道负春辉。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百零七",
        "no#": 17
    },
    {
        "title": "送皇甫冉往安宜",
        "author": "李嘉佑",
        "biography": "",
        "paragraphs": [
            "江皋尽日唯烟水，君向白田何日归。楚地蒹葭连海迥，",
            "若问行人与征战，使君双泪定沾衣。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百零七",
        "no#": 18
    },
    {
        "title": "晚发咸阳，寄同院遗补",
        "author": "李嘉佑",
        "biography": "",
        "paragraphs": [
            "征战初休草又衰，咸阳晚眺泪堪垂。去路全无千里客，",
            "回首青山独不语，羡君谈笑万年枝。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百零七",
        "no#": 19
    },
    {
        "title": "早秋京口旅泊，章侍御寄书相问，因以赠之，时七夕",
        "author": "李嘉佑",
        "biography": "",
        "paragraphs": [
            "移家避寇逐行舟，厌见南徐江水流。吴越征徭非旧日，",
            "只有同时骢马客，偏宜尺牍问穷愁。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百零七",
        "no#": 20
    },
    {
        "title": "秋晓招隐寺东峰茶宴，送内弟阎伯均归江州",
        "author": "李嘉佑",
        "biography": "",
        "paragraphs": [
            "万畦新稻傍山村，数里深松到寺门。幸有香茶留稚子，",
            "莫怪临歧独垂泪，魏舒偏念外家恩。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百零七",
        "no#": 21
    },
    {
        "title": "送严员外",
        "author": "李嘉佑",
        "biography": "",
        "paragraphs": [
            "春风倚棹阖闾城，水国春寒阴复晴。细雨湿衣看不见，",
            "君去若逢相识问，青袍今已误儒生。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百零七",
        "no#": 22
    },
    {
        "title": "赴南中留别褚七少府湖上林亭",
        "author": "李嘉佑",
        "biography": "",
        "paragraphs": [
            "种田东郭傍春陂，万事无情把钓丝。绿竹放侵行径里，",
            "从此别君千万里，白云流水忆佳期。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百零七",
        "no#": 23
    },
    {
        "title": "与从弟正字、从兄兵曹宴集林园",
        "author": "李嘉佑",
        "biography": "",
        "paragraphs": [
            "竹窗松户有佳期，美酒香茶慰所思。辅嗣外生还解易，",
            "去路归程仍待月，垂缰不控马行迟。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百零七",
        "no#": 24
    },
    {
        "title": "酬皇甫十六侍御曾见寄",
        "author": "李嘉佑",
        "biography": "",
        "paragraphs": [
            "自顾衰容累玉除，忽承优诏赴铜鱼。江头鸟避青旄节，",
            "更枉新诗思何苦，离骚愁处亦无如。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百零七",
        "no#": 25
    },
    {
        "title": "暮春宜阳郡斋愁坐，忽枉刘七侍御新诗，因以酬答",
        "author": "李嘉佑",
        "biography": "",
        "paragraphs": [
            "子规夜夜啼槠叶，远道逢春半是愁。芳草伴人还易老，",
            "唯羡君为周柱史，手持黄纸到沧洲。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百零七",
        "no#": 26
    },
    {
        "title": "送舍弟",
        "author": "李嘉佑",
        "biography": "",
        "paragraphs": [
            "诸谢偏推永嘉守，三何独许水曹郎。老兄鄙思难俦匹，",
            "定知马上多新句，早寄袁溪当八行。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百零七",
        "no#": 27
    },
    {
        "title": "送从弟永任饶州录事参军",
        "author": "李嘉佑",
        "biography": "",
        "paragraphs": [
            "一官万里向千溪，水宿山行鱼浦西。日晚长烟高岸近，",
            "闻道慈亲倚门待，到时兰叶正萋萋。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百零七",
        "no#": 28
    },
    {
        "title": "送马将军奏事毕归滑州使幕",
        "author": "李嘉佑",
        "biography": "",
        "paragraphs": [
            "吴门别后蹈沧州，帝里相逢俱白头。自叹马卿常带病，",
            "想到滑台桑叶落，黄河东注荻花秋。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百零七",
        "no#": 29
    },
    {
        "title": "闻逝者自惊",
        "author": "李嘉佑",
        "biography": "",
        "paragraphs": [
            "亦知死是人间事，年老闻之心自疑。黄卷清琴总为累，",
            "儿女眼前难喜舍，弥怜双鬓渐如丝。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百零七",
        "no#": 30
    },
    {
        "title": "伤歙州陈二使君",
        "author": "李嘉佑",
        "biography": "",
        "paragraphs": [
            "怜君辞满卧沧洲，一旦云亡万事休。慈母断肠妻独泣，",
            "寂寞荒坟近渔浦，野松孤月即千秋。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百零七",
        "no#": 31
    },
    {
        "title": "白田西忆楚州使君弟",
        "author": "李嘉佑",
        "biography": "",
        "paragraphs": [
            "山阳郭里无潮，野水自向新桥。鱼网平铺荷叶，",
            "却忆士龙宾阁，清琴绿竹萧萧。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百零七",
        "no#": 32
    },
    {
        "title": "送陆澧还吴中",
        "author": "李嘉佑",
        "biography": "",
        "paragraphs": [
            "瓜步寒潮送客，杨花暮雨沾衣。",
            "故乡南望何处，春水连天独归。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百零七",
        "no#": 33
    },
    {
        "title": "春日忆家",
        "author": "李嘉佑",
        "biography": "",
        "paragraphs": [
            "自觉劳乡梦，无人见客心。空余庭草色，日日伴愁襟。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百零七",
        "no#": 34
    },
    {
        "title": "远寺钟",
        "author": "李嘉佑",
        "biography": "",
        "paragraphs": [
            "疏钟何处来，度竹兼拂水。渐逐微风声，依依犹在耳。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百零七",
        "no#": 35
    },
    {
        "title": "白鹭",
        "author": "李嘉佑",
        "biography": "",
        "paragraphs": [
            "江南渌水多，顾影逗轻波。落日秦云里，山高奈若何。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百零七",
        "no#": 36
    },
    {
        "title": "夜宴南陵留别",
        "author": "李嘉佑",
        "biography": "",
        "paragraphs": [
            "雪满前庭月色闲，主人留客未能还。",
            "预愁明日相思处，匹马千山与万山。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百零七",
        "no#": 37
    },
    {
        "title": "题前溪馆",
        "author": "李嘉佑",
        "biography": "",
        "paragraphs": [
            "两年谪宦在江西，举目云山要自迷。",
            "今日始知风土异，浔阳南去鹧鸪啼。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百零七",
        "no#": 38
    },
    {
        "title": "过乌公山寄钱起员外",
        "author": "李嘉佑",
        "biography": "",
        "paragraphs": [
            "雨过青山猿叫时，愁人泪点石榴枝。",
            "无端王事还相系，肠断蒹葭君不知。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百零七",
        "no#": 39
    },
    {
        "title": "寄王舍人竹楼",
        "author": "李嘉佑",
        "biography": "",
        "paragraphs": [
            "傲吏身闲笑五侯，西江取竹起高楼。",
            "南风不用蒲葵扇，纱帽闲眠对水鸥。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百零七",
        "no#": 40
    },
    {
        "title": "韦润州后亭海榴",
        "author": "李嘉佑",
        "biography": "",
        "paragraphs": [
            "江上年年小雪迟，年光独报海榴知。",
            "寂寂山城风日暖，谢公含笑向南枝。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百零七",
        "no#": 41
    },
    {
        "title": "送崔十一弟归北京",
        "author": "李嘉佑",
        "biography": "",
        "paragraphs": [
            "潘郎美貌谢公诗，银印花骢年少时。",
            "楚地江皋一为别，晋山沙水独相思。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百零七",
        "no#": 42
    },
    {
        "title": "访韩司空不遇",
        "author": "李嘉佑",
        "biography": "",
        "paragraphs": [
            "图画风流似长康，文词体格效陈王。",
            "蓬莱对去归常晚，丛竹闲飞满夕阳。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百零七",
        "no#": 43
    },
    {
        "title": "题道虔上人竹房",
        "author": "李嘉佑",
        "biography": "",
        "paragraphs": [
